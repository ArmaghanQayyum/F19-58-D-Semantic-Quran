  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "وَمِن دُونِهِمَا جَنَّتَانِ";
  dbp:text "So which of the favors of your Lord would you deny? -";
  dbo:Surah "Ar-Rahman";
