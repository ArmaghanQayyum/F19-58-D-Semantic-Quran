  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ",
    "مُدْهَآمَّتَانِ";
  dbp:text "Dark green [in color].";
  dbo:Surah "Ar-Rahman";
  a dbr:Quranic_verse;
  dbo:ayah "4965"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Ayah;
  dbp:text "So which of the favors of your Lord would you deny?";
  dbo:Surah "Ar-Rahman";
  dbo:ayah "4966"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ";
  dbp:text "In both of them are two springs, spouting.";
  dbo:Surah "Ar-Rahman";
  dbo:ayah "4967"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "فِيهِمَا عَيْنَانِ نَضَّاخَتَانِ";
  dbp:text "So which of the favors of your Lord would you deny?";
  dbo:Surah "Ar-Rahman";
  dbo:ayah "4968"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ";
  dbp:text "In both of them are fruit and palm trees and pomegranates.";
  dbo:Surah "Ar-Rahman";
  dbo:ayah "4969"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "فِيهِمَا فَٰكِهَةٌ وَنَخْلٌ وَرُمَّانٌ";
  dbp:text "So which of the favors of your Lord would you deny?";
  dbo:Surah "Ar-Rahman";
  dbo:ayah "4970"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ";
  dbo:ayah "4971"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:Surah "Ar-Rahman";
  dbp:text "In them are good and beautiful women -";
  <http://localhost:3333/has_arabic_text> "فِيهِنَّ خَيْرَٰتٌ حِسَانٌ";
  dbp:text "So which of the favors of your Lord would you deny? -";
  dbo:Surah "Ar-Rahman";
  dbo:ayah "4972"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "4973"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Ar-Rahman";
  dbp:text "Fair ones reserved in pavilions -";
  <http://localhost:3333/has_arabic_text> "حُورٌ مَّقْصُورَٰتٌ فِى ٱلْخِيَامِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "4974"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Ar-Rahman";
  dbp:text "So which of the favors of your Lord would you deny? -";
  <http://localhost:3333/has_arabic_text> "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "4975"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Ar-Rahman";
  dbp:text "Untouched before them by man or jinni -";
  <http://localhost:3333/has_arabic_text> "لَمْ يَطْمِثْهُنَّ إِنسٌ قَبْلَهُمْ وَلَا جَآنٌّ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "4976"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Ar-Rahman";
  dbp:text "So which of the favors of your Lord would you deny? -";
  <http://localhost:3333/has_arabic_text> "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "4977"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Ar-Rahman";
  dbp:text "Reclining on green cushions and beautiful fine carpets.";
  <http://localhost:3333/has_arabic_text> "مُتَّكِـِٔينَ عَلَىٰ رَفْرَفٍ خُضْرٍ وَعَبْقَرِىٍّ حِسَانٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "4978"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Ar-Rahman";
  dbp:text "So which of the favors of your Lord would you deny?";
  <http://localhost:3333/has_arabic_text> "فَبِأَىِّ ءَالَآءِ رَبِّكُمَا تُكَذِّبَانِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "4979"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Ar-Rahman";
  dbp:text "Blessed is the name of your Lord, Owner of Majesty and Honor.";
  <http://localhost:3333/has_arabic_text> "تَبَٰرَكَ ٱسْمُ رَبِّكَ ذِى ٱلْجَلَٰلِ وَٱلْإِكْرَامِ",
    "ٱلرَّحْمَٰنُ", "ٱلشَّمْسُ وَٱلْقَمَرُ بِحُسْبَانٍ" .

<http://localhost:3333/Al-Waqi'ah> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Waqi'ah";
  dbo:ayah "4980"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "4981"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "There is, at its occurrence, no denial.", "When the Occurrence occurs,";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "4982"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "It will bring down [some] and raise up [others].";
  <http://localhost:3333/has_arabic_text> "إِذَا وَقَعَتِ ٱلْوَاقِعَةُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "4983"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "When the earth is shaken with convulsion";
  <http://localhost:3333/has_arabic_text> "إِذَا رُجَّتِ ٱلْأَرْضُ رَجًّا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "4984"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And the mountains are broken down, crumbling";
  <http://localhost:3333/has_arabic_text> "خَافِضَةٌ رَّافِعَةٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "4985"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And become dust dispersing.";
  <http://localhost:3333/has_arabic_text> "فَكَانَتْ هَبَآءً مُّنۢبَثًّا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "4986"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And you become [of] three kinds:";
  <http://localhost:3333/has_arabic_text> "لَيْسَ لِوَقْعَتِهَا كَاذِبَةٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "4987"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Then the companions of the right - what are the companions of the right?";
  <http://localhost:3333/has_arabic_text> "فَأَصْحَٰبُ ٱلْمَيْمَنَةِ مَآ أَصْحَٰبُ ٱلْمَيْمَنَةِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "4988"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And the companions of the left - what are the companions of the left?";
  <http://localhost:3333/has_arabic_text> "وَأَصْحَٰبُ ٱلْمَشْـَٔمَةِ مَآ أَصْحَٰبُ ٱلْمَشْـَٔمَةِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "4989"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And the forerunners, the forerunners -";
  <http://localhost:3333/has_arabic_text> "وَبُسَّتِ ٱلْجِبَالُ بَسًّا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "4990"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Those are the ones brought near [to Allah]";
  <http://localhost:3333/has_arabic_text> "أُو۟لَٰٓئِكَ ٱلْمُقَرَّبُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "4991"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "In the Gardens of Pleasure,";
  <http://localhost:3333/has_arabic_text> "فِى جَنَّٰتِ ٱلنَّعِيمِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "4992"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "A [large] company of the former peoples";
  <http://localhost:3333/has_arabic_text> "ثُلَّةٌ مِّنَ ٱلْأَوَّلِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "4993"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And a few of the later peoples,";
  <http://localhost:3333/has_arabic_text> "وَقَلِيلٌ مِّنَ ٱلْءَاخِرِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "4994"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "On thrones woven [with ornament],";
  <http://localhost:3333/has_arabic_text> "عَلَىٰ سُرُرٍ مَّوْضُونَةٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "4995"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Reclining on them, facing each other.";
  <http://localhost:3333/has_arabic_text> "مُّتَّكِـِٔينَ عَلَيْهَا مُتَقَٰبِلِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "4996"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "There will circulate among them young boys made eternal";
  <http://localhost:3333/has_arabic_text> "وَكُنتُمْ أَزْوَٰجًا ثَلَٰثَةً";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "4997"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "With vessels, pitchers and a cup [of wine] from a flowing spring -";
  <http://localhost:3333/has_arabic_text> "بِأَكْوَابٍ وَأَبَارِيقَ وَكَأْسٍ مِّن مَّعِينٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "4998"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "No headache will they have therefrom, nor will they be intoxicated -";
  <http://localhost:3333/has_arabic_text> "لَّا يُصَدَّعُونَ عَنْهَا وَلَا يُنزِفُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "4999"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And fruit of what they select";
  <http://localhost:3333/has_arabic_text> "وَفَٰكِهَةٍ مِّمَّا يَتَخَيَّرُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5000"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And the meat of fowl, from whatever they desire.";
  <http://localhost:3333/has_arabic_text> "وَلَحْمِ طَيْرٍ مِّمَّا يَشْتَهُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5001"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And [for them are] fair women with large, [beautiful] eyes,";
  <http://localhost:3333/has_arabic_text> "وَحُورٌ عِينٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5002"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "The likenesses of pearls well-protected,";
  <http://localhost:3333/has_arabic_text> "كَأَمْثَٰلِ ٱللُّؤْلُؤِ ٱلْمَكْنُونِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5003"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "As reward for what they used to do.";
  <http://localhost:3333/has_arabic_text> "جَزَآءًۢ بِمَا كَانُوا۟ يَعْمَلُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5004"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "They will not hear therein ill speech or commission of sin -";
  <http://localhost:3333/has_arabic_text> "لَا يَسْمَعُونَ فِيهَا لَغْوًا وَلَا تَأْثِيمًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5005"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Only a saying: \"Peace, peace.\"";
  <http://localhost:3333/has_arabic_text> "إِلَّا قِيلًا سَلَٰمًا سَلَٰمًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5006"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "The companions of the right - what are the companions of the right?";
  <http://localhost:3333/has_arabic_text> "وَأَصْحَٰبُ ٱلْيَمِينِ مَآ أَصْحَٰبُ ٱلْيَمِينِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5007"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "[They will be] among lote trees with thorns removed";
  <http://localhost:3333/has_arabic_text> "فِى سِدْرٍ مَّخْضُودٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5008"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And [banana] trees layered [with fruit]";
  <http://localhost:3333/has_arabic_text> "وَطَلْحٍ مَّنضُودٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5009"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And shade extended";
  <http://localhost:3333/has_arabic_text> "وَظِلٍّ مَّمْدُودٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5010"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And water poured out";
  <http://localhost:3333/has_arabic_text> "وَمَآءٍ مَّسْكُوبٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5011"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And fruit, abundant [and varied],";
  <http://localhost:3333/has_arabic_text> "وَفَٰكِهَةٍ كَثِيرَةٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5012"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Neither limited [to season] nor forbidden,";
  <http://localhost:3333/has_arabic_text> "لَّا مَقْطُوعَةٍ وَلَا مَمْنُوعَةٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5013"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And [upon] beds raised high.";
  <http://localhost:3333/has_arabic_text> "وَفُرُشٍ مَّرْفُوعَةٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5014"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Indeed, We have produced the women of Paradise in a [new] creation";
  <http://localhost:3333/has_arabic_text> "إِنَّآ أَنشَأْنَٰهُنَّ إِنشَآءً";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5015"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And made them virgins,";
  <http://localhost:3333/has_arabic_text> "فَجَعَلْنَٰهُنَّ أَبْكَارًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5016"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Devoted [to their husbands] and of equal age,";
  <http://localhost:3333/has_arabic_text> "عُرُبًا أَتْرَابًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5017"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "For the companions of the right [who are]";
  <http://localhost:3333/has_arabic_text> "لِّأَصْحَٰبِ ٱلْيَمِينِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5018"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "A company of the former peoples";
  <http://localhost:3333/has_arabic_text> "ثُلَّةٌ مِّنَ ٱلْأَوَّلِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5019"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And a company of the later peoples.";
  <http://localhost:3333/has_arabic_text> "وَثُلَّةٌ مِّنَ ٱلْءَاخِرِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5020"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And the companions of the left - what are the companions of the left?";
  <http://localhost:3333/has_arabic_text> "وَأَصْحَٰبُ ٱلشِّمَالِ مَآ أَصْحَٰبُ ٱلشِّمَالِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5021"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "[They will be] in scorching fire and scalding water";
  <http://localhost:3333/has_arabic_text> "فِى سَمُومٍ وَحَمِيمٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5022"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And a shade of black smoke,";
  <http://localhost:3333/has_arabic_text> "وَظِلٍّ مِّن يَحْمُومٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5023"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Neither cool nor beneficial.";
  <http://localhost:3333/has_arabic_text> "لَّا بَارِدٍ وَلَا كَرِيمٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5024"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Indeed they were, before that, indulging in affluence,";
  <http://localhost:3333/has_arabic_text> "إِنَّهُمْ كَانُوا۟ قَبْلَ ذَٰلِكَ مُتْرَفِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5025"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And they used to persist in the great violation,";
  <http://localhost:3333/has_arabic_text> "وَكَانُوا۟ يُصِرُّونَ عَلَى ٱلْحِنثِ ٱلْعَظِيمِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5026"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And they used to say, \"When we die and become dust and bones, are we indeed to be resurrected?";
  <http://localhost:3333/has_arabic_text> "وَكَانُوا۟ يَقُولُونَ أَئِذَا مِتْنَا وَكُنَّا تُرَابًا وَعِظَٰمًا أَءِنَّا لَمَبْعُوثُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5027"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And our forefathers [as well]?\"";
  <http://localhost:3333/has_arabic_text> "أَوَءَابَآؤُنَا ٱلْأَوَّلُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5028"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Say, [O Muhammad], \"Indeed, the former and the later peoples";
  <http://localhost:3333/has_arabic_text> "قُلْ إِنَّ ٱلْأَوَّلِينَ وَٱلْءَاخِرِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5029"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Are to be gathered together for the appointment of a known Day.\"";
  <http://localhost:3333/has_arabic_text> "لَمَجْمُوعُونَ إِلَىٰ مِيقَٰتِ يَوْمٍ مَّعْلُومٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5030"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Then indeed you, O those astray [who are] deniers,";
  <http://localhost:3333/has_arabic_text> "ثُمَّ إِنَّكُمْ أَيُّهَا ٱلضَّآلُّونَ ٱلْمُكَذِّبُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5031"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Will be eating from trees of zaqqum";
  <http://localhost:3333/has_arabic_text> "لَءَاكِلُونَ مِن شَجَرٍ مِّن زَقُّومٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5032"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And filling with it your bellies";
  <http://localhost:3333/has_arabic_text> "فَمَالِـُٔونَ مِنْهَا ٱلْبُطُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5033"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And drinking on top of it from scalding water";
  <http://localhost:3333/has_arabic_text> "فَشَٰرِبُونَ عَلَيْهِ مِنَ ٱلْحَمِيمِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5034"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And will drink as the drinking of thirsty camels.";
  <http://localhost:3333/has_arabic_text> "فَشَٰرِبُونَ شُرْبَ ٱلْهِيمِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5035"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "That is their accommodation on the Day of Recompense.";
  <http://localhost:3333/has_arabic_text> "هَٰذَا نُزُلُهُمْ يَوْمَ ٱلدِّينِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5036"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "We have created you, so why do you not believe?";
  <http://localhost:3333/has_arabic_text> "نَحْنُ خَلَقْنَٰكُمْ فَلَوْلَا تُصَدِّقُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5037"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Have you seen that which you emit?";
  <http://localhost:3333/has_arabic_text> "أَفَرَءَيْتُم مَّا تُمْنُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5038"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Is it you who creates it, or are We the Creator?";
  <http://localhost:3333/has_arabic_text> "ءَأَنتُمْ تَخْلُقُونَهُۥٓ أَمْ نَحْنُ ٱلْخَٰلِقُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5039"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "We have decreed death among you, and We are not to be outdone";
  <http://localhost:3333/has_arabic_text> "نَحْنُ قَدَّرْنَا بَيْنَكُمُ ٱلْمَوْتَ وَمَا نَحْنُ بِمَسْبُوقِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5040"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "In that We will change your likenesses and produce you in that [form] which you do not know.";
  <http://localhost:3333/has_arabic_text> "عَلَىٰٓ أَن نُّبَدِّلَ أَمْثَٰلَكُمْ وَنُنشِئَكُمْ فِى مَا لَا تَعْلَمُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5041"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And you have already known the first creation, so will you not remember?";
  <http://localhost:3333/has_arabic_text> "وَلَقَدْ عَلِمْتُمُ ٱلنَّشْأَةَ ٱلْأُولَىٰ فَلَوْلَا تَذَكَّرُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5042"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And have you seen that [seed] which you sow?";
  <http://localhost:3333/has_arabic_text> "أَفَرَءَيْتُم مَّا تَحْرُثُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5043"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Is it you who makes it grow, or are We the grower?";
  <http://localhost:3333/has_arabic_text> "ءَأَنتُمْ تَزْرَعُونَهُۥٓ أَمْ نَحْنُ ٱلزَّٰرِعُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5044"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "If We willed, We could make it [dry] debris, and you would remain in wonder,";
  <http://localhost:3333/has_arabic_text> "لَوْ نَشَآءُ لَجَعَلْنَٰهُ حُطَٰمًا فَظَلْتُمْ تَفَكَّهُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5045"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "[Saying], \"Indeed, we are [now] in debt;";
  <http://localhost:3333/has_arabic_text> "إِنَّا لَمُغْرَمُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5046"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Rather, we have been deprived.\"";
  <http://localhost:3333/has_arabic_text> "بَلْ نَحْنُ مَحْرُومُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5047"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And have you seen the water that you drink?";
  <http://localhost:3333/has_arabic_text> "أَفَرَءَيْتُمُ ٱلْمَآءَ ٱلَّذِى تَشْرَبُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5048"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Is it you who brought it down from the clouds, or is it We who bring it down?";
  <http://localhost:3333/has_arabic_text> "ءَأَنتُمْ أَنزَلْتُمُوهُ مِنَ ٱلْمُزْنِ أَمْ نَحْنُ ٱلْمُنزِلُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5049"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "If We willed, We could make it bitter, so why are you not grateful?";
  <http://localhost:3333/has_arabic_text> "لَوْ نَشَآءُ جَعَلْنَٰهُ أُجَاجًا فَلَوْلَا تَشْكُرُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5050"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And have you seen the fire that you ignite?";
  <http://localhost:3333/has_arabic_text> "أَفَرَءَيْتُمُ ٱلنَّارَ ٱلَّتِى تُورُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5051"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Is it you who produced its tree, or are We the producer?";
  <http://localhost:3333/has_arabic_text> "ءَأَنتُمْ أَنشَأْتُمْ شَجَرَتَهَآ أَمْ نَحْنُ ٱلْمُنشِـُٔونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5052"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "We have made it a reminder and provision for the travelers,";
  <http://localhost:3333/has_arabic_text> "نَحْنُ جَعَلْنَٰهَا تَذْكِرَةً وَمَتَٰعًا لِّلْمُقْوِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5053"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "So exalt the name of your Lord, the Most Great.";
  <http://localhost:3333/has_arabic_text> "فَسَبِّحْ بِٱسْمِ رَبِّكَ ٱلْعَظِيمِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5054"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Then I swear by the setting of the stars,";
  <http://localhost:3333/has_arabic_text> "فَلَآ أُقْسِمُ بِمَوَٰقِعِ ٱلنُّجُومِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5055"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And indeed, it is an oath - if you could know - [most] great.";
  <http://localhost:3333/has_arabic_text> "وَإِنَّهُۥ لَقَسَمٌ لَّوْ تَعْلَمُونَ عَظِيمٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5056"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Indeed, it is a noble Qur'an";
  <http://localhost:3333/has_arabic_text> "إِنَّهُۥ لَقُرْءَانٌ كَرِيمٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5057"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "In a Register well-protected;";
  <http://localhost:3333/has_arabic_text> "فِى كِتَٰبٍ مَّكْنُونٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5058"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "None touch it except the purified.";
  <http://localhost:3333/has_arabic_text> "لَّا يَمَسُّهُۥٓ إِلَّا ٱلْمُطَهَّرُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5059"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "[It is] a revelation from the Lord of the worlds.";
  <http://localhost:3333/has_arabic_text> "تَنزِيلٌ مِّن رَّبِّ ٱلْعَٰلَمِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5060"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Then is it to this statement that you are indifferent";
  <http://localhost:3333/has_arabic_text> "أَفَبِهَٰذَا ٱلْحَدِيثِ أَنتُم مُّدْهِنُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5061"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And make [the thanks for] your provision that you deny [the Provider]?";
  <http://localhost:3333/has_arabic_text> "وَتَجْعَلُونَ رِزْقَكُمْ أَنَّكُمْ تُكَذِّبُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5062"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Then why, when the soul at death reaches the throat";
  <http://localhost:3333/has_arabic_text> "فَلَوْلَآ إِذَا بَلَغَتِ ٱلْحُلْقُومَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5063"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And you are at that time looking on -";
  <http://localhost:3333/has_arabic_text> "وَأَنتُمْ حِينَئِذٍ تَنظُرُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5064"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And Our angels are nearer to him than you, but you do not see -";
  <http://localhost:3333/has_arabic_text> "وَنَحْنُ أَقْرَبُ إِلَيْهِ مِنكُمْ وَلَٰكِن لَّا تُبْصِرُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5065"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Then why do you not, if you are not to be recompensed,";
  <http://localhost:3333/has_arabic_text> "فَلَوْلَآ إِن كُنتُمْ غَيْرَ مَدِينِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5066"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Bring it back, if you should be truthful?";
  <http://localhost:3333/has_arabic_text> "تَرْجِعُونَهَآ إِن كُنتُمْ صَٰدِقِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5067"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And if the deceased was of those brought near to Allah,";
  <http://localhost:3333/has_arabic_text> "فَأَمَّآ إِن كَانَ مِنَ ٱلْمُقَرَّبِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5068"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Then [for him is] rest and bounty and a garden of pleasure.";
  <http://localhost:3333/has_arabic_text> "فَرَوْحٌ وَرَيْحَانٌ وَجَنَّتُ نَعِيمٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5069"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And if he was of the companions of the right,";
  <http://localhost:3333/has_arabic_text> "وَأَمَّآ إِن كَانَ مِنْ أَصْحَٰبِ ٱلْيَمِينِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5070"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Then [the angels will say], \"Peace for you; [you are] from the companions of the right.\"";
  <http://localhost:3333/has_arabic_text> "فَسَلَٰمٌ لَّكَ مِنْ أَصْحَٰبِ ٱلْيَمِينِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5071"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "But if he was of the deniers [who were] astray,";
  <http://localhost:3333/has_arabic_text> "وَأَمَّآ إِن كَانَ مِنَ ٱلْمُكَذِّبِينَ ٱلضَّآلِّينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5072"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "Then [for him is] accommodation of scalding water";
  <http://localhost:3333/has_arabic_text> "فَنُزُلٌ مِّنْ حَمِيمٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5073"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  dbp:text "And burning in Hellfire";
  <http://localhost:3333/has_arabic_text> "وَتَصْلِيَةُ جَحِيمٍ";
  dbp:text "Indeed, this is the true certainty,";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5074"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Waqi'ah";
  <http://localhost:3333/has_arabic_text> "إِنَّ هَٰذَا لَهُوَ حَقُّ ٱلْيَقِينِ";
  dbp:text "So exalt the name of your Lord, the Most Great.";
  dbo:Surah "Al-Waqi'ah";
  a dbr:Ayah;
  dbo:ayah "5075"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "فَسَبِّحْ بِٱسْمِ رَبِّكَ ٱلْعَظِيمِ", "وَٱلسَّٰبِقُونَ ٱلسَّٰبِقُونَ",
    "يَطُوفُ عَلَيْهِمْ وِلْدَٰنٌ مُّخَلَّدُونَ" .

<http://localhost:3333/Al-Hadeed> a dbr:Ayah, dbr:Quranic_verse;
  dbo:Surah "Al-Hadeed";
  a dbr:Ayat;
  dbo:ayah "5076"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "Whatever is in the heavens and earth exalts Allah, and He is the Exalted in Might, the Wise.",
    "His is the dominion of the heavens and earth. He gives life and causes death, and He is over all things competent.";
  dbo:Surah "Al-Hadeed";
  dbo:ayah "5077"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbp:text "He is the First and the Last, the Ascendant and the Intimate, and He is, of all things, Knowing.";
  dbo:Surah "Al-Hadeed";
  dbo:ayah "5078"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "سَبَّحَ لِلَّهِ مَا فِى ٱلسَّمَٰوَٰتِ وَٱلْأَرْضِ ۖ وَهُوَ ٱلْعَزِيزُ ٱلْحَكِيمُ",
    "لَهُۥ مُلْكُ ٱلسَّمَٰوَٰتِ وَٱلْأَرْضِ ۖ يُحْىِۦ وَيُمِيتُ ۖ وَهُوَ عَلَىٰ كُلِّ شَىْءٍ قَدِيرٌ";
  dbp:text "It is He who created the heavens and earth in six days and then established Himself above the Throne. He knows what penetrates into the earth and what emerges from it and what descends from the heaven and what ascends therein; and He is with you wherever you are. And Allah, of what you do, is Seeing.";
  dbo:Surah "Al-Hadeed";
  dbo:ayah "5079"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbp:text "His is the dominion of the heavens and earth. And to Allah are returned [all] matters.";
  dbo:Surah "Al-Hadeed";
  dbo:ayah "5080"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "لَّهُۥ مُلْكُ ٱلسَّمَٰوَٰتِ وَٱلْأَرْضِ ۚ وَإِلَى ٱللَّهِ تُرْجَعُ ٱلْأُمُورُ";
  dbp:text "He causes the night to enter the day and causes the day to enter the night, and he is Knowing of that within the breasts.";
  dbo:Surah "Al-Hadeed";
  dbo:ayah "5081"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "هُوَ ٱلَّذِى خَلَقَ ٱلسَّمَٰوَٰتِ وَٱلْأَرْضَ فِى سِتَّةِ أَيَّامٍ ثُمَّ ٱسْتَوَىٰ عَلَى ٱلْعَرْشِ ۚ يَعْلَمُ مَا يَلِجُ فِى ٱلْأَرْضِ وَمَا يَخْرُجُ مِنْهَا وَمَا يَنزِلُ مِنَ ٱلسَّمَآءِ وَمَا يَعْرُجُ فِيهَا ۖ وَهُوَ مَعَكُمْ أَيْنَ مَا كُنتُمْ ۚ وَٱللَّهُ بِمَا تَعْمَلُونَ بَصِيرٌ";
  dbp:text "Believe in Allah and His Messenger and spend out of that in which He has made you successors. For those who have believed among you and spent, there will be a great reward.";
  dbo:ayah "5082"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Hadeed";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "ءَامِنُوا۟ بِٱللَّهِ وَرَسُولِهِۦ وَأَنفِقُوا۟ مِمَّا جَعَلَكُم مُّسْتَخْلَفِينَ فِيهِ ۖ فَٱلَّذِينَ ءَامَنُوا۟ مِنكُمْ وَأَنفَقُوا۟ لَهُمْ أَجْرٌ كَبِيرٌ",
    "هُوَ ٱلْأَوَّلُ وَٱلْءَاخِرُ وَٱلظَّٰهِرُ وَٱلْبَاطِنُ ۖ وَهُوَ بِكُلِّ شَىْءٍ عَلِيمٌ";
  dbp:text "And why do you not believe in Allah while the Messenger invites you to believe in your Lord and He has taken your covenant, if you should [truly] be believers?";
  dbo:Surah "Al-Hadeed";
  dbo:ayah "5083"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "هُوَ ٱلَّذِى يُنَزِّلُ عَلَىٰ عَبْدِهِۦٓ ءَايَٰتٍۭ بَيِّنَٰتٍ لِّيُخْرِجَكُم مِّنَ ٱلظُّلُمَٰتِ إِلَى ٱلنُّورِ ۚ وَإِنَّ ٱللَّهَ بِكُمْ لَرَءُوفٌ رَّحِيمٌ";
  dbp:text "It is He who sends down upon His Servant [Muhammad] verses of clear evidence that He may bring you out from darknesses into the light. And indeed, Allah is to you Kind and Merciful.";
  dbo:Surah "Al-Hadeed";
  a dbr:Quranic_verse;
  dbo:ayah "5084"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "وَمَا لَكُمْ أَلَّا تُنفِقُوا۟ فِى سَبِيلِ ٱللَّهِ وَلِلَّهِ مِيرَٰثُ ٱلسَّمَٰوَٰتِ وَٱلْأَرْضِ ۚ لَا يَسْتَوِى مِنكُم مَّنْ أَنفَقَ مِن قَبْلِ ٱلْفَتْحِ وَقَٰتَلَ ۚ أُو۟لَٰٓئِكَ أَعْظَمُ دَرَجَةً مِّنَ ٱلَّذِينَ أَنفَقُوا۟ مِنۢ بَعْدُ وَقَٰتَلُوا۟ ۚ وَكُلًّا وَعَدَ ٱللَّهُ ٱلْحُسْنَىٰ ۚ وَٱللَّهُ بِمَا تَعْمَلُونَ خَبِيرٌ";
  dbp:text "And why do you not spend in the cause of Allah while to Allah belongs the heritage of the heavens and the earth? Not equal among you are those who spent before the conquest [of Makkah] and fought [and those who did so after it]. Those are greater in degree than they who spent afterwards and fought. But to all Allah has promised the best [reward]. And Allah, with what you do, is Acquainted.";
  dbo:Surah "Al-Hadeed";
  dbo:ayah "5085"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbp:text "Who is it that would loan Allah a goodly loan so He will multiply it for him and he will have a noble reward?";
  dbo:Surah "Al-Hadeed";
  dbo:ayah "5086"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "مَّن ذَا ٱلَّذِى يُقْرِضُ ٱللَّهَ قَرْضًا حَسَنًا فَيُضَٰعِفَهُۥ لَهُۥ وَلَهُۥٓ أَجْرٌ كَرِيمٌ",
    "وَمَا لَكُمْ لَا تُؤْمِنُونَ بِٱللَّهِ ۙ وَٱلرَّسُولُ يَدْعُوكُمْ لِتُؤْمِنُوا۟ بِرَبِّكُمْ وَقَدْ أَخَذَ مِيثَٰقَكُمْ إِن كُنتُم مُّؤْمِنِينَ";
  dbp:text "On the Day you see the believing men and believing women, their light proceeding before them and on their right, [it will be said], \"Your good tidings today are [of] gardens beneath which rivers flow, wherein you will abide eternally.\" That is what is the great attainment.";
  dbo:Surah "Al-Hadeed";
  dbo:ayah "5087"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah, dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5088"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Hadeed";
  dbp:text "On the [same] Day the hypocrite men and hypocrite women will say to those who believed, \"Wait for us that we may acquire some of your light.\" It will be said, \"Go back behind you and seek light.\" And a wall will be placed between them with a door, its interior containing mercy, but on the outside of it is torment.";
  <http://localhost:3333/has_arabic_text> "يَوْمَ تَرَى ٱلْمُؤْمِنِينَ وَٱلْمُؤْمِنَٰتِ يَسْعَىٰ نُورُهُم بَيْنَ أَيْدِيهِمْ وَبِأَيْمَٰنِهِم بُشْرَىٰكُمُ ٱلْيَوْمَ جَنَّٰتٌ تَجْرِى مِن تَحْتِهَا ٱلْأَنْهَٰرُ خَٰلِدِينَ فِيهَا ۚ ذَٰلِكَ هُوَ ٱلْفَوْزُ ٱلْعَظِيمُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5089"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Hadeed";
  dbp:text "The hypocrites will call to the believers, \"Were we not with you?\" They will say, \"Yes, but you afflicted yourselves and awaited [misfortune for us] and doubted, and wishful thinking deluded you until there came the command of Allah. And the Deceiver deceived you concerning Allah.";
  <http://localhost:3333/has_arabic_text> "يَوْمَ يَقُولُ ٱلْمُنَٰفِقُونَ وَٱلْمُنَٰفِقَٰتُ لِلَّذِينَ ءَامَنُوا۟ ٱنظُرُونَا نَقْتَبِسْ مِن نُّورِكُمْ قِيلَ ٱرْجِعُوا۟ وَرَآءَكُمْ فَٱلْتَمِسُوا۟ نُورًا فَضُرِبَ بَيْنَهُم بِسُورٍ لَّهُۥ بَابٌۢ بَاطِنُهُۥ فِيهِ ٱلرَّحْمَةُ وَظَٰهِرُهُۥ مِن قِبَلِهِ ٱلْعَذَابُ",
    "فَٱلْيَوْمَ لَا يُؤْخَذُ مِنكُمْ فِدْيَةٌ وَلَا مِنَ ٱلَّذِينَ كَفَرُوا۟ ۚ مَأْوَىٰكُمُ ٱلنَّارُ ۖ هِىَ مَوْلَىٰكُمْ ۖ وَبِئْسَ ٱلْمَصِيرُ";
  dbo:Surah "Al-Hadeed";
  dbp:text "So today no ransom will be taken from you or from those who disbelieved. Your refuge is the Fire. It is most worthy of you, and wretched is the destination.";
  dbo:ayah "5090"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "أَلَمْ يَأْنِ لِلَّذِينَ ءَامَنُوٓا۟ أَن تَخْشَعَ قُلُوبُهُمْ لِذِكْرِ ٱللَّهِ وَمَا نَزَلَ مِنَ ٱلْحَقِّ وَلَا يَكُونُوا۟ كَٱلَّذِينَ أُوتُوا۟ ٱلْكِتَٰبَ مِن قَبْلُ فَطَالَ عَلَيْهِمُ ٱلْأَمَدُ فَقَسَتْ قُلُوبُهُمْ ۖ وَكَثِيرٌ مِّنْهُمْ فَٰسِقُونَ";
  dbp:text "Has the time not come for those who have believed that their hearts should become humbly submissive at the remembrance of Allah and what has come down of the truth? And let them not be like those who were given the Scripture before, and a long period passed over them, so their hearts hardened; and many of them are defiantly disobedient.";
  dbo:Surah "Al-Hadeed";
  dbo:ayah "5091"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah, dbr:Ayah;
  dbp:text "Know that Allah gives life to the earth after its lifelessness. We have made clear to you the signs; perhaps you will understand.";
  dbo:Surah "Al-Hadeed";
  dbo:ayah "5092"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "يُنَادُونَهُمْ أَلَمْ نَكُن مَّعَكُمْ ۖ قَالُوا۟ بَلَىٰ وَلَٰكِنَّكُمْ فَتَنتُمْ أَنفُسَكُمْ وَتَرَبَّصْتُمْ وَٱرْتَبْتُمْ وَغَرَّتْكُمُ ٱلْأَمَانِىُّ حَتَّىٰ جَآءَ أَمْرُ ٱللَّهِ وَغَرَّكُم بِٱللَّهِ ٱلْغَرُورُ";
  dbp:text "Indeed, the men who practice charity and the women who practice charity and [they who] have loaned Allah a goodly loan - it will be multiplied for them, and they will have a noble reward.";
  dbo:Surah "Al-Hadeed";
  dbo:ayah "5093"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "إِنَّ ٱلْمُصَّدِّقِينَ وَٱلْمُصَّدِّقَٰتِ وَأَقْرَضُوا۟ ٱللَّهَ قَرْضًا حَسَنًا يُضَٰعَفُ لَهُمْ وَلَهُمْ أَجْرٌ كَرِيمٌ",
    "وَٱلَّذِينَ ءَامَنُوا۟ بِٱللَّهِ وَرُسُلِهِۦٓ أُو۟لَٰٓئِكَ هُمُ ٱلصِّدِّيقُونَ ۖ وَٱلشُّهَدَآءُ عِندَ رَبِّهِمْ لَهُمْ أَجْرُهُمْ وَنُورُهُمْ ۖ وَٱلَّذِينَ كَفَرُوا۟ وَكَذَّبُوا۟ بِـَٔايَٰتِنَآ أُو۟لَٰٓئِكَ أَصْحَٰبُ ٱلْجَحِيمِ";
  dbp:text "And those who have believed in Allah and His messengers - those are [in the ranks of] the supporters of truth and the martyrs, with their Lord. For them is their reward and their light. But those who have disbelieved and denied Our verses - those are the companions of Hellfire.";
  dbo:Surah "Al-Hadeed";
  a dbr:Ayat;
  dbo:ayah "5094"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "يُولِجُ ٱلَّيْلَ فِى ٱلنَّهَارِ وَيُولِجُ ٱلنَّهَارَ فِى ٱلَّيْلِ ۚ وَهُوَ عَلِيمٌۢ بِذَاتِ ٱلصُّدُورِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5095"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Hadeed";
  dbp:text "Know that the life of this world is but amusement and diversion and adornment and boasting to one another and competition in increase of wealth and children - like the example of a rain whose [resulting] plant growth pleases the tillers; then it dries and you see it turned yellow; then it becomes [scattered] debris. And in the Hereafter is severe punishment and forgiveness from Allah and approval. And what is the worldly life except the enjoyment of delusion.",
    "Race toward forgiveness from your Lord and a Garden whose width is like the width of the heavens and earth, prepared for those who believed in Allah and His messengers. That is the bounty of Allah which He gives to whom He wills, and Allah is the possessor of great bounty.";
  dbo:Surah "Al-Hadeed";
  dbo:ayah "5096"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "سَابِقُوٓا۟ إِلَىٰ مَغْفِرَةٍ مِّن رَّبِّكُمْ وَجَنَّةٍ عَرْضُهَا كَعَرْضِ ٱلسَّمَآءِ وَٱلْأَرْضِ أُعِدَّتْ لِلَّذِينَ ءَامَنُوا۟ بِٱللَّهِ وَرُسُلِهِۦ ۚ ذَٰلِكَ فَضْلُ ٱللَّهِ يُؤْتِيهِ مَن يَشَآءُ ۚ وَٱللَّهُ ذُو ٱلْفَضْلِ ٱلْعَظِيمِ",
    "مَآ أَصَابَ مِن مُّصِيبَةٍ فِى ٱلْأَرْضِ وَلَا فِىٓ أَنفُسِكُمْ إِلَّا فِى كِتَٰبٍ مِّن قَبْلِ أَن نَّبْرَأَهَآ ۚ إِنَّ ذَٰلِكَ عَلَى ٱللَّهِ يَسِيرٌ";
  dbp:text "No disaster strikes upon the earth or among yourselves except that it is in a register before We bring it into being - indeed that, for Allah, is easy -";
  dbo:Surah "Al-Hadeed";
  dbo:ayah "5097"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat, dbr:Ayah;
  dbp:text "In order that you not despair over what has eluded you and not exult [in pride] over what He has given you. And Allah does not like everyone self-deluded and boastful -";
  dbo:Surah "Al-Hadeed";
  dbo:ayah "5098"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "لِّكَيْلَا تَأْسَوْا۟ عَلَىٰ مَا فَاتَكُمْ وَلَا تَفْرَحُوا۟ بِمَآ ءَاتَىٰكُمْ ۗ وَٱللَّهُ لَا يُحِبُّ كُلَّ مُخْتَالٍ فَخُورٍ",
    "ٱعْلَمُوٓا۟ أَنَّ ٱللَّهَ يُحْىِ ٱلْأَرْضَ بَعْدَ مَوْتِهَا ۚ قَدْ بَيَّنَّا لَكُمُ ٱلْءَايَٰتِ لَعَلَّكُمْ تَعْقِلُونَ";
  dbp:text "[Those] who are stingy and enjoin upon people stinginess. And whoever turns away - then indeed, Allah is the Free of need, the Praiseworthy.";
  dbo:Surah "Al-Hadeed";
  a dbr:Ayah;
  dbo:ayah "5099"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat;
  dbp:text "We have already sent Our messengers with clear evidences and sent down with them the Scripture and the balance that the people may maintain [their affairs] in justice. And We sent down iron, wherein is great military might and benefits for the people, and so that Allah may make evident those who support Him and His messengers unseen. Indeed, Allah is Powerful and Exalted in Might.";
  dbo:Surah "Al-Hadeed";
  dbo:ayah "5100"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "لَقَدْ أَرْسَلْنَا رُسُلَنَا بِٱلْبَيِّنَٰتِ وَأَنزَلْنَا مَعَهُمُ ٱلْكِتَٰبَ وَٱلْمِيزَانَ لِيَقُومَ ٱلنَّاسُ بِٱلْقِسْطِ ۖ وَأَنزَلْنَا ٱلْحَدِيدَ فِيهِ بَأْسٌ شَدِيدٌ وَمَنَٰفِعُ لِلنَّاسِ وَلِيَعْلَمَ ٱللَّهُ مَن يَنصُرُهُۥ وَرُسُلَهُۥ بِٱلْغَيْبِ ۚ إِنَّ ٱللَّهَ قَوِىٌّ عَزِيزٌ",
    "وَلَقَدْ أَرْسَلْنَا نُوحًا وَإِبْرَٰهِيمَ وَجَعَلْنَا فِى ذُرِّيَّتِهِمَا ٱلنُّبُوَّةَ وَٱلْكِتَٰبَ ۖ فَمِنْهُم مُّهْتَدٍ ۖ وَكَثِيرٌ مِّنْهُمْ فَٰسِقُونَ";
  dbp:text "And We have already sent Noah and Abraham and placed in their descendants prophethood and scripture; and among them is he who is guided, but many of them are defiantly disobedient.";
  dbo:Surah "Al-Hadeed";
  dbo:ayah "5101"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat, dbr:Ayah;
  dbp:text "Then We sent following their footsteps Our messengers and followed [them] with Jesus, the son of Mary, and gave him the Gospel. And We placed in the hearts of those who followed him compassion and mercy and monasticism, which they innovated; We did not prescribe it for them except [that they did so] seeking the approval of Allah. But they did not observe it with due observance. So We gave the ones who believed among them their reward, but many of them are defiantly disobedient.";
  dbo:Surah "Al-Hadeed";
  a dbr:Ayah;
  dbo:ayah "5102"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "ثُمَّ قَفَّيْنَا عَلَىٰٓ ءَاثَٰرِهِم بِرُسُلِنَا وَقَفَّيْنَا بِعِيسَى ٱبْنِ مَرْيَمَ وَءَاتَيْنَٰهُ ٱلْإِنجِيلَ وَجَعَلْنَا فِى قُلُوبِ ٱلَّذِينَ ٱتَّبَعُوهُ رَأْفَةً وَرَحْمَةً وَرَهْبَانِيَّةً ٱبْتَدَعُوهَا مَا كَتَبْنَٰهَا عَلَيْهِمْ إِلَّا ٱبْتِغَآءَ رِضْوَٰنِ ٱللَّهِ فَمَا رَعَوْهَا حَقَّ رِعَايَتِهَا ۖ فَـَٔاتَيْنَا ٱلَّذِينَ ءَامَنُوا۟ مِنْهُمْ أَجْرَهُمْ ۖ وَكَثِيرٌ مِّنْهُمْ فَٰسِقُونَ";
  dbp:text "O you who have believed, fear Allah and believe in His Messenger; He will [then] give you a double portion of His mercy and make for you a light by which you will walk and forgive you; and Allah is Forgiving and Merciful.";
  a dbr:Ayah, dbr:Quranic_verse;
  dbo:Surah "Al-Hadeed";
  a dbr:Ayat;
  dbo:ayah "5103"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "يَٰٓأَيُّهَا ٱلَّذِينَ ءَامَنُوا۟ ٱتَّقُوا۟ ٱللَّهَ وَءَامِنُوا۟ بِرَسُولِهِۦ يُؤْتِكُمْ كِفْلَيْنِ مِن رَّحْمَتِهِۦ وَيَجْعَل لَّكُمْ نُورًا تَمْشُونَ بِهِۦ وَيَغْفِرْ لَكُمْ ۚ وَٱللَّهُ غَفُورٌ رَّحِيمٌ";
  dbp:text "[This is] so that the People of the Scripture may know that they are not able [to obtain] anything from the bounty of Allah and that [all] bounty is in the hand of Allah; He gives it to whom He wills. And Allah is the possessor of great bounty.";
  dbo:Surah "Al-Hadeed";
  dbo:ayah "5104"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "لِّئَلَّا يَعْلَمَ أَهْلُ ٱلْكِتَٰبِ أَلَّا يَقْدِرُونَ عَلَىٰ شَىْءٍ مِّن فَضْلِ ٱللَّهِ ۙ وَأَنَّ ٱلْفَضْلَ بِيَدِ ٱللَّهِ يُؤْتِيهِ مَن يَشَآءُ ۚ وَٱللَّهُ ذُو ٱلْفَضْلِ ٱلْعَظِيمِ",
    "ٱعْلَمُوٓا۟ أَنَّمَا ٱلْحَيَوٰةُ ٱلدُّنْيَا لَعِبٌ وَلَهْوٌ وَزِينَةٌ وَتَفَاخُرٌۢ بَيْنَكُمْ وَتَكَاثُرٌ فِى ٱلْأَمْوَٰلِ وَٱلْأَوْلَٰدِ ۖ كَمَثَلِ غَيْثٍ أَعْجَبَ ٱلْكُفَّارَ نَبَاتُهُۥ ثُمَّ يَهِيجُ فَتَرَىٰهُ مُصْفَرًّا ثُمَّ يَكُونُ حُطَٰمًا ۖ وَفِى ٱلْءَاخِرَةِ عَذَابٌ شَدِيدٌ وَمَغْفِرَةٌ مِّنَ ٱللَّهِ وَرِضْوَٰنٌ ۚ وَمَا ٱلْحَيَوٰةُ ٱلدُّنْيَآ إِلَّا مَتَٰعُ ٱلْغُرُورِ",
    "ٱلَّذِينَ يَبْخَلُونَ وَيَأْمُرُونَ ٱلنَّاسَ بِٱلْبُخْلِ ۗ وَمَن يَتَوَلَّ فَإِنَّ ٱللَّهَ هُوَ ٱلْغَنِىُّ ٱلْحَمِيدُ" .

<http://localhost:3333/Al-Mujadilah> dbo:Surah "Al-Mujadilah";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5105"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5106"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mujadilah";
  dbp:text "Certainly has Allah heard the speech of the one who argues with you, [O Muhammad], concerning her husband and directs her complaint to Allah. And Allah hears your dialogue; indeed, Allah is Hearing and Seeing.",
    "Those who pronounce thihar among you [to separate] from their wives - they are not [consequently] their mothers. Their mothers are none but those who gave birth to them. And indeed, they are saying an objectionable statement and a falsehood. But indeed, Allah is Pardoning and Forgiving.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5107"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mujadilah";
  dbp:text "And those who pronounce thihar from their wives and then [wish to] go back on what they said - then [there must be] the freeing of a slave before they touch one another. That is what you are admonished thereby; and Allah is Acquainted with what you do.";
  <http://localhost:3333/has_arabic_text> "قَدْ سَمِعَ ٱللَّهُ قَوْلَ ٱلَّتِى تُجَٰدِلُكَ فِى زَوْجِهَا وَتَشْتَكِىٓ إِلَى ٱللَّهِ وَٱللَّهُ يَسْمَعُ تَحَاوُرَكُمَآ ۚ إِنَّ ٱللَّهَ سَمِيعٌۢ بَصِيرٌ";
  a dbr:Ayah, dbr:Quranic_verse;
  dbo:Surah "Al-Mujadilah";
  a dbr:Ayat;
  dbo:ayah "5108"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "And he who does not find [a slave] - then a fast for two months consecutively before they touch one another; and he who is unable - then the feeding of sixty poor persons. That is for you to believe [completely] in Allah and His Messenger; and those are the limits [set by] Allah. And for the disbelievers is a painful punishment.";
  <http://localhost:3333/has_arabic_text> "فَمَن لَّمْ يَجِدْ فَصِيَامُ شَهْرَيْنِ مُتَتَابِعَيْنِ مِن قَبْلِ أَن يَتَمَآسَّا ۖ فَمَن لَّمْ يَسْتَطِعْ فَإِطْعَامُ سِتِّينَ مِسْكِينًا ۚ ذَٰلِكَ لِتُؤْمِنُوا۟ بِٱللَّهِ وَرَسُولِهِۦ ۚ وَتِلْكَ حُدُودُ ٱللَّهِ ۗ وَلِلْكَٰفِرِينَ عَذَابٌ أَلِيمٌ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbp:text "Indeed, those who oppose Allah and His Messenger are abased as those before them were abased. And We have certainly sent down verses of clear evidence. And for the disbelievers is a humiliating punishment.";
  dbo:Surah "Al-Mujadilah";
  dbo:ayah "5109"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "إِنَّ ٱلَّذِينَ يُحَآدُّونَ ٱللَّهَ وَرَسُولَهُۥ كُبِتُوا۟ كَمَا كُبِتَ ٱلَّذِينَ مِن قَبْلِهِمْ ۚ وَقَدْ أَنزَلْنَآ ءَايَٰتٍۭ بَيِّنَٰتٍ ۚ وَلِلْكَٰفِرِينَ عَذَابٌ مُّهِينٌ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbp:text "On the Day when Allah will resurrect them all and inform them of what they did. Allah had enumerated it, while they forgot it; and Allah is, over all things, Witness.";
  dbo:Surah "Al-Mujadilah";
  dbo:ayah "5110"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "وَٱلَّذِينَ يُظَٰهِرُونَ مِن نِّسَآئِهِمْ ثُمَّ يَعُودُونَ لِمَا قَالُوا۟ فَتَحْرِيرُ رَقَبَةٍ مِّن قَبْلِ أَن يَتَمَآسَّا ۚ ذَٰلِكُمْ تُوعَظُونَ بِهِۦ ۚ وَٱللَّهُ بِمَا تَعْمَلُونَ خَبِيرٌ",
    "أَلَمْ تَرَ أَنَّ ٱللَّهَ يَعْلَمُ مَا فِى ٱلسَّمَٰوَٰتِ وَمَا فِى ٱلْأَرْضِ ۖ مَا يَكُونُ مِن نَّجْوَىٰ ثَلَٰثَةٍ إِلَّا هُوَ رَابِعُهُمْ وَلَا خَمْسَةٍ إِلَّا هُوَ سَادِسُهُمْ وَلَآ أَدْنَىٰ مِن ذَٰلِكَ وَلَآ أَكْثَرَ إِلَّا هُوَ مَعَهُمْ أَيْنَ مَا كَانُوا۟ ۖ ثُمَّ يُنَبِّئُهُم بِمَا عَمِلُوا۟ يَوْمَ ٱلْقِيَٰمَةِ ۚ إِنَّ ٱللَّهَ بِكُلِّ شَىْءٍ عَلِيمٌ";
  dbp:text "Have you not considered that Allah knows what is in the heavens and what is on the earth? There is in no private conversation three but that He is the fourth of them, nor are there five but that He is the sixth of them - and no less than that and no more except that He is with them [in knowledge] wherever they are. Then He will inform them of what they did, on the Day of Resurrection. Indeed Allah is, of all things, Knowing.";
  dbo:Surah "Al-Mujadilah";
  dbo:ayah "5111"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "أَلَمْ تَرَ إِلَى ٱلَّذِينَ نُهُوا۟ عَنِ ٱلنَّجْوَىٰ ثُمَّ يَعُودُونَ لِمَا نُهُوا۟ عَنْهُ وَيَتَنَٰجَوْنَ بِٱلْإِثْمِ وَٱلْعُدْوَٰنِ وَمَعْصِيَتِ ٱلرَّسُولِ وَإِذَا جَآءُوكَ حَيَّوْكَ بِمَا لَمْ يُحَيِّكَ بِهِ ٱللَّهُ وَيَقُولُونَ فِىٓ أَنفُسِهِمْ لَوْلَا يُعَذِّبُنَا ٱللَّهُ بِمَا نَقُولُ ۚ حَسْبُهُمْ جَهَنَّمُ يَصْلَوْنَهَا ۖ فَبِئْسَ ٱلْمَصِيرُ";
  dbp:text "Have you not considered those who were forbidden from private conversation, then they return to that which they were forbidden and converse among themselves about sin and aggression and disobedience to the Messenger? And when they come to you, they greet you with that [word] by which Allah does not greet you and say among themselves, \"Why does Allah not punish us for what we say?\" Sufficient for them is Hell, which they will [enter to] burn, and wretched is the destination.";
  dbo:Surah "Al-Mujadilah";
  dbo:ayah "5112"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbp:text "O you who have believed, when you converse privately, do not converse about sin and aggression and disobedience to the Messenger but converse about righteousness and piety. And fear Allah, to whom you will be gathered.";
  dbo:Surah "Al-Mujadilah";
  dbo:ayah "5113"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "يَوْمَ يَبْعَثُهُمُ ٱللَّهُ جَمِيعًا فَيُنَبِّئُهُم بِمَا عَمِلُوٓا۟ ۚ أَحْصَىٰهُ ٱللَّهُ وَنَسُوهُ ۚ وَٱللَّهُ عَلَىٰ كُلِّ شَىْءٍ شَهِيدٌ";
  dbp:text "Private conversation is only from Satan that he may grieve those who have believed, but he will not harm them at all except by permission of Allah. And upon Allah let the believers rely.";
  dbo:Surah "Al-Mujadilah";
  dbo:ayah "5114"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "إِنَّمَا ٱلنَّجْوَىٰ مِنَ ٱلشَّيْطَٰنِ لِيَحْزُنَ ٱلَّذِينَ ءَامَنُوا۟ وَلَيْسَ بِضَآرِّهِمْ شَيْـًٔا إِلَّا بِإِذْنِ ٱللَّهِ ۚ وَعَلَى ٱللَّهِ فَلْيَتَوَكَّلِ ٱلْمُؤْمِنُونَ";
  dbp:text "O you who have believed, when you are told, \"Space yourselves\" in assemblies, then make space; Allah will make space for you. And when you are told, \"Arise,\" then arise; Allah will raise those who have believed among you and those who were given knowledge, by degrees. And Allah is Acquainted with what you do.";
  dbo:Surah "Al-Mujadilah";
  dbo:ayah "5115"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "يَٰٓأَيُّهَا ٱلَّذِينَ ءَامَنُوٓا۟ إِذَا تَنَٰجَيْتُمْ فَلَا تَتَنَٰجَوْا۟ بِٱلْإِثْمِ وَٱلْعُدْوَٰنِ وَمَعْصِيَتِ ٱلرَّسُولِ وَتَنَٰجَوْا۟ بِٱلْبِرِّ وَٱلتَّقْوَىٰ ۖ وَٱتَّقُوا۟ ٱللَّهَ ٱلَّذِىٓ إِلَيْهِ تُحْشَرُونَ";
  dbo:Surah "Al-Mujadilah";
  dbo:ayah "5116"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbp:text "O you who have believed, when you [wish to] privately consult the Messenger, present before your consultation a charity. That is better for you and purer. But if you find not [the means] - then indeed, Allah is Forgiving and Merciful.";
  <http://localhost:3333/has_arabic_text> "يَٰٓأَيُّهَا ٱلَّذِينَ ءَامَنُوٓا۟ إِذَا قِيلَ لَكُمْ تَفَسَّحُوا۟ فِى ٱلْمَجَٰلِسِ فَٱفْسَحُوا۟ يَفْسَحِ ٱللَّهُ لَكُمْ ۖ وَإِذَا قِيلَ ٱنشُزُوا۟ فَٱنشُزُوا۟ يَرْفَعِ ٱللَّهُ ٱلَّذِينَ ءَامَنُوا۟ مِنكُمْ وَٱلَّذِينَ أُوتُوا۟ ٱلْعِلْمَ دَرَجَٰتٍ ۚ وَٱللَّهُ بِمَا تَعْمَلُونَ خَبِيرٌ",
    "ءَأَشْفَقْتُمْ أَن تُقَدِّمُوا۟ بَيْنَ يَدَىْ نَجْوَىٰكُمْ صَدَقَٰتٍ ۚ فَإِذْ لَمْ تَفْعَلُوا۟ وَتَابَ ٱللَّهُ عَلَيْكُمْ فَأَقِيمُوا۟ ٱلصَّلَوٰةَ وَءَاتُوا۟ ٱلزَّكَوٰةَ وَأَطِيعُوا۟ ٱللَّهَ وَرَسُولَهُۥ ۚ وَٱللَّهُ خَبِيرٌۢ بِمَا تَعْمَلُونَ";
  dbp:text "Have you feared to present before your consultation charities? Then when you do not and Allah has forgiven you, then [at least] establish prayer and give zakah and obey Allah and His Messenger. And Allah is Acquainted with what you do.";
  dbo:Surah "Al-Mujadilah";
  dbo:ayah "5117"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbp:text "Have you not considered those who make allies of a people with whom Allah has become angry? They are neither of you nor of them, and they swear to untruth while they know [they are lying].";
  dbo:Surah "Al-Mujadilah";
  dbo:ayah "5118"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "أَلَمْ تَرَ إِلَى ٱلَّذِينَ تَوَلَّوْا۟ قَوْمًا غَضِبَ ٱللَّهُ عَلَيْهِم مَّا هُم مِّنكُمْ وَلَا مِنْهُمْ وَيَحْلِفُونَ عَلَى ٱلْكَذِبِ وَهُمْ يَعْلَمُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5119"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mujadilah";
  dbp:text "Allah has prepared for them a severe punishment. Indeed, it was evil that they were doing.";
  <http://localhost:3333/has_arabic_text> "أَعَدَّ ٱللَّهُ لَهُمْ عَذَابًا شَدِيدًا ۖ إِنَّهُمْ سَآءَ مَا كَانُوا۟ يَعْمَلُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5120"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mujadilah";
  dbp:text "They took their [false] oaths as a cover, so they averted [people] from the way of Allah, and for them is a humiliating punishment.";
  <http://localhost:3333/has_arabic_text> "يَٰٓأَيُّهَا ٱلَّذِينَ ءَامَنُوٓا۟ إِذَا نَٰجَيْتُمُ ٱلرَّسُولَ فَقَدِّمُوا۟ بَيْنَ يَدَىْ نَجْوَىٰكُمْ صَدَقَةً ۚ ذَٰلِكَ خَيْرٌ لَّكُمْ وَأَطْهَرُ ۚ فَإِن لَّمْ تَجِدُوا۟ فَإِنَّ ٱللَّهَ غَفُورٌ رَّحِيمٌ";
  dbo:Surah "Al-Mujadilah";
  dbp:text "Never will their wealth or their children avail them against Allah at all. Those are the companions of the Fire; they will abide therein eternally";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5121"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "لَّن تُغْنِىَ عَنْهُمْ أَمْوَٰلُهُمْ وَلَآ أَوْلَٰدُهُم مِّنَ ٱللَّهِ شَيْـًٔا ۚ أُو۟لَٰٓئِكَ أَصْحَٰبُ ٱلنَّارِ ۖ هُمْ فِيهَا خَٰلِدُونَ";
  dbp:text "On the Day Allah will resurrect them all, and they will swear to Him as they swear to you and think that they are [standing] on something. Unquestionably, it is they who are the liars.";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:Surah "Al-Mujadilah";
  dbo:ayah "5122"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "يَوْمَ يَبْعَثُهُمُ ٱللَّهُ جَمِيعًا فَيَحْلِفُونَ لَهُۥ كَمَا يَحْلِفُونَ لَكُمْ ۖ وَيَحْسَبُونَ أَنَّهُمْ عَلَىٰ شَىْءٍ ۚ أَلَآ إِنَّهُمْ هُمُ ٱلْكَٰذِبُونَ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbp:text "Satan has overcome them and made them forget the remembrance of Allah. Those are the party of Satan. Unquestionably, the party of Satan - they will be the losers.";
  dbo:Surah "Al-Mujadilah";
  dbo:ayah "5123"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "ٱتَّخَذُوٓا۟ أَيْمَٰنَهُمْ جُنَّةً فَصَدُّوا۟ عَن سَبِيلِ ٱللَّهِ فَلَهُمْ عَذَابٌ مُّهِينٌ";
  dbp:text "Indeed, the ones who oppose Allah and His Messenger - those will be among the most humbled.";
  dbo:Surah "Al-Mujadilah";
  dbo:ayah "5124"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "إِنَّ ٱلَّذِينَ يُحَآدُّونَ ٱللَّهَ وَرَسُولَهُۥٓ أُو۟لَٰٓئِكَ فِى ٱلْأَذَلِّينَ";
  dbo:ayah "5125"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:Surah "Al-Mujadilah";
  dbp:text "Allah has written, \"I will surely overcome, I and My messengers.\" Indeed, Allah is Powerful and Exalted in Might.";
  <http://localhost:3333/has_arabic_text> "كَتَبَ ٱللَّهُ لَأَغْلِبَنَّ أَنَا۠ وَرُسُلِىٓ ۚ إِنَّ ٱللَّهَ قَوِىٌّ عَزِيزٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5126"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mujadilah";
  dbp:text "You will not find a people who believe in Allah and the Last Day having affection for those who oppose Allah and His Messenger, even if they were their fathers or their sons or their brothers or their kindred. Those - He has decreed within their hearts faith and supported them with spirit from Him. And We will admit them to gardens beneath which rivers flow, wherein they abide eternally. Allah is pleased with them, and they are pleased with Him - those are the party of Allah. Unquestionably, the party of Allah - they are the successful.";
  <http://localhost:3333/has_arabic_text> "لَّا تَجِدُ قَوْمًا يُؤْمِنُونَ بِٱللَّهِ وَٱلْيَوْمِ ٱلْءَاخِرِ يُوَآدُّونَ مَنْ حَآدَّ ٱللَّهَ وَرَسُولَهُۥ وَلَوْ كَانُوٓا۟ ءَابَآءَهُمْ أَوْ أَبْنَآءَهُمْ أَوْ إِخْوَٰنَهُمْ أَوْ عَشِيرَتَهُمْ ۚ أُو۟لَٰٓئِكَ كَتَبَ فِى قُلُوبِهِمُ ٱلْإِيمَٰنَ وَأَيَّدَهُم بِرُوحٍ مِّنْهُ ۖ وَيُدْخِلُهُمْ جَنَّٰتٍ تَجْرِى مِن تَحْتِهَا ٱلْأَنْهَٰرُ خَٰلِدِينَ فِيهَا ۚ رَضِىَ ٱللَّهُ عَنْهُمْ وَرَضُوا۟ عَنْهُ ۚ أُو۟لَٰٓئِكَ حِزْبُ ٱللَّهِ ۚ أَلَآ إِنَّ حِزْبَ ٱللَّهِ هُمُ ٱلْمُفْلِحُونَ",
    "ٱسْتَحْوَذَ عَلَيْهِمُ ٱلشَّيْطَٰنُ فَأَنسَىٰهُمْ ذِكْرَ ٱللَّهِ ۚ أُو۟لَٰٓئِكَ حِزْبُ ٱلشَّيْطَٰنِ ۚ أَلَآ إِنَّ حِزْبَ ٱلشَّيْطَٰنِ هُمُ ٱلْخَٰسِرُونَ",
    "ٱلَّذِينَ يُظَٰهِرُونَ مِنكُم مِّن نِّسَآئِهِم مَّا هُنَّ أُمَّهَٰتِهِمْ ۖ إِنْ أُمَّهَٰتُهُمْ إِلَّا ٱلَّٰٓـِٔى وَلَدْنَهُمْ ۚ وَإِنَّهُمْ لَيَقُولُونَ مُنكَرًا مِّنَ ٱلْقَوْلِ وَزُورًا ۚ وَإِنَّ ٱللَّهَ لَعَفُوٌّ غَفُورٌ" .

<http://localhost:3333/Al-Hashr> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Hashr";
  dbo:ayah "5127"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "Whatever is in the heavens and whatever is on the earth exalts Allah, and He is the Exalted in Might, the Wise.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbp:text "It is He who expelled the ones who disbelieved among the People of the Scripture from their homes at the first gathering. You did not think they would leave, and they thought that their fortresses would protect them from Allah; but [the decree of] Allah came upon them from where they had not expected, and He cast terror into their hearts [so] they destroyed their houses by their [own] hands and the hands of the believers. So take warning, O people of vision.";
  dbo:ayah "5128"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Hashr";
  dbo:ayah "5129"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "And if not that Allah had decreed for them evacuation, He would have punished them in [this] world, and for them in the Hereafter is the punishment of the Fire.";
  dbo:Surah "Al-Hashr";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "سَبَّحَ لِلَّهِ مَا فِى ٱلسَّمَٰوَٰتِ وَمَا فِى ٱلْأَرْضِ ۖ وَهُوَ ٱلْعَزِيزُ ٱلْحَكِيمُ";
  dbp:text "That is because they opposed Allah and His Messenger. And whoever opposes Allah - then indeed, Allah is severe in penalty.";
  dbo:ayah "5130"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Hashr";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "ذَٰلِكَ بِأَنَّهُمْ شَآقُّوا۟ ٱللَّهَ وَرَسُولَهُۥ ۖ وَمَن يُشَآقِّ ٱللَّهَ فَإِنَّ ٱللَّهَ شَدِيدُ ٱلْعِقَابِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5131"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Hashr";
  dbp:text "Whatever you have cut down of [their] palm trees or left standing on their trunks - it was by permission of Allah and so He would disgrace the defiantly disobedient.";
  <http://localhost:3333/has_arabic_text> "مَا قَطَعْتُم مِّن لِّينَةٍ أَوْ تَرَكْتُمُوهَا قَآئِمَةً عَلَىٰٓ أُصُولِهَا فَبِإِذْنِ ٱللَّهِ وَلِيُخْزِىَ ٱلْفَٰسِقِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5132"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Hashr";
  dbp:text "And what Allah restored [of property] to His Messenger from them - you did not spur for it [in an expedition] any horses or camels, but Allah gives His messengers power over whom He wills, and Allah is over all things competent.";
  <http://localhost:3333/has_arabic_text> "هُوَ ٱلَّذِىٓ أَخْرَجَ ٱلَّذِينَ كَفَرُوا۟ مِنْ أَهْلِ ٱلْكِتَٰبِ مِن دِيَٰرِهِمْ لِأَوَّلِ ٱلْحَشْرِ ۚ مَا ظَنَنتُمْ أَن يَخْرُجُوا۟ ۖ وَظَنُّوٓا۟ أَنَّهُم مَّانِعَتُهُمْ حُصُونُهُم مِّنَ ٱللَّهِ فَأَتَىٰهُمُ ٱللَّهُ مِنْ حَيْثُ لَمْ يَحْتَسِبُوا۟ ۖ وَقَذَفَ فِى قُلُوبِهِمُ ٱلرُّعْبَ ۚ يُخْرِبُونَ بُيُوتَهُم بِأَيْدِيهِمْ وَأَيْدِى ٱلْمُؤْمِنِينَ فَٱعْتَبِرُوا۟ يَٰٓأُو۟لِى ٱلْأَبْصَٰرِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5133"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Hashr";
  dbp:text "And what Allah restored to His Messenger from the people of the towns - it is for Allah and for the Messenger and for [his] near relatives and orphans and the [stranded] traveler - so that it will not be a perpetual distribution among the rich from among you. And whatever the Messenger has given you - take; and what he has forbidden you - refrain from. And fear Allah; indeed, Allah is severe in penalty.";
  <http://localhost:3333/has_arabic_text> "مَّآ أَفَآءَ ٱللَّهُ عَلَىٰ رَسُولِهِۦ مِنْ أَهْلِ ٱلْقُرَىٰ فَلِلَّهِ وَلِلرَّسُولِ وَلِذِى ٱلْقُرْبَىٰ وَٱلْيَتَٰمَىٰ وَٱلْمَسَٰكِينِ وَٱبْنِ ٱلسَّبِيلِ كَىْ لَا يَكُونَ دُولَةًۢ بَيْنَ ٱلْأَغْنِيَآءِ مِنكُمْ ۚ وَمَآ ءَاتَىٰكُمُ ٱلرَّسُولُ فَخُذُوهُ وَمَا نَهَىٰكُمْ عَنْهُ فَٱنتَهُوا۟ ۚ وَٱتَّقُوا۟ ٱللَّهَ ۖ إِنَّ ٱللَّهَ شَدِيدُ ٱلْعِقَابِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5134"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Hashr";
  dbp:text "For the poor emigrants who were expelled from their homes and their properties, seeking bounty from Allah and [His] approval and supporting Allah and His Messenger, [there is also a share]. Those are the truthful.";
  <http://localhost:3333/has_arabic_text> "لِلْفُقَرَآءِ ٱلْمُهَٰجِرِينَ ٱلَّذِينَ أُخْرِجُوا۟ مِن دِيَٰرِهِمْ وَأَمْوَٰلِهِمْ يَبْتَغُونَ فَضْلًا مِّنَ ٱللَّهِ وَرِضْوَٰنًا وَيَنصُرُونَ ٱللَّهَ وَرَسُولَهُۥٓ ۚ أُو۟لَٰٓئِكَ هُمُ ٱلصَّٰدِقُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5135"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Hashr";
  dbp:text "And [also for] those who were settled in al-Madinah and [adopted] the faith before them. They love those who emigrated to them and find not any want in their breasts of what the emigrants were given but give [them] preference over themselves, even though they are in privation. And whoever is protected from the stinginess of his soul - it is those who will be the successful.";
  <http://localhost:3333/has_arabic_text> "وَلَوْلَآ أَن كَتَبَ ٱللَّهُ عَلَيْهِمُ ٱلْجَلَآءَ لَعَذَّبَهُمْ فِى ٱلدُّنْيَا ۖ وَلَهُمْ فِى ٱلْءَاخِرَةِ عَذَابُ ٱلنَّارِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5136"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Hashr";
  dbp:text "And [there is a share for] those who came after them, saying, \"Our Lord, forgive us and our brothers who preceded us in faith and put not in our hearts [any] resentment toward those who have believed. Our Lord, indeed You are Kind and Merciful.\"";
  <http://localhost:3333/has_arabic_text> "وَمَآ أَفَآءَ ٱللَّهُ عَلَىٰ رَسُولِهِۦ مِنْهُمْ فَمَآ أَوْجَفْتُمْ عَلَيْهِ مِنْ خَيْلٍ وَلَا رِكَابٍ وَلَٰكِنَّ ٱللَّهَ يُسَلِّطُ رُسُلَهُۥ عَلَىٰ مَن يَشَآءُ ۚ وَٱللَّهُ عَلَىٰ كُلِّ شَىْءٍ قَدِيرٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5137"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Hashr";
  dbp:text "Have you not considered those who practice hypocrisy, saying to their brothers who have disbelieved among the People of the Scripture, \"If you are expelled, we will surely leave with you, and we will not obey, in regard to you, anyone - ever; and if you are fought, we will surely aid you.\" But Allah testifies that they are liars.";
  <http://localhost:3333/has_arabic_text> "أَلَمْ تَرَ إِلَى ٱلَّذِينَ نَافَقُوا۟ يَقُولُونَ لِإِخْوَٰنِهِمُ ٱلَّذِينَ كَفَرُوا۟ مِنْ أَهْلِ ٱلْكِتَٰبِ لَئِنْ أُخْرِجْتُمْ لَنَخْرُجَنَّ مَعَكُمْ وَلَا نُطِيعُ فِيكُمْ أَحَدًا أَبَدًا وَإِن قُوتِلْتُمْ لَنَنصُرَنَّكُمْ وَٱللَّهُ يَشْهَدُ إِنَّهُمْ لَكَٰذِبُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5138"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Hashr";
  dbp:text "If they are expelled, they will not leave with them, and if they are fought, they will not aid them. And [even] if they should aid them, they will surely turn their backs; then [thereafter] they will not be aided.";
  <http://localhost:3333/has_arabic_text> "لَئِنْ أُخْرِجُوا۟ لَا يَخْرُجُونَ مَعَهُمْ وَلَئِن قُوتِلُوا۟ لَا يَنصُرُونَهُمْ وَلَئِن نَّصَرُوهُمْ لَيُوَلُّنَّ ٱلْأَدْبَٰرَ ثُمَّ لَا يُنصَرُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5139"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Hashr";
  dbp:text "You [believers] are more fearful within their breasts than Allah. That is because they are a people who do not understand.";
  <http://localhost:3333/has_arabic_text> "لَأَنتُمْ أَشَدُّ رَهْبَةً فِى صُدُورِهِم مِّنَ ٱللَّهِ ۚ ذَٰلِكَ بِأَنَّهُمْ قَوْمٌ لَّا يَفْقَهُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5140"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Hashr";
  dbp:text "They will not fight you all except within fortified cities or from behind walls. Their violence among themselves is severe. You think they are together, but their hearts are diverse. That is because they are a people who do not reason.";
  <http://localhost:3333/has_arabic_text> "لَا يُقَٰتِلُونَكُمْ جَمِيعًا إِلَّا فِى قُرًى مُّحَصَّنَةٍ أَوْ مِن وَرَآءِ جُدُرٍۭ ۚ بَأْسُهُم بَيْنَهُمْ شَدِيدٌ ۚ تَحْسَبُهُمْ جَمِيعًا وَقُلُوبُهُمْ شَتَّىٰ ۚ ذَٰلِكَ بِأَنَّهُمْ قَوْمٌ لَّا يَعْقِلُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5141"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Hashr";
  dbp:text "[Theirs is] like the example of those shortly before them: they tasted the bad consequence of their affair, and they will have a painful punishment.";
  <http://localhost:3333/has_arabic_text> "كَمَثَلِ ٱلَّذِينَ مِن قَبْلِهِمْ قَرِيبًا ۖ ذَاقُوا۟ وَبَالَ أَمْرِهِمْ وَلَهُمْ عَذَابٌ أَلِيمٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5142"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Hashr";
  dbp:text "[The hypocrites are] like the example of Satan when he says to man, \"Disbelieve.\" But when he disbelieves, he says, \"Indeed, I am disassociated from you. Indeed, I fear Allah, Lord of the worlds.\"";
  <http://localhost:3333/has_arabic_text> "كَمَثَلِ ٱلشَّيْطَٰنِ إِذْ قَالَ لِلْإِنسَٰنِ ٱكْفُرْ فَلَمَّا كَفَرَ قَالَ إِنِّى بَرِىٓءٌ مِّنكَ إِنِّىٓ أَخَافُ ٱللَّهَ رَبَّ ٱلْعَٰلَمِينَ";
  dbp:text "So the outcome for both of them is that they will be in the Fire, abiding eternally therein. And that is the recompense of the wrong-doers.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5143"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Hashr";
  <http://localhost:3333/has_arabic_text> "فَكَانَ عَٰقِبَتَهُمَآ أَنَّهُمَا فِى ٱلنَّارِ خَٰلِدَيْنِ فِيهَا ۚ وَذَٰلِكَ جَزَٰٓؤُا۟ ٱلظَّٰلِمِينَ",
    "وَٱلَّذِينَ تَبَوَّءُو ٱلدَّارَ وَٱلْإِيمَٰنَ مِن قَبْلِهِمْ يُحِبُّونَ مَنْ هَاجَرَ إِلَيْهِمْ وَلَا يَجِدُونَ فِى صُدُورِهِمْ حَاجَةً مِّمَّآ أُوتُوا۟ وَيُؤْثِرُونَ عَلَىٰٓ أَنفُسِهِمْ وَلَوْ كَانَ بِهِمْ خَصَاصَةٌ ۚ وَمَن يُوقَ شُحَّ نَفْسِهِۦ فَأُو۟لَٰٓئِكَ هُمُ ٱلْمُفْلِحُونَ";
  dbp:text "O you who have believed, fear Allah. And let every soul look to what it has put forth for tomorrow - and fear Allah. Indeed, Allah is Acquainted with what you do.";
  dbo:Surah "Al-Hashr";
  dbo:ayah "5144"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah, dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5145"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Hashr";
  dbp:text "And be not like those who forgot Allah, so He made them forget themselves. Those are the defiantly disobedient.";
  <http://localhost:3333/has_arabic_text> "وَلَا تَكُونُوا۟ كَٱلَّذِينَ نَسُوا۟ ٱللَّهَ فَأَنسَىٰهُمْ أَنفُسَهُمْ ۚ أُو۟لَٰٓئِكَ هُمُ ٱلْفَٰسِقُونَ",
    "لَا يَسْتَوِىٓ أَصْحَٰبُ ٱلنَّارِ وَأَصْحَٰبُ ٱلْجَنَّةِ ۚ أَصْحَٰبُ ٱلْجَنَّةِ هُمُ ٱلْفَآئِزُونَ";
  dbo:Surah "Al-Hashr";
  dbp:text "Not equal are the companions of the Fire and the companions of Paradise. The companions of Paradise - they are the attainers [of success].";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5146"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Hashr";
  dbo:ayah "5147"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbp:text "If We had sent down this Qur'an upon a mountain, you would have seen it humbled and coming apart from fear of Allah. And these examples We present to the people that perhaps they will give thought.";
  <http://localhost:3333/has_arabic_text> "لَوْ أَنزَلْنَا هَٰذَا ٱلْقُرْءَانَ عَلَىٰ جَبَلٍ لَّرَأَيْتَهُۥ خَٰشِعًا مُّتَصَدِّعًا مِّنْ خَشْيَةِ ٱللَّهِ ۚ وَتِلْكَ ٱلْأَمْثَٰلُ نَضْرِبُهَا لِلنَّاسِ لَعَلَّهُمْ يَتَفَكَّرُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5148"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Hashr";
  dbp:text "He is Allah, other than whom there is no deity, Knower of the unseen and the witnessed. He is the Entirely Merciful, the Especially Merciful.";
  <http://localhost:3333/has_arabic_text> "هُوَ ٱللَّهُ ٱلَّذِى لَآ إِلَٰهَ إِلَّا هُوَ ۖ عَٰلِمُ ٱلْغَيْبِ وَٱلشَّهَٰدَةِ ۖ هُوَ ٱلرَّحْمَٰنُ ٱلرَّحِيمُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5149"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Hashr";
  dbp:text "He is Allah, other than whom there is no deity, the Sovereign, the Pure, the Perfection, the Bestower of Faith, the Overseer, the Exalted in Might, the Compeller, the Superior. Exalted is Allah above whatever they associate with Him.";
  <http://localhost:3333/has_arabic_text> "هُوَ ٱللَّهُ ٱلَّذِى لَآ إِلَٰهَ إِلَّا هُوَ ٱلْمَلِكُ ٱلْقُدُّوسُ ٱلسَّلَٰمُ ٱلْمُؤْمِنُ ٱلْمُهَيْمِنُ ٱلْعَزِيزُ ٱلْجَبَّارُ ٱلْمُتَكَبِّرُ ۚ سُبْحَٰنَ ٱللَّهِ عَمَّا يُشْرِكُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5150"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Hashr";
  dbp:text "He is Allah, the Creator, the Inventor, the Fashioner; to Him belong the best names. Whatever is in the heavens and earth is exalting Him. And He is the Exalted in Might, the Wise.";
  <http://localhost:3333/has_arabic_text> "هُوَ ٱللَّهُ ٱلْخَٰلِقُ ٱلْبَارِئُ ٱلْمُصَوِّرُ ۖ لَهُ ٱلْأَسْمَآءُ ٱلْحُسْنَىٰ ۚ يُسَبِّحُ لَهُۥ مَا فِى ٱلسَّمَٰوَٰتِ وَٱلْأَرْضِ ۖ وَهُوَ ٱلْعَزِيزُ ٱلْحَكِيمُ",
    "وَٱلَّذِينَ جَآءُو مِنۢ بَعْدِهِمْ يَقُولُونَ رَبَّنَا ٱغْفِرْ لَنَا وَلِإِخْوَٰنِنَا ٱلَّذِينَ سَبَقُونَا بِٱلْإِيمَٰنِ وَلَا تَجْعَلْ فِى قُلُوبِنَا غِلًّا لِّلَّذِينَ ءَامَنُوا۟ رَبَّنَآ إِنَّكَ رَءُوفٌ رَّحِيمٌ",
    "يَٰٓأَيُّهَا ٱلَّذِينَ ءَامَنُوا۟ ٱتَّقُوا۟ ٱللَّهَ وَلْتَنظُرْ نَفْسٌ مَّا قَدَّمَتْ لِغَدٍ ۖ وَٱتَّقُوا۟ ٱللَّهَ ۚ إِنَّ ٱللَّهَ خَبِيرٌۢ بِمَا تَعْمَلُونَ" .

<http://localhost:3333/Al-Mumtahanah> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Mumtahanah";
  dbo:ayah "5151"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5152"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mumtahanah";
  dbp:text "If they gain dominance over you, they would be to you as enemies and extend against you their hands and their tongues with evil, and they wish you would disbelieve.",
    "O you who have believed, do not take My enemies and your enemies as allies, extending to them affection while they have disbelieved in what came to you of the truth, having driven out the Prophet and yourselves [only] because you believe in Allah, your Lord. If you have come out for jihad in My cause and seeking means to My approval, [take them not as friends]. You confide to them affection, but I am most knowing of what you have concealed and what you have declared. And whoever does it among you has certainly strayed from the soundness of the way.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5153"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mumtahanah";
  dbp:text "Never will your relatives or your children benefit you; the Day of Resurrection He will judge between you. And Allah, of what you do, is Seeing.";
  <http://localhost:3333/has_arabic_text> "إِن يَثْقَفُوكُمْ يَكُونُوا۟ لَكُمْ أَعْدَآءً وَيَبْسُطُوٓا۟ إِلَيْكُمْ أَيْدِيَهُمْ وَأَلْسِنَتَهُم بِٱلسُّوٓءِ وَوَدُّوا۟ لَوْ تَكْفُرُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5154"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mumtahanah";
  dbp:text "There has already been for you an excellent pattern in Abraham and those with him, when they said to their people, \"Indeed, we are disassociated from you and from whatever you worship other than Allah. We have denied you, and there has appeared between us and you animosity and hatred forever until you believe in Allah alone\" except for the saying of Abraham to his father, \"I will surely ask forgiveness for you, but I have not [power to do] for you anything against Allah. Our Lord, upon You we have relied, and to You we have returned, and to You is the destination.";
  <http://localhost:3333/has_arabic_text> "قَدْ كَانَتْ لَكُمْ أُسْوَةٌ حَسَنَةٌ فِىٓ إِبْرَٰهِيمَ وَٱلَّذِينَ مَعَهُۥٓ إِذْ قَالُوا۟ لِقَوْمِهِمْ إِنَّا بُرَءَٰٓؤُا۟ مِنكُمْ وَمِمَّا تَعْبُدُونَ مِن دُونِ ٱللَّهِ كَفَرْنَا بِكُمْ وَبَدَا بَيْنَنَا وَبَيْنَكُمُ ٱلْعَدَٰوَةُ وَٱلْبَغْضَآءُ أَبَدًا حَتَّىٰ تُؤْمِنُوا۟ بِٱللَّهِ وَحْدَهُۥٓ إِلَّا قَوْلَ إِبْرَٰهِيمَ لِأَبِيهِ لَأَسْتَغْفِرَنَّ لَكَ وَمَآ أَمْلِكُ لَكَ مِنَ ٱللَّهِ مِن شَىْءٍ ۖ رَّبَّنَا عَلَيْكَ تَوَكَّلْنَا وَإِلَيْكَ أَنَبْنَا وَإِلَيْكَ ٱلْمَصِيرُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5155"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mumtahanah";
  dbp:text "Our Lord, make us not [objects of] torment for the disbelievers and forgive us, our Lord. Indeed, it is You who is the Exalted in Might, the Wise.\"";
  <http://localhost:3333/has_arabic_text> "رَبَّنَا لَا تَجْعَلْنَا فِتْنَةً لِّلَّذِينَ كَفَرُوا۟ وَٱغْفِرْ لَنَا رَبَّنَآ ۖ إِنَّكَ أَنتَ ٱلْعَزِيزُ ٱلْحَكِيمُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5156"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mumtahanah";
  dbp:text "There has certainly been for you in them an excellent pattern for anyone whose hope is in Allah and the Last Day. And whoever turns away - then indeed, Allah is the Free of need, the Praiseworthy.";
  <http://localhost:3333/has_arabic_text> "لَقَدْ كَانَ لَكُمْ فِيهِمْ أُسْوَةٌ حَسَنَةٌ لِّمَن كَانَ يَرْجُوا۟ ٱللَّهَ وَٱلْيَوْمَ ٱلْءَاخِرَ ۚ وَمَن يَتَوَلَّ فَإِنَّ ٱللَّهَ هُوَ ٱلْغَنِىُّ ٱلْحَمِيدُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5157"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mumtahanah";
  dbp:text "Perhaps Allah will put, between you and those to whom you have been enemies among them, affection. And Allah is competent, and Allah is Forgiving and Merciful.";
  <http://localhost:3333/has_arabic_text> "عَسَى ٱللَّهُ أَن يَجْعَلَ بَيْنَكُمْ وَبَيْنَ ٱلَّذِينَ عَادَيْتُم مِّنْهُم مَّوَدَّةً ۚ وَٱللَّهُ قَدِيرٌ ۚ وَٱللَّهُ غَفُورٌ رَّحِيمٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5158"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mumtahanah";
  dbp:text "Allah does not forbid you from those who do not fight you because of religion and do not expel you from your homes - from being righteous toward them and acting justly toward them. Indeed, Allah loves those who act justly.";
  <http://localhost:3333/has_arabic_text> "لَن تَنفَعَكُمْ أَرْحَامُكُمْ وَلَآ أَوْلَٰدُكُمْ ۚ يَوْمَ ٱلْقِيَٰمَةِ يَفْصِلُ بَيْنَكُمْ ۚ وَٱللَّهُ بِمَا تَعْمَلُونَ بَصِيرٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5159"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mumtahanah";
  dbp:text "Allah only forbids you from those who fight you because of religion and expel you from your homes and aid in your expulsion - [forbids] that you make allies of them. And whoever makes allies of them, then it is those who are the wrongdoers.";
  <http://localhost:3333/has_arabic_text> "إِنَّمَا يَنْهَىٰكُمُ ٱللَّهُ عَنِ ٱلَّذِينَ قَٰتَلُوكُمْ فِى ٱلدِّينِ وَأَخْرَجُوكُم مِّن دِيَٰرِكُمْ وَظَٰهَرُوا۟ عَلَىٰٓ إِخْرَاجِكُمْ أَن تَوَلَّوْهُمْ ۚ وَمَن يَتَوَلَّهُمْ فَأُو۟لَٰٓئِكَ هُمُ ٱلظَّٰلِمُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5160"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mumtahanah";
  dbp:text "O you who have believed, when the believing women come to you as emigrants, examine them. Allah is most knowing as to their faith. And if you know them to be believers, then do not return them to the disbelievers; they are not lawful [wives] for them, nor are they lawful [husbands] for them. But give the disbelievers what they have spent. And there is no blame upon you if you marry them when you have given them their due compensation. And hold not to marriage bonds with disbelieving women, but ask for what you have spent and let them ask for what they have spent. That is the judgement of Allah; He judges between you. And Allah is Knowing and Wise.";
  <http://localhost:3333/has_arabic_text> "لَّا يَنْهَىٰكُمُ ٱللَّهُ عَنِ ٱلَّذِينَ لَمْ يُقَٰتِلُوكُمْ فِى ٱلدِّينِ وَلَمْ يُخْرِجُوكُم مِّن دِيَٰرِكُمْ أَن تَبَرُّوهُمْ وَتُقْسِطُوٓا۟ إِلَيْهِمْ ۚ إِنَّ ٱللَّهَ يُحِبُّ ٱلْمُقْسِطِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5161"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mumtahanah";
  dbp:text "And if you have lost any of your wives to the disbelievers and you subsequently obtain [something], then give those whose wives have gone the equivalent of what they had spent. And fear Allah, in whom you are believers.";
  <http://localhost:3333/has_arabic_text> "وَإِن فَاتَكُمْ شَىْءٌ مِّنْ أَزْوَٰجِكُمْ إِلَى ٱلْكُفَّارِ فَعَاقَبْتُمْ فَـَٔاتُوا۟ ٱلَّذِينَ ذَهَبَتْ أَزْوَٰجُهُم مِّثْلَ مَآ أَنفَقُوا۟ ۚ وَٱتَّقُوا۟ ٱللَّهَ ٱلَّذِىٓ أَنتُم بِهِۦ مُؤْمِنُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5162"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mumtahanah";
  dbp:text "O Prophet, when the believing women come to you pledging to you that they will not associate anything with Allah, nor will they steal, nor will they commit unlawful sexual intercourse, nor will they kill their children, nor will they bring forth a slander they have invented between their arms and legs, nor will they disobey you in what is right - then accept their pledge and ask forgiveness for them of Allah. Indeed, Allah is Forgiving and Merciful.";
  <http://localhost:3333/has_arabic_text> "يَٰٓأَيُّهَا ٱلنَّبِىُّ إِذَا جَآءَكَ ٱلْمُؤْمِنَٰتُ يُبَايِعْنَكَ عَلَىٰٓ أَن لَّا يُشْرِكْنَ بِٱللَّهِ شَيْـًٔا وَلَا يَسْرِقْنَ وَلَا يَزْنِينَ وَلَا يَقْتُلْنَ أَوْلَٰدَهُنَّ وَلَا يَأْتِينَ بِبُهْتَٰنٍ يَفْتَرِينَهُۥ بَيْنَ أَيْدِيهِنَّ وَأَرْجُلِهِنَّ وَلَا يَعْصِينَكَ فِى مَعْرُوفٍ ۙ فَبَايِعْهُنَّ وَٱسْتَغْفِرْ لَهُنَّ ٱللَّهَ ۖ إِنَّ ٱللَّهَ غَفُورٌ رَّحِيمٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5163"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mumtahanah";
  dbp:text "O you who have believed, do not make allies of a people with whom Allah has become angry. They have despaired of [reward in] the Hereafter just as the disbelievers have despaired of [meeting] the inhabitants of the graves.";
  <http://localhost:3333/has_arabic_text> "يَٰٓأَيُّهَا ٱلَّذِينَ ءَامَنُوا۟ لَا تَتَوَلَّوْا۟ قَوْمًا غَضِبَ ٱللَّهُ عَلَيْهِمْ قَدْ يَئِسُوا۟ مِنَ ٱلْءَاخِرَةِ كَمَا يَئِسَ ٱلْكُفَّارُ مِنْ أَصْحَٰبِ ٱلْقُبُورِ",
    "يَٰٓأَيُّهَا ٱلَّذِينَ ءَامَنُوا۟ لَا تَتَّخِذُوا۟ عَدُوِّى وَعَدُوَّكُمْ أَوْلِيَآءَ تُلْقُونَ إِلَيْهِم بِٱلْمَوَدَّةِ وَقَدْ كَفَرُوا۟ بِمَا جَآءَكُم مِّنَ ٱلْحَقِّ يُخْرِجُونَ ٱلرَّسُولَ وَإِيَّاكُمْ ۙ أَن تُؤْمِنُوا۟ بِٱللَّهِ رَبِّكُمْ إِن كُنتُمْ خَرَجْتُمْ جِهَٰدًا فِى سَبِيلِى وَٱبْتِغَآءَ مَرْضَاتِى ۚ تُسِرُّونَ إِلَيْهِم بِٱلْمَوَدَّةِ وَأَنَا۠ أَعْلَمُ بِمَآ أَخْفَيْتُمْ وَمَآ أَعْلَنتُمْ ۚ وَمَن يَفْعَلْهُ مِنكُمْ فَقَدْ ضَلَّ سَوَآءَ ٱلسَّبِيلِ",
    "يَٰٓأَيُّهَا ٱلَّذِينَ ءَامَنُوٓا۟ إِذَا جَآءَكُمُ ٱلْمُؤْمِنَٰتُ مُهَٰجِرَٰتٍ فَٱمْتَحِنُوهُنَّ ۖ ٱللَّهُ أَعْلَمُ بِإِيمَٰنِهِنَّ ۖ فَإِنْ عَلِمْتُمُوهُنَّ مُؤْمِنَٰتٍ فَلَا تَرْجِعُوهُنَّ إِلَى ٱلْكُفَّارِ ۖ لَا هُنَّ حِلٌّ لَّهُمْ وَلَا هُمْ يَحِلُّونَ لَهُنَّ ۖ وَءَاتُوهُم مَّآ أَنفَقُوا۟ ۚ وَلَا جُنَاحَ عَلَيْكُمْ أَن تَنكِحُوهُنَّ إِذَآ ءَاتَيْتُمُوهُنَّ أُجُورَهُنَّ ۚ وَلَا تُمْسِكُوا۟ بِعِصَمِ ٱلْكَوَافِرِ وَسْـَٔلُوا۟ مَآ أَنفَقْتُمْ وَلْيَسْـَٔلُوا۟ مَآ أَنفَقُوا۟ ۚ ذَٰلِكُمْ حُكْمُ ٱللَّهِ ۖ يَحْكُمُ بَيْنَكُمْ ۚ وَٱللَّهُ عَلِيمٌ حَكِيمٌ" .

<http://localhost:3333/As-Saff> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "As-Saff";
  dbo:ayah "5164"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5165"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "As-Saff";
  dbp:text "O you who have believed, why do you say what you do not do?", "Whatever is in the heavens and whatever is on the earth exalts Allah, and He is the Exalted in Might, the Wise.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5166"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "As-Saff";
  dbp:text "Great is hatred in the sight of Allah that you say what you do not do.";
  <http://localhost:3333/has_arabic_text> "سَبَّحَ لِلَّهِ مَا فِى ٱلسَّمَٰوَٰتِ وَمَا فِى ٱلْأَرْضِ ۖ وَهُوَ ٱلْعَزِيزُ ٱلْحَكِيمُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5167"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "As-Saff";
  dbp:text "Indeed, Allah loves those who fight in His cause in a row as though they are a [single] structure joined firmly.";
  <http://localhost:3333/has_arabic_text> "إِنَّ ٱللَّهَ يُحِبُّ ٱلَّذِينَ يُقَٰتِلُونَ فِى سَبِيلِهِۦ صَفًّا كَأَنَّهُم بُنْيَٰنٌ مَّرْصُوصٌ";
  a dbr:Ayah;
  dbp:text "And [mention, O Muhammad], when Moses said to his people, \"O my people, why do you harm me while you certainly know that I am the messenger of Allah to you?\" And when they deviated, Allah caused their hearts to deviate. And Allah does not guide the defiantly disobedient people.";
  a dbr:Quranic_verse;
  dbo:Surah "As-Saff";
  dbo:ayah "5168"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "كَبُرَ مَقْتًا عِندَ ٱللَّهِ أَن تَقُولُوا۟ مَا لَا تَفْعَلُونَ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbp:text "And [mention] when Jesus, the son of Mary, said, \"O children of Israel, indeed I am the messenger of Allah to you confirming what came before me of the Torah and bringing good tidings of a messenger to come after me, whose name is Ahmad.\" But when he came to them with clear evidences, they said, \"This is obvious magic.\"";
  dbo:Surah "As-Saff";
  dbo:ayah "5169"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "وَإِذْ قَالَ عِيسَى ٱبْنُ مَرْيَمَ يَٰبَنِىٓ إِسْرَٰٓءِيلَ إِنِّى رَسُولُ ٱللَّهِ إِلَيْكُم مُّصَدِّقًا لِّمَا بَيْنَ يَدَىَّ مِنَ ٱلتَّوْرَىٰةِ وَمُبَشِّرًۢا بِرَسُولٍ يَأْتِى مِنۢ بَعْدِى ٱسْمُهُۥٓ أَحْمَدُ ۖ فَلَمَّا جَآءَهُم بِٱلْبَيِّنَٰتِ قَالُوا۟ هَٰذَا سِحْرٌ مُّبِينٌ";
  dbp:text "And who is more unjust than one who invents about Allah untruth while he is being invited to Islam. And Allah does not guide the wrongdoing people.";
  dbo:Surah "As-Saff";
  dbo:ayah "5170"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "وَإِذْ قَالَ مُوسَىٰ لِقَوْمِهِۦ يَٰقَوْمِ لِمَ تُؤْذُونَنِى وَقَد تَّعْلَمُونَ أَنِّى رَسُولُ ٱللَّهِ إِلَيْكُمْ ۖ فَلَمَّا زَاغُوٓا۟ أَزَاغَ ٱللَّهُ قُلُوبَهُمْ ۚ وَٱللَّهُ لَا يَهْدِى ٱلْقَوْمَ ٱلْفَٰسِقِينَ";
  dbp:text "They want to extinguish the light of Allah with their mouths, but Allah will perfect His light, although the disbelievers dislike it.";
  dbo:Surah "As-Saff";
  dbo:ayah "5171"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "وَمَنْ أَظْلَمُ مِمَّنِ ٱفْتَرَىٰ عَلَى ٱللَّهِ ٱلْكَذِبَ وَهُوَ يُدْعَىٰٓ إِلَى ٱلْإِسْلَٰمِ ۚ وَٱللَّهُ لَا يَهْدِى ٱلْقَوْمَ ٱلظَّٰلِمِينَ",
    "هُوَ ٱلَّذِىٓ أَرْسَلَ رَسُولَهُۥ بِٱلْهُدَىٰ وَدِينِ ٱلْحَقِّ لِيُظْهِرَهُۥ عَلَى ٱلدِّينِ كُلِّهِۦ وَلَوْ كَرِهَ ٱلْمُشْرِكُونَ";
  dbp:text "It is He who sent His Messenger with guidance and the religion of truth to manifest it over all religion, although those who associate others with Allah dislike it.";
  dbo:Surah "As-Saff";
  dbo:ayah "5172"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbp:text "O you who have believed, shall I guide you to a transaction that will save you from a painful punishment?";
  dbo:Surah "As-Saff";
  dbo:ayah "5173"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "يَٰٓأَيُّهَا ٱلَّذِينَ ءَامَنُوا۟ لِمَ تَقُولُونَ مَا لَا تَفْعَلُونَ";
  dbp:text "[It is that] you believe in Allah and His Messenger and strive in the cause of Allah with your wealth and your lives. That is best for you, if you should know.";
  dbo:Surah "As-Saff";
  dbo:ayah "5174"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "تُؤْمِنُونَ بِٱللَّهِ وَرَسُولِهِۦ وَتُجَٰهِدُونَ فِى سَبِيلِ ٱللَّهِ بِأَمْوَٰلِكُمْ وَأَنفُسِكُمْ ۚ ذَٰلِكُمْ خَيْرٌ لَّكُمْ إِن كُنتُمْ تَعْلَمُونَ";
  dbp:text "He will forgive for you your sins and admit you to gardens beneath which rivers flow and pleasant dwellings in gardens of perpetual residence. That is the great attainment.";
  dbo:Surah "As-Saff";
  dbo:ayah "5175"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "يَغْفِرْ لَكُمْ ذُنُوبَكُمْ وَيُدْخِلْكُمْ جَنَّٰتٍ تَجْرِى مِن تَحْتِهَا ٱلْأَنْهَٰرُ وَمَسَٰكِنَ طَيِّبَةً فِى جَنَّٰتِ عَدْنٍ ۚ ذَٰلِكَ ٱلْفَوْزُ ٱلْعَظِيمُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5176"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "As-Saff";
  dbp:text "And [you will obtain] another [favor] that you love - victory from Allah and an imminent conquest; and give good tidings to the believers.";
  <http://localhost:3333/has_arabic_text> "وَأُخْرَىٰ تُحِبُّونَهَا ۖ نَصْرٌ مِّنَ ٱللَّهِ وَفَتْحٌ قَرِيبٌ ۗ وَبَشِّرِ ٱلْمُؤْمِنِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5177"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "As-Saff";
  dbp:text "O you who have believed, be supporters of Allah, as when Jesus, the son of Mary, said to the disciples, \"Who are my supporters for Allah?\" The disciples said, \"We are supporters of Allah.\" And a faction of the Children of Israel believed and a faction disbelieved. So We supported those who believed against their enemy, and they became dominant.";
  <http://localhost:3333/has_arabic_text> "يَٰٓأَيُّهَا ٱلَّذِينَ ءَامَنُوا۟ كُونُوٓا۟ أَنصَارَ ٱللَّهِ كَمَا قَالَ عِيسَى ٱبْنُ مَرْيَمَ لِلْحَوَارِيِّۦنَ مَنْ أَنصَارِىٓ إِلَى ٱللَّهِ ۖ قَالَ ٱلْحَوَارِيُّونَ نَحْنُ أَنصَارُ ٱللَّهِ ۖ فَـَٔامَنَت طَّآئِفَةٌ مِّنۢ بَنِىٓ إِسْرَٰٓءِيلَ وَكَفَرَت طَّآئِفَةٌ ۖ فَأَيَّدْنَا ٱلَّذِينَ ءَامَنُوا۟ عَلَىٰ عَدُوِّهِمْ فَأَصْبَحُوا۟ ظَٰهِرِينَ",
    "يَٰٓأَيُّهَا ٱلَّذِينَ ءَامَنُوا۟ هَلْ أَدُلُّكُمْ عَلَىٰ تِجَٰرَةٍ تُنجِيكُم مِّنْ عَذَابٍ أَلِيمٍ",
    "يُرِيدُونَ لِيُطْفِـُٔوا۟ نُورَ ٱللَّهِ بِأَفْوَٰهِهِمْ وَٱللَّهُ مُتِمُّ نُورِهِۦ وَلَوْ كَرِهَ ٱلْكَٰفِرُونَ" .

<http://localhost:3333/Al-Jumu'ah> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Jumu'ah";
  dbo:ayah "5178"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5179"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Jumu'ah";
  dbp:text "It is He who has sent among the unlettered a Messenger from themselves reciting to them His verses and purifying them and teaching them the Book and wisdom - although they were before in clear error -",
    "Whatever is in the heavens and whatever is on the earth is exalting Allah, the Sovereign, the Pure, the Exalted in Might, the Wise.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5180"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Jumu'ah";
  dbp:text "And [to] others of them who have not yet joined them. And He is the Exalted in Might, the Wise.";
  <http://localhost:3333/has_arabic_text> "هُوَ ٱلَّذِى بَعَثَ فِى ٱلْأُمِّيِّۦنَ رَسُولًا مِّنْهُمْ يَتْلُوا۟ عَلَيْهِمْ ءَايَٰتِهِۦ وَيُزَكِّيهِمْ وَيُعَلِّمُهُمُ ٱلْكِتَٰبَ وَٱلْحِكْمَةَ وَإِن كَانُوا۟ مِن قَبْلُ لَفِى ضَلَٰلٍ مُّبِينٍ";
  dbo:Surah "Al-Jumu'ah";
  dbp:text "That is the bounty of Allah, which He gives to whom He wills, and Allah is the possessor of great bounty.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5181"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "ذَٰلِكَ فَضْلُ ٱللَّهِ يُؤْتِيهِ مَن يَشَآءُ ۚ وَٱللَّهُ ذُو ٱلْفَضْلِ ٱلْعَظِيمِ";
  dbp:text "The example of those who were entrusted with the Torah and then did not take it on is like that of a donkey who carries volumes [of books]. Wretched is the example of the people who deny the signs of Allah. And Allah does not guide the wrongdoing people.";
  dbo:Surah "Al-Jumu'ah";
  a dbr:Ayah, dbr:Quranic_verse;
  dbo:ayah "5182"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "مَثَلُ ٱلَّذِينَ حُمِّلُوا۟ ٱلتَّوْرَىٰةَ ثُمَّ لَمْ يَحْمِلُوهَا كَمَثَلِ ٱلْحِمَارِ يَحْمِلُ أَسْفَارًۢا ۚ بِئْسَ مَثَلُ ٱلْقَوْمِ ٱلَّذِينَ كَذَّبُوا۟ بِـَٔايَٰتِ ٱللَّهِ ۚ وَٱللَّهُ لَا يَهْدِى ٱلْقَوْمَ ٱلظَّٰلِمِينَ";
  dbp:text "Say, \"O you who are Jews, if you claim that you are allies of Allah, excluding the [other] people, then wish for death, if you should be truthful.\"";
  dbo:Surah "Al-Jumu'ah";
  dbo:ayah "5183"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "قُلْ يَٰٓأَيُّهَا ٱلَّذِينَ هَادُوٓا۟ إِن زَعَمْتُمْ أَنَّكُمْ أَوْلِيَآءُ لِلَّهِ مِن دُونِ ٱلنَّاسِ فَتَمَنَّوُا۟ ٱلْمَوْتَ إِن كُنتُمْ صَٰدِقِينَ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5184"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Jumu'ah";
  dbp:text "But they will not wish for it, ever, because of what their hands have put forth. And Allah is Knowing of the wrongdoers.";
  <http://localhost:3333/has_arabic_text> "وَءَاخَرِينَ مِنْهُمْ لَمَّا يَلْحَقُوا۟ بِهِمْ ۚ وَهُوَ ٱلْعَزِيزُ ٱلْحَكِيمُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5185"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Jumu'ah";
  dbp:text "Say, \"Indeed, the death from which you flee - indeed, it will meet you. Then you will be returned to the Knower of the unseen and the witnessed, and He will inform you about what you used to do.\"";
  <http://localhost:3333/has_arabic_text> "قُلْ إِنَّ ٱلْمَوْتَ ٱلَّذِى تَفِرُّونَ مِنْهُ فَإِنَّهُۥ مُلَٰقِيكُمْ ۖ ثُمَّ تُرَدُّونَ إِلَىٰ عَٰلِمِ ٱلْغَيْبِ وَٱلشَّهَٰدَةِ فَيُنَبِّئُكُم بِمَا كُنتُمْ تَعْمَلُونَ";
  a dbr:Ayat;
  dbo:ayah "5186"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "O you who have believed, when [the adhan] is called for the prayer on the day of Jumu'ah [Friday], then proceed to the remembrance of Allah and leave trade. That is better for you, if you only knew.";
  dbo:Surah "Al-Jumu'ah";
  a dbr:Ayah, dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "وَلَا يَتَمَنَّوْنَهُۥٓ أَبَدًۢا بِمَا قَدَّمَتْ أَيْدِيهِمْ ۚ وَٱللَّهُ عَلِيمٌۢ بِٱلظَّٰلِمِينَ";
  dbp:text "And when the prayer has been concluded, disperse within the land and seek from the bounty of Allah, and remember Allah often that you may succeed.";
  dbo:ayah "5187"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse;
  dbo:Surah "Al-Jumu'ah";
  a dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "فَإِذَا قُضِيَتِ ٱلصَّلَوٰةُ فَٱنتَشِرُوا۟ فِى ٱلْأَرْضِ وَٱبْتَغُوا۟ مِن فَضْلِ ٱللَّهِ وَٱذْكُرُوا۟ ٱللَّهَ كَثِيرًا لَّعَلَّكُمْ تُفْلِحُونَ";
  dbo:Surah "Al-Jumu'ah";
  dbo:ayah "5188"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbp:text "But when they saw a transaction or a diversion, [O Muhammad], they rushed to it and left you standing. Say, \"What is with Allah is better than diversion and than a transaction, and Allah is the best of providers.\"";
  <http://localhost:3333/has_arabic_text> "وَإِذَا رَأَوْا۟ تِجَٰرَةً أَوْ لَهْوًا ٱنفَضُّوٓا۟ إِلَيْهَا وَتَرَكُوكَ قَآئِمًا ۚ قُلْ مَا عِندَ ٱللَّهِ خَيْرٌ مِّنَ ٱللَّهْوِ وَمِنَ ٱلتِّجَٰرَةِ ۚ وَٱللَّهُ خَيْرُ ٱلرَّٰزِقِينَ",
    "يَٰٓأَيُّهَا ٱلَّذِينَ ءَامَنُوٓا۟ إِذَا نُودِىَ لِلصَّلَوٰةِ مِن يَوْمِ ٱلْجُمُعَةِ فَٱسْعَوْا۟ إِلَىٰ ذِكْرِ ٱللَّهِ وَذَرُوا۟ ٱلْبَيْعَ ۚ ذَٰلِكُمْ خَيْرٌ لَّكُمْ إِن كُنتُمْ تَعْلَمُونَ",
    "يُسَبِّحُ لِلَّهِ مَا فِى ٱلسَّمَٰوَٰتِ وَمَا فِى ٱلْأَرْضِ ٱلْمَلِكِ ٱلْقُدُّوسِ ٱلْعَزِيزِ ٱلْحَكِيمِ" .

<http://localhost:3333/Al-Munafiqun> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Munafiqun";
  dbo:ayah "5189"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5190"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Munafiqun";
  dbp:text "They have taken their oaths as a cover, so they averted [people] from the way of Allah. Indeed, it was evil that they were doing.",
    "When the hypocrites come to you, [O Muhammad], they say, \"We testify that you are the Messenger of Allah.\" And Allah knows that you are His Messenger, and Allah testifies that the hypocrites are liars.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5191"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Munafiqun";
  dbp:text "That is because they believed, and then they disbelieved; so their hearts were sealed over, and they do not understand.";
  <http://localhost:3333/has_arabic_text> "إِذَا جَآءَكَ ٱلْمُنَٰفِقُونَ قَالُوا۟ نَشْهَدُ إِنَّكَ لَرَسُولُ ٱللَّهِ ۗ وَٱللَّهُ يَعْلَمُ إِنَّكَ لَرَسُولُهُۥ وَٱللَّهُ يَشْهَدُ إِنَّ ٱلْمُنَٰفِقِينَ لَكَٰذِبُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5192"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Munafiqun";
  dbp:text "And when you see them, their forms please you, and if they speak, you listen to their speech. [They are] as if they were pieces of wood propped up - they think that every shout is against them. They are the enemy, so beware of them. May Allah destroy them; how are they deluded?";
  <http://localhost:3333/has_arabic_text> "ذَٰلِكَ بِأَنَّهُمْ ءَامَنُوا۟ ثُمَّ كَفَرُوا۟ فَطُبِعَ عَلَىٰ قُلُوبِهِمْ فَهُمْ لَا يَفْقَهُونَ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5193"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Munafiqun";
  dbp:text "And when it is said to them, \"Come, the Messenger of Allah will ask forgiveness for you,\" they turn their heads aside and you see them evading while they are arrogant.";
  <http://localhost:3333/has_arabic_text> "وَإِذَا رَأَيْتَهُمْ تُعْجِبُكَ أَجْسَامُهُمْ ۖ وَإِن يَقُولُوا۟ تَسْمَعْ لِقَوْلِهِمْ ۖ كَأَنَّهُمْ خُشُبٌ مُّسَنَّدَةٌ ۖ يَحْسَبُونَ كُلَّ صَيْحَةٍ عَلَيْهِمْ ۚ هُمُ ٱلْعَدُوُّ فَٱحْذَرْهُمْ ۚ قَٰتَلَهُمُ ٱللَّهُ ۖ أَنَّىٰ يُؤْفَكُونَ";
  dbo:ayah "5194"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:Surah "Al-Munafiqun";
  dbp:text "It is all the same for them whether you ask forgiveness for them or do not ask forgiveness for them; never will Allah forgive them. Indeed, Allah does not guide the defiantly disobedient people.";
  <http://localhost:3333/has_arabic_text> "سَوَآءٌ عَلَيْهِمْ أَسْتَغْفَرْتَ لَهُمْ أَمْ لَمْ تَسْتَغْفِرْ لَهُمْ لَن يَغْفِرَ ٱللَّهُ لَهُمْ ۚ إِنَّ ٱللَّهَ لَا يَهْدِى ٱلْقَوْمَ ٱلْفَٰسِقِينَ";
  dbp:text "They are the ones who say, \"Do not spend on those who are with the Messenger of Allah until they disband.\" And to Allah belongs the depositories of the heavens and the earth, but the hypocrites do not understand.";
  a dbr:Quranic_verse, dbr:Ayat;
  dbo:Surah "Al-Munafiqun";
  dbo:ayah "5195"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "هُمُ ٱلَّذِينَ يَقُولُونَ لَا تُنفِقُوا۟ عَلَىٰ مَنْ عِندَ رَسُولِ ٱللَّهِ حَتَّىٰ يَنفَضُّوا۟ ۗ وَلِلَّهِ خَزَآئِنُ ٱلسَّمَٰوَٰتِ وَٱلْأَرْضِ وَلَٰكِنَّ ٱلْمُنَٰفِقِينَ لَا يَفْقَهُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5196"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Munafiqun";
  dbp:text "They say, \"If we return to al-Madinah, the more honored [for power] will surely expel therefrom the more humble.\" And to Allah belongs [all] honor, and to His Messenger, and to the believers, but the hypocrites do not know.";
  <http://localhost:3333/has_arabic_text> "وَإِذَا قِيلَ لَهُمْ تَعَالَوْا۟ يَسْتَغْفِرْ لَكُمْ رَسُولُ ٱللَّهِ لَوَّوْا۟ رُءُوسَهُمْ وَرَأَيْتَهُمْ يَصُدُّونَ وَهُم مُّسْتَكْبِرُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5197"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Munafiqun";
  dbp:text "O you who have believed, let not your wealth and your children divert you from remembrance of Allah. And whoever does that - then those are the losers.";
  <http://localhost:3333/has_arabic_text> "يَقُولُونَ لَئِن رَّجَعْنَآ إِلَى ٱلْمَدِينَةِ لَيُخْرِجَنَّ ٱلْأَعَزُّ مِنْهَا ٱلْأَذَلَّ ۚ وَلِلَّهِ ٱلْعِزَّةُ وَلِرَسُولِهِۦ وَلِلْمُؤْمِنِينَ وَلَٰكِنَّ ٱلْمُنَٰفِقِينَ لَا يَعْلَمُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5198"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Munafiqun";
  dbp:text "And spend [in the way of Allah] from what We have provided you before death approaches one of you and he says, \"My Lord, if only You would delay me for a brief term so I would give charity and be among the righteous.\"";
  <http://localhost:3333/has_arabic_text> "وَأَنفِقُوا۟ مِن مَّا رَزَقْنَٰكُم مِّن قَبْلِ أَن يَأْتِىَ أَحَدَكُمُ ٱلْمَوْتُ فَيَقُولَ رَبِّ لَوْلَآ أَخَّرْتَنِىٓ إِلَىٰٓ أَجَلٍ قَرِيبٍ فَأَصَّدَّقَ وَأَكُن مِّنَ ٱلصَّٰلِحِينَ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:Surah "Al-Munafiqun";
  dbo:ayah "5199"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "But never will Allah delay a soul when its time has come. And Allah is Acquainted with what you do.";
  <http://localhost:3333/has_arabic_text> "وَلَن يُؤَخِّرَ ٱللَّهُ نَفْسًا إِذَا جَآءَ أَجَلُهَا ۚ وَٱللَّهُ خَبِيرٌۢ بِمَا تَعْمَلُونَ",
    "يَٰٓأَيُّهَا ٱلَّذِينَ ءَامَنُوا۟ لَا تُلْهِكُمْ أَمْوَٰلُكُمْ وَلَآ أَوْلَٰدُكُمْ عَن ذِكْرِ ٱللَّهِ ۚ وَمَن يَفْعَلْ ذَٰلِكَ فَأُو۟لَٰٓئِكَ هُمُ ٱلْخَٰسِرُونَ",
    "ٱتَّخَذُوٓا۟ أَيْمَٰنَهُمْ جُنَّةً فَصَدُّوا۟ عَن سَبِيلِ ٱللَّهِ ۚ إِنَّهُمْ سَآءَ مَا كَانُوا۟ يَعْمَلُونَ" .

<http://localhost:3333/At-Taghabun> a dbr:Ayat, dbr:Ayah, dbr:Quranic_verse;
  dbo:Surah "At-Taghabun";
  dbo:ayah "5200"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5201"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Taghabun";
  dbp:text "It is He who created you, and among you is the disbeliever, and among you is the believer. And Allah, of what you do, is Seeing.",
    "Whatever is in the heavens and whatever is on the earth is exalting Allah. To Him belongs dominion, and to Him belongs [all] praise, and He is over all things competent.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5202"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Taghabun";
  dbp:text "He created the heavens and earth in truth and formed you and perfected your forms; and to Him is the [final] destination.";
  <http://localhost:3333/has_arabic_text> "خَلَقَ ٱلسَّمَٰوَٰتِ وَٱلْأَرْضَ بِٱلْحَقِّ وَصَوَّرَكُمْ فَأَحْسَنَ صُوَرَكُمْ ۖ وَإِلَيْهِ ٱلْمَصِيرُ";
  a dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "At-Taghabun";
  a dbr:Ayah;
  dbo:ayah "5203"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "He knows what is within the heavens and earth and knows what you conceal and what you declare. And Allah is Knowing of that within the breasts.";
  <http://localhost:3333/has_arabic_text> "هُوَ ٱلَّذِى خَلَقَكُمْ فَمِنكُمْ كَافِرٌ وَمِنكُم مُّؤْمِنٌ ۚ وَٱللَّهُ بِمَا تَعْمَلُونَ بَصِيرٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5204"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Taghabun";
  dbp:text "Has there not come to you the news of those who disbelieved before? So they tasted the bad consequence of their affair, and they will have a painful punishment.";
  <http://localhost:3333/has_arabic_text> "أَلَمْ يَأْتِكُمْ نَبَؤُا۟ ٱلَّذِينَ كَفَرُوا۟ مِن قَبْلُ فَذَاقُوا۟ وَبَالَ أَمْرِهِمْ وَلَهُمْ عَذَابٌ أَلِيمٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbp:text "That is because their messengers used to come to them with clear evidences, but they said, \"Shall human beings guide us?\" and disbelieved and turned away. And Allah dispensed [with them]; and Allah is Free of need and Praiseworthy.";
  dbo:ayah "5205"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Taghabun";
  <http://localhost:3333/has_arabic_text> "ذَٰلِكَ بِأَنَّهُۥ كَانَت تَّأْتِيهِمْ رُسُلُهُم بِٱلْبَيِّنَٰتِ فَقَالُوٓا۟ أَبَشَرٌ يَهْدُونَنَا فَكَفَرُوا۟ وَتَوَلَّوا۟ ۚ وَّٱسْتَغْنَى ٱللَّهُ ۚ وَٱللَّهُ غَنِىٌّ حَمِيدٌ";
  dbp:text "Those who disbelieve have claimed that they will never be resurrected. Say, \"Yes, by my Lord, you will surely be resurrected; then you will surely be informed of what you did. And that, for Allah, is easy.\"";
  a dbr:Ayah, dbr:Quranic_verse;
  dbo:Surah "At-Taghabun";
  a dbr:Ayat;
  dbo:ayah "5206"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "زَعَمَ ٱلَّذِينَ كَفَرُوٓا۟ أَن لَّن يُبْعَثُوا۟ ۚ قُلْ بَلَىٰ وَرَبِّى لَتُبْعَثُنَّ ثُمَّ لَتُنَبَّؤُنَّ بِمَا عَمِلْتُمْ ۚ وَذَٰلِكَ عَلَى ٱللَّهِ يَسِيرٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5207"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Taghabun";
  dbp:text "So believe in Allah and His Messenger and the Qur'an which We have sent down. And Allah is Acquainted with what you do.";
  <http://localhost:3333/has_arabic_text> "فَـَٔامِنُوا۟ بِٱللَّهِ وَرَسُولِهِۦ وَٱلنُّورِ ٱلَّذِىٓ أَنزَلْنَا ۚ وَٱللَّهُ بِمَا تَعْمَلُونَ خَبِيرٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5208"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Taghabun";
  dbp:text "The Day He will assemble you for the Day of Assembly - that is the Day of Deprivation. And whoever believes in Allah and does righteousness - He will remove from him his misdeeds and admit him to gardens beneath which rivers flow, wherein they will abide forever. That is the great attainment.";
  <http://localhost:3333/has_arabic_text> "يَعْلَمُ مَا فِى ٱلسَّمَٰوَٰتِ وَٱلْأَرْضِ وَيَعْلَمُ مَا تُسِرُّونَ وَمَا تُعْلِنُونَ ۚ وَٱللَّهُ عَلِيمٌۢ بِذَاتِ ٱلصُّدُورِ";
  dbo:Surah "At-Taghabun";
  a dbr:Ayah;
  dbo:ayah "5209"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "But the ones who disbelieved and denied Our verses - those are the companions of the Fire, abiding eternally therein; and wretched is the destination.";
  a dbr:Quranic_verse, dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "وَٱلَّذِينَ كَفَرُوا۟ وَكَذَّبُوا۟ بِـَٔايَٰتِنَآ أُو۟لَٰٓئِكَ أَصْحَٰبُ ٱلنَّارِ خَٰلِدِينَ فِيهَا ۖ وَبِئْسَ ٱلْمَصِيرُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5210"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Taghabun";
  dbp:text "No disaster strikes except by permission of Allah. And whoever believes in Allah - He will guide his heart. And Allah is Knowing of all things.";
  <http://localhost:3333/has_arabic_text> "مَآ أَصَابَ مِن مُّصِيبَةٍ إِلَّا بِإِذْنِ ٱللَّهِ ۗ وَمَن يُؤْمِنۢ بِٱللَّهِ يَهْدِ قَلْبَهُۥ ۚ وَٱللَّهُ بِكُلِّ شَىْءٍ عَلِيمٌ";
  a dbr:Ayat;
  dbo:Surah "At-Taghabun";
  dbp:text "And obey Allah and obey the Messenger; but if you turn away - then upon Our Messenger is only [the duty of] clear notification.";
  dbo:ayah "5211"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "وَأَطِيعُوا۟ ٱللَّهَ وَأَطِيعُوا۟ ٱلرَّسُولَ ۚ فَإِن تَوَلَّيْتُمْ فَإِنَّمَا عَلَىٰ رَسُولِنَا ٱلْبَلَٰغُ ٱلْمُبِينُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5212"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Taghabun";
  dbp:text "Allah - there is no deity except Him. And upon Allah let the believers rely.";
  <http://localhost:3333/has_arabic_text> "يَوْمَ يَجْمَعُكُمْ لِيَوْمِ ٱلْجَمْعِ ۖ ذَٰلِكَ يَوْمُ ٱلتَّغَابُنِ ۗ وَمَن يُؤْمِنۢ بِٱللَّهِ وَيَعْمَلْ صَٰلِحًا يُكَفِّرْ عَنْهُ سَيِّـَٔاتِهِۦ وَيُدْخِلْهُ جَنَّٰتٍ تَجْرِى مِن تَحْتِهَا ٱلْأَنْهَٰرُ خَٰلِدِينَ فِيهَآ أَبَدًا ۚ ذَٰلِكَ ٱلْفَوْزُ ٱلْعَظِيمُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5213"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Taghabun";
  dbp:text "O you who have believed, indeed, among your wives and your children are enemies to you, so beware of them. But if you pardon and overlook and forgive - then indeed, Allah is Forgiving and Merciful.";
  <http://localhost:3333/has_arabic_text> "يَٰٓأَيُّهَا ٱلَّذِينَ ءَامَنُوٓا۟ إِنَّ مِنْ أَزْوَٰجِكُمْ وَأَوْلَٰدِكُمْ عَدُوًّا لَّكُمْ فَٱحْذَرُوهُمْ ۚ وَإِن تَعْفُوا۟ وَتَصْفَحُوا۟ وَتَغْفِرُوا۟ فَإِنَّ ٱللَّهَ غَفُورٌ رَّحِيمٌ";
  dbo:ayah "5214"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Ayah;
  dbp:text "Your wealth and your children are but a trial, and Allah has with Him a great reward.";
  dbo:Surah "At-Taghabun";
  a dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "إِنَّمَآ أَمْوَٰلُكُمْ وَأَوْلَٰدُكُمْ فِتْنَةٌ ۚ وَٱللَّهُ عِندَهُۥٓ أَجْرٌ عَظِيمٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5215"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Taghabun";
  dbp:text "So fear Allah as much as you are able and listen and obey and spend [in the way of Allah]; it is better for your selves. And whoever is protected from the stinginess of his soul - it is those who will be the successful.";
  <http://localhost:3333/has_arabic_text> "فَٱتَّقُوا۟ ٱللَّهَ مَا ٱسْتَطَعْتُمْ وَٱسْمَعُوا۟ وَأَطِيعُوا۟ وَأَنفِقُوا۟ خَيْرًا لِّأَنفُسِكُمْ ۗ وَمَن يُوقَ شُحَّ نَفْسِهِۦ فَأُو۟لَٰٓئِكَ هُمُ ٱلْمُفْلِحُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5216"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Taghabun";
  dbp:text "If you loan Allah a goodly loan, He will multiply it for you and forgive you. And Allah is Most Appreciative and Forbearing.";
  <http://localhost:3333/has_arabic_text> "إِن تُقْرِضُوا۟ ٱللَّهَ قَرْضًا حَسَنًا يُضَٰعِفْهُ لَكُمْ وَيَغْفِرْ لَكُمْ ۚ وَٱللَّهُ شَكُورٌ حَلِيمٌ";
  a dbr:Ayat, dbr:Ayah;
  dbo:Surah "At-Taghabun";
  dbo:ayah "5217"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse;
  dbp:text "Knower of the unseen and the witnessed, the Exalted in Might, the Wise.";
  <http://localhost:3333/has_arabic_text> "عَٰلِمُ ٱلْغَيْبِ وَٱلشَّهَٰدَةِ ٱلْعَزِيزُ ٱلْحَكِيمُ",
    "يُسَبِّحُ لِلَّهِ مَا فِى ٱلسَّمَٰوَٰتِ وَمَا فِى ٱلْأَرْضِ ۖ لَهُ ٱلْمُلْكُ وَلَهُ ٱلْحَمْدُ ۖ وَهُوَ عَلَىٰ كُلِّ شَىْءٍ قَدِيرٌ",
    "ٱللَّهُ لَآ إِلَٰهَ إِلَّا هُوَ ۚ وَعَلَى ٱللَّهِ فَلْيَتَوَكَّلِ ٱلْمُؤْمِنُونَ" .

<http://localhost:3333/At-Talaq> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "At-Talaq";
  dbo:ayah "5218"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbp:text "And when they have [nearly] fulfilled their term, either retain them according to acceptable terms or part with them according to acceptable terms. And bring to witness two just men from among you and establish the testimony for [the acceptance of] Allah. That is instructed to whoever should believe in Allah and the Last day. And whoever fears Allah - He will make for him a way out";
  dbo:ayah "5219"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Talaq";
  dbp:text "O Prophet, when you [Muslims] divorce women, divorce them for [the commencement of] their waiting period and keep count of the waiting period, and fear Allah, your Lord. Do not turn them out of their [husbands'] houses, nor should they [themselves] leave [during that period] unless they are committing a clear immorality. And those are the limits [set by] Allah. And whoever transgresses the limits of Allah has certainly wronged himself. You know not; perhaps Allah will bring about after that a [different] matter.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5220"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Talaq";
  dbp:text "And will provide for him from where he does not expect. And whoever relies upon Allah - then He is sufficient for him. Indeed, Allah will accomplish His purpose. Allah has already set for everything a [decreed] extent.";
  <http://localhost:3333/has_arabic_text> "فَإِذَا بَلَغْنَ أَجَلَهُنَّ فَأَمْسِكُوهُنَّ بِمَعْرُوفٍ أَوْ فَارِقُوهُنَّ بِمَعْرُوفٍ وَأَشْهِدُوا۟ ذَوَىْ عَدْلٍ مِّنكُمْ وَأَقِيمُوا۟ ٱلشَّهَٰدَةَ لِلَّهِ ۚ ذَٰلِكُمْ يُوعَظُ بِهِۦ مَن كَانَ يُؤْمِنُ بِٱللَّهِ وَٱلْيَوْمِ ٱلْءَاخِرِ ۚ وَمَن يَتَّقِ ٱللَّهَ يَجْعَل لَّهُۥ مَخْرَجًا",
    "وَيَرْزُقْهُ مِنْ حَيْثُ لَا يَحْتَسِبُ ۚ وَمَن يَتَوَكَّلْ عَلَى ٱللَّهِ فَهُوَ حَسْبُهُۥٓ ۚ إِنَّ ٱللَّهَ بَٰلِغُ أَمْرِهِۦ ۚ قَدْ جَعَلَ ٱللَّهُ لِكُلِّ شَىْءٍ قَدْرًا";
  a dbr:Ayah;
  dbo:ayah "5221"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse;
  dbp:text "And those who no longer expect menstruation among your women - if you doubt, then their period is three months, and [also for] those who have not menstruated. And for those who are pregnant, their term is until they give birth. And whoever fears Allah - He will make for him of his matter ease.";
  dbo:Surah "At-Talaq";
  a dbr:Ayat, dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5222"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Talaq";
  dbp:text "That is the command of Allah, which He has sent down to you; and whoever fears Allah - He will remove for him his misdeeds and make great for him his reward.";
  <http://localhost:3333/has_arabic_text> "ذَٰلِكَ أَمْرُ ٱللَّهِ أَنزَلَهُۥٓ إِلَيْكُمْ ۚ وَمَن يَتَّقِ ٱللَّهَ يُكَفِّرْ عَنْهُ سَيِّـَٔاتِهِۦ وَيُعْظِمْ لَهُۥٓ أَجْرًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5223"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Talaq";
  dbp:text "Lodge them [in a section] of where you dwell out of your means and do not harm them in order to oppress them. And if they should be pregnant, then spend on them until they give birth. And if they breastfeed for you, then give them their payment and confer among yourselves in the acceptable way; but if you are in discord, then there may breastfeed for the father another woman.";
  <http://localhost:3333/has_arabic_text> "أَسْكِنُوهُنَّ مِنْ حَيْثُ سَكَنتُم مِّن وُجْدِكُمْ وَلَا تُضَآرُّوهُنَّ لِتُضَيِّقُوا۟ عَلَيْهِنَّ ۚ وَإِن كُنَّ أُو۟لَٰتِ حَمْلٍ فَأَنفِقُوا۟ عَلَيْهِنَّ حَتَّىٰ يَضَعْنَ حَمْلَهُنَّ ۚ فَإِنْ أَرْضَعْنَ لَكُمْ فَـَٔاتُوهُنَّ أُجُورَهُنَّ ۖ وَأْتَمِرُوا۟ بَيْنَكُم بِمَعْرُوفٍ ۖ وَإِن تَعَاسَرْتُمْ فَسَتُرْضِعُ لَهُۥٓ أُخْرَىٰ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5224"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Talaq";
  dbp:text "Let a man of wealth spend from his wealth, and he whose provision is restricted - let him spend from what Allah has given him. Allah does not charge a soul except [according to] what He has given it. Allah will bring about, after hardship, ease.";
  <http://localhost:3333/has_arabic_text> "لِيُنفِقْ ذُو سَعَةٍ مِّن سَعَتِهِۦ ۖ وَمَن قُدِرَ عَلَيْهِ رِزْقُهُۥ فَلْيُنفِقْ مِمَّآ ءَاتَىٰهُ ٱللَّهُ ۚ لَا يُكَلِّفُ ٱللَّهُ نَفْسًا إِلَّا مَآ ءَاتَىٰهَا ۚ سَيَجْعَلُ ٱللَّهُ بَعْدَ عُسْرٍ يُسْرًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5225"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Talaq";
  dbp:text "And how many a city was insolent toward the command of its Lord and His messengers, so We took it to severe account and punished it with a terrible punishment.";
  <http://localhost:3333/has_arabic_text> "وَكَأَيِّن مِّن قَرْيَةٍ عَتَتْ عَنْ أَمْرِ رَبِّهَا وَرُسُلِهِۦ فَحَاسَبْنَٰهَا حِسَابًا شَدِيدًا وَعَذَّبْنَٰهَا عَذَابًا نُّكْرًا",
    "فَذَاقَتْ وَبَالَ أَمْرِهَا وَكَانَ عَٰقِبَةُ أَمْرِهَا خُسْرًا";
  dbp:text "And it tasted the bad consequence of its affair, and the outcome of its affair was loss.";
  dbo:Surah "At-Talaq";
  a dbr:Ayah;
  dbo:ayah "5226"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat;
  dbo:Surah "At-Talaq";
  dbo:ayah "5227"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbp:text "Allah has prepared for them a severe punishment; so fear Allah, O you of understanding who have believed. Allah has sent down to you the Qur'an.";
  <http://localhost:3333/has_arabic_text> "أَعَدَّ ٱللَّهُ لَهُمْ عَذَابًا شَدِيدًا ۖ فَٱتَّقُوا۟ ٱللَّهَ يَٰٓأُو۟لِى ٱلْأَلْبَٰبِ ٱلَّذِينَ ءَامَنُوا۟ ۚ قَدْ أَنزَلَ ٱللَّهُ إِلَيْكُمْ ذِكْرًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5228"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Talaq";
  dbp:text "[He sent] a Messenger [Muhammad] reciting to you the distinct verses of Allah that He may bring out those who believe and do righteous deeds from darknesses into the light. And whoever believes in Allah and does righteousness - He will admit him into gardens beneath which rivers flow to abide therein forever. Allah will have perfected for him a provision.";
  <http://localhost:3333/has_arabic_text> "رَّسُولًا يَتْلُوا۟ عَلَيْكُمْ ءَايَٰتِ ٱللَّهِ مُبَيِّنَٰتٍ لِّيُخْرِجَ ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّٰلِحَٰتِ مِنَ ٱلظُّلُمَٰتِ إِلَى ٱلنُّورِ ۚ وَمَن يُؤْمِنۢ بِٱللَّهِ وَيَعْمَلْ صَٰلِحًا يُدْخِلْهُ جَنَّٰتٍ تَجْرِى مِن تَحْتِهَا ٱلْأَنْهَٰرُ خَٰلِدِينَ فِيهَآ أَبَدًا ۖ قَدْ أَحْسَنَ ٱللَّهُ لَهُۥ رِزْقًا",
    "وَٱلَّٰٓـِٔى يَئِسْنَ مِنَ ٱلْمَحِيضِ مِن نِّسَآئِكُمْ إِنِ ٱرْتَبْتُمْ فَعِدَّتُهُنَّ ثَلَٰثَةُ أَشْهُرٍ وَٱلَّٰٓـِٔى لَمْ يَحِضْنَ ۚ وَأُو۟لَٰتُ ٱلْأَحْمَالِ أَجَلُهُنَّ أَن يَضَعْنَ حَمْلَهُنَّ ۚ وَمَن يَتَّقِ ٱللَّهَ يَجْعَل لَّهُۥ مِنْ أَمْرِهِۦ يُسْرًا";
  a dbr:Ayah, dbr:Quranic_verse;
  dbo:Surah "At-Talaq";
  a dbr:Ayat;
  dbp:text "It is Allah who has created seven heavens and of the earth, the like of them. [His] command descends among them so you may know that Allah is over all things competent and that Allah has encompassed all things in knowledge.";
  dbo:ayah "5229"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "يَٰٓأَيُّهَا ٱلنَّبِىُّ إِذَا طَلَّقْتُمُ ٱلنِّسَآءَ فَطَلِّقُوهُنَّ لِعِدَّتِهِنَّ وَأَحْصُوا۟ ٱلْعِدَّةَ ۖ وَٱتَّقُوا۟ ٱللَّهَ رَبَّكُمْ ۖ لَا تُخْرِجُوهُنَّ مِنۢ بُيُوتِهِنَّ وَلَا يَخْرُجْنَ إِلَّآ أَن يَأْتِينَ بِفَٰحِشَةٍ مُّبَيِّنَةٍ ۚ وَتِلْكَ حُدُودُ ٱللَّهِ ۚ وَمَن يَتَعَدَّ حُدُودَ ٱللَّهِ فَقَدْ ظَلَمَ نَفْسَهُۥ ۚ لَا تَدْرِى لَعَلَّ ٱللَّهَ يُحْدِثُ بَعْدَ ذَٰلِكَ أَمْرًا",
    "ٱللَّهُ ٱلَّذِى خَلَقَ سَبْعَ سَمَٰوَٰتٍ وَمِنَ ٱلْأَرْضِ مِثْلَهُنَّ يَتَنَزَّلُ ٱلْأَمْرُ بَيْنَهُنَّ لِتَعْلَمُوٓا۟ أَنَّ ٱللَّهَ عَلَىٰ كُلِّ شَىْءٍ قَدِيرٌ وَأَنَّ ٱللَّهَ قَدْ أَحَاطَ بِكُلِّ شَىْءٍ عِلْمًۢا" .

<http://localhost:3333/At-Tahreem> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "At-Tahreem";
  dbo:ayah "5230"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5231"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Tahreem";
  dbp:text "Allah has already ordained for you [Muslims] the dissolution of your oaths. And Allah is your protector, and He is the Knowing, the Wise.",
    "O Prophet, why do you prohibit [yourself from] what Allah has made lawful for you, seeking the approval of your wives? And Allah is Forgiving and Merciful.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5232"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Tahreem";
  dbp:text "And [remember] when the Prophet confided to one of his wives a statement; and when she informed [another] of it and Allah showed it to him, he made known part of it and ignored a part. And when he informed her about it, she said, \"Who told you this?\" He said, \"I was informed by the Knowing, the Acquainted.\"";
  <http://localhost:3333/has_arabic_text> "قَدْ فَرَضَ ٱللَّهُ لَكُمْ تَحِلَّةَ أَيْمَٰنِكُمْ ۚ وَٱللَّهُ مَوْلَىٰكُمْ ۖ وَهُوَ ٱلْعَلِيمُ ٱلْحَكِيمُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5233"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Tahreem";
  dbp:text "If you two [wives] repent to Allah, [it is best], for your hearts have deviated. But if you cooperate against him - then indeed Allah is his protector, and Gabriel and the righteous of the believers and the angels, moreover, are [his] assistants.";
  <http://localhost:3333/has_arabic_text> "إِن تَتُوبَآ إِلَى ٱللَّهِ فَقَدْ صَغَتْ قُلُوبُكُمَا ۖ وَإِن تَظَٰهَرَا عَلَيْهِ فَإِنَّ ٱللَّهَ هُوَ مَوْلَىٰهُ وَجِبْرِيلُ وَصَٰلِحُ ٱلْمُؤْمِنِينَ ۖ وَٱلْمَلَٰٓئِكَةُ بَعْدَ ذَٰلِكَ ظَهِيرٌ";
  a dbr:Ayah;
  dbo:Surah "At-Tahreem";
  dbo:ayah "5234"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "Perhaps his Lord, if he divorced you [all], would substitute for him wives better than you - submitting [to Allah], believing, devoutly obedient, repentant, worshipping, and traveling - [ones] previously married and virgins.";
  a dbr:Ayat, dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "عَسَىٰ رَبُّهُۥٓ إِن طَلَّقَكُنَّ أَن يُبْدِلَهُۥٓ أَزْوَٰجًا خَيْرًا مِّنكُنَّ مُسْلِمَٰتٍ مُّؤْمِنَٰتٍ قَٰنِتَٰتٍ تَٰٓئِبَٰتٍ عَٰبِدَٰتٍ سَٰٓئِحَٰتٍ ثَيِّبَٰتٍ وَأَبْكَارًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5235"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Tahreem";
  dbp:text "O you who have believed, protect yourselves and your families from a Fire whose fuel is people and stones, over which are [appointed] angels, harsh and severe; they do not disobey Allah in what He commands them but do what they are commanded.";
  <http://localhost:3333/has_arabic_text> "وَإِذْ أَسَرَّ ٱلنَّبِىُّ إِلَىٰ بَعْضِ أَزْوَٰجِهِۦ حَدِيثًا فَلَمَّا نَبَّأَتْ بِهِۦ وَأَظْهَرَهُ ٱللَّهُ عَلَيْهِ عَرَّفَ بَعْضَهُۥ وَأَعْرَضَ عَنۢ بَعْضٍ ۖ فَلَمَّا نَبَّأَهَا بِهِۦ قَالَتْ مَنْ أَنۢبَأَكَ هَٰذَا ۖ قَالَ نَبَّأَنِىَ ٱلْعَلِيمُ ٱلْخَبِيرُ",
    "يَٰٓأَيُّهَا ٱلنَّبِىُّ لِمَ تُحَرِّمُ مَآ أَحَلَّ ٱللَّهُ لَكَ ۖ تَبْتَغِى مَرْضَاتَ أَزْوَٰجِكَ ۚ وَٱللَّهُ غَفُورٌ رَّحِيمٌ";
  dbp:text "O you who have disbelieved, make no excuses that Day. You will only be recompensed for what you used to do.";
  a dbr:Ayat;
  dbo:ayah "5236"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Tahreem";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5237"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Tahreem";
  dbp:text "O you who have believed, repent to Allah with sincere repentance. Perhaps your Lord will remove from you your misdeeds and admit you into gardens beneath which rivers flow [on] the Day when Allah will not disgrace the Prophet and those who believed with him. Their light will proceed before them and on their right; they will say, \"Our Lord, perfect for us our light and forgive us. Indeed, You are over all things competent.\"";
  <http://localhost:3333/has_arabic_text> "يَٰٓأَيُّهَا ٱلَّذِينَ ءَامَنُوا۟ تُوبُوٓا۟ إِلَى ٱللَّهِ تَوْبَةً نَّصُوحًا عَسَىٰ رَبُّكُمْ أَن يُكَفِّرَ عَنكُمْ سَيِّـَٔاتِكُمْ وَيُدْخِلَكُمْ جَنَّٰتٍ تَجْرِى مِن تَحْتِهَا ٱلْأَنْهَٰرُ يَوْمَ لَا يُخْزِى ٱللَّهُ ٱلنَّبِىَّ وَٱلَّذِينَ ءَامَنُوا۟ مَعَهُۥ ۖ نُورُهُمْ يَسْعَىٰ بَيْنَ أَيْدِيهِمْ وَبِأَيْمَٰنِهِمْ يَقُولُونَ رَبَّنَآ أَتْمِمْ لَنَا نُورَنَا وَٱغْفِرْ لَنَآ ۖ إِنَّكَ عَلَىٰ كُلِّ شَىْءٍ قَدِيرٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5238"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Tahreem";
  dbp:text "O Prophet, strive against the disbelievers and the hypocrites and be harsh upon them. And their refuge is Hell, and wretched is the destination.";
  <http://localhost:3333/has_arabic_text> "يَٰٓأَيُّهَا ٱلنَّبِىُّ جَٰهِدِ ٱلْكُفَّارَ وَٱلْمُنَٰفِقِينَ وَٱغْلُظْ عَلَيْهِمْ ۚ وَمَأْوَىٰهُمْ جَهَنَّمُ ۖ وَبِئْسَ ٱلْمَصِيرُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5239"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Tahreem";
  dbp:text "Allah presents an example of those who disbelieved: the wife of Noah and the wife of Lot. They were under two of Our righteous servants but betrayed them, so those prophets did not avail them from Allah at all, and it was said, \"Enter the Fire with those who enter.\"";
  <http://localhost:3333/has_arabic_text> "ضَرَبَ ٱللَّهُ مَثَلًا لِّلَّذِينَ كَفَرُوا۟ ٱمْرَأَتَ نُوحٍ وَٱمْرَأَتَ لُوطٍ ۖ كَانَتَا تَحْتَ عَبْدَيْنِ مِنْ عِبَادِنَا صَٰلِحَيْنِ فَخَانَتَاهُمَا فَلَمْ يُغْنِيَا عَنْهُمَا مِنَ ٱللَّهِ شَيْـًٔا وَقِيلَ ٱدْخُلَا ٱلنَّارَ مَعَ ٱلدَّٰخِلِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5240"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Tahreem";
  dbp:text "And Allah presents an example of those who believed: the wife of Pharaoh, when she said, \"My Lord, build for me near You a house in Paradise and save me from Pharaoh and his deeds and save me from the wrongdoing people.\"";
  <http://localhost:3333/has_arabic_text> "وَضَرَبَ ٱللَّهُ مَثَلًا لِّلَّذِينَ ءَامَنُوا۟ ٱمْرَأَتَ فِرْعَوْنَ إِذْ قَالَتْ رَبِّ ٱبْنِ لِى عِندَكَ بَيْتًا فِى ٱلْجَنَّةِ وَنَجِّنِى مِن فِرْعَوْنَ وَعَمَلِهِۦ وَنَجِّنِى مِنَ ٱلْقَوْمِ ٱلظَّٰلِمِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5241"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Tahreem";
  dbp:text "And [the example of] Mary, the daughter of 'Imran, who guarded her chastity, so We blew into [her garment] through Our angel, and she believed in the words of her Lord and His scriptures and was of the devoutly obedient.";
  <http://localhost:3333/has_arabic_text> "وَمَرْيَمَ ٱبْنَتَ عِمْرَٰنَ ٱلَّتِىٓ أَحْصَنَتْ فَرْجَهَا فَنَفَخْنَا فِيهِ مِن رُّوحِنَا وَصَدَّقَتْ بِكَلِمَٰتِ رَبِّهَا وَكُتُبِهِۦ وَكَانَتْ مِنَ ٱلْقَٰنِتِينَ",
    "يَٰٓأَيُّهَا ٱلَّذِينَ ءَامَنُوا۟ قُوٓا۟ أَنفُسَكُمْ وَأَهْلِيكُمْ نَارًا وَقُودُهَا ٱلنَّاسُ وَٱلْحِجَارَةُ عَلَيْهَا مَلَٰٓئِكَةٌ غِلَاظٌ شِدَادٌ لَّا يَعْصُونَ ٱللَّهَ مَآ أَمَرَهُمْ وَيَفْعَلُونَ مَا يُؤْمَرُونَ",
    "يَٰٓأَيُّهَا ٱلَّذِينَ كَفَرُوا۟ لَا تَعْتَذِرُوا۟ ٱلْيَوْمَ ۖ إِنَّمَا تُجْزَوْنَ مَا كُنتُمْ تَعْمَلُونَ" .

<http://localhost:3333/Al-Mulk> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Mulk";
  dbo:ayah "5242"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5243"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mulk";
  dbp:text "Blessed is He in whose hand is dominion, and He is over all things competent -",
    "[He] who created death and life to test you [as to] which of you is best in deed - and He is the Exalted in Might, the Forgiving -";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5244"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mulk";
  dbp:text "[And] who created seven heavens in layers. You do not see in the creation of the Most Merciful any inconsistency. So return [your] vision [to the sky]; do you see any breaks?";
  <http://localhost:3333/has_arabic_text> "تَبَٰرَكَ ٱلَّذِى بِيَدِهِ ٱلْمُلْكُ وَهُوَ عَلَىٰ كُلِّ شَىْءٍ قَدِيرٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5245"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mulk";
  dbp:text "Then return [your] vision twice again. [Your] vision will return to you humbled while it is fatigued.";
  <http://localhost:3333/has_arabic_text> "ثُمَّ ٱرْجِعِ ٱلْبَصَرَ كَرَّتَيْنِ يَنقَلِبْ إِلَيْكَ ٱلْبَصَرُ خَاسِئًا وَهُوَ حَسِيرٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5246"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mulk";
  dbp:text "And We have certainly beautified the nearest heaven with stars and have made [from] them what is thrown at the devils and have prepared for them the punishment of the Blaze.";
  <http://localhost:3333/has_arabic_text> "وَلَقَدْ زَيَّنَّا ٱلسَّمَآءَ ٱلدُّنْيَا بِمَصَٰبِيحَ وَجَعَلْنَٰهَا رُجُومًا لِّلشَّيَٰطِينِ ۖ وَأَعْتَدْنَا لَهُمْ عَذَابَ ٱلسَّعِيرِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5247"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mulk";
  dbp:text "And for those who disbelieved in their Lord is the punishment of Hell, and wretched is the destination.";
  <http://localhost:3333/has_arabic_text> "وَلِلَّذِينَ كَفَرُوا۟ بِرَبِّهِمْ عَذَابُ جَهَنَّمَ ۖ وَبِئْسَ ٱلْمَصِيرُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5248"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mulk";
  dbp:text "When they are thrown into it, they hear from it a [dreadful] inhaling while it boils up.";
  <http://localhost:3333/has_arabic_text> "إِذَآ أُلْقُوا۟ فِيهَا سَمِعُوا۟ لَهَا شَهِيقًا وَهِىَ تَفُورُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5249"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mulk";
  dbp:text "It almost bursts with rage. Every time a company is thrown into it, its keepers ask them, \"Did there not come to you a warner?\"";
  <http://localhost:3333/has_arabic_text> "تَكَادُ تَمَيَّزُ مِنَ ٱلْغَيْظِ ۖ كُلَّمَآ أُلْقِىَ فِيهَا فَوْجٌ سَأَلَهُمْ خَزَنَتُهَآ أَلَمْ يَأْتِكُمْ نَذِيرٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5250"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mulk";
  dbp:text "They will say,\" Yes, a warner had come to us, but we denied and said, 'Allah has not sent down anything. You are not but in great error.'\"";
  <http://localhost:3333/has_arabic_text> "قَالُوا۟ بَلَىٰ قَدْ جَآءَنَا نَذِيرٌ فَكَذَّبْنَا وَقُلْنَا مَا نَزَّلَ ٱللَّهُ مِن شَىْءٍ إِنْ أَنتُمْ إِلَّا فِى ضَلَٰلٍ كَبِيرٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5251"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mulk";
  dbp:text "And they will say, \"If only we had been listening or reasoning, we would not be among the companions of the Blaze.\"";
  <http://localhost:3333/has_arabic_text> "وَقَالُوا۟ لَوْ كُنَّا نَسْمَعُ أَوْ نَعْقِلُ مَا كُنَّا فِىٓ أَصْحَٰبِ ٱلسَّعِيرِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5252"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mulk";
  dbp:text "And they will admit their sin, so [it is] alienation for the companions of the Blaze.";
  <http://localhost:3333/has_arabic_text> "فَٱعْتَرَفُوا۟ بِذَنۢبِهِمْ فَسُحْقًا لِّأَصْحَٰبِ ٱلسَّعِيرِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5253"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mulk";
  dbp:text "Indeed, those who fear their Lord unseen will have forgiveness and great reward.";
  <http://localhost:3333/has_arabic_text> "إِنَّ ٱلَّذِينَ يَخْشَوْنَ رَبَّهُم بِٱلْغَيْبِ لَهُم مَّغْفِرَةٌ وَأَجْرٌ كَبِيرٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5254"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mulk";
  dbp:text "And conceal your speech or publicize it; indeed, He is Knowing of that within the breasts.";
  <http://localhost:3333/has_arabic_text> "وَأَسِرُّوا۟ قَوْلَكُمْ أَوِ ٱجْهَرُوا۟ بِهِۦٓ ۖ إِنَّهُۥ عَلِيمٌۢ بِذَاتِ ٱلصُّدُورِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5255"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mulk";
  dbp:text "Does He who created not know, while He is the Subtle, the Acquainted?";
  <http://localhost:3333/has_arabic_text> "أَلَا يَعْلَمُ مَنْ خَلَقَ وَهُوَ ٱللَّطِيفُ ٱلْخَبِيرُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5256"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mulk";
  dbp:text "It is He who made the earth tame for you - so walk among its slopes and eat of His provision - and to Him is the resurrection.";
  <http://localhost:3333/has_arabic_text> "هُوَ ٱلَّذِى جَعَلَ لَكُمُ ٱلْأَرْضَ ذَلُولًا فَٱمْشُوا۟ فِى مَنَاكِبِهَا وَكُلُوا۟ مِن رِّزْقِهِۦ ۖ وَإِلَيْهِ ٱلنُّشُورُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5257"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mulk";
  dbp:text "Do you feel secure that He who [holds authority] in the heaven would not cause the earth to swallow you and suddenly it would sway?";
  <http://localhost:3333/has_arabic_text> "ءَأَمِنتُم مَّن فِى ٱلسَّمَآءِ أَن يَخْسِفَ بِكُمُ ٱلْأَرْضَ فَإِذَا هِىَ تَمُورُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5258"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mulk";
  dbp:text "Or do you feel secure that He who [holds authority] in the heaven would not send against you a storm of stones? Then you would know how [severe] was My warning.";
  <http://localhost:3333/has_arabic_text> "أَمْ أَمِنتُم مَّن فِى ٱلسَّمَآءِ أَن يُرْسِلَ عَلَيْكُمْ حَاصِبًا ۖ فَسَتَعْلَمُونَ كَيْفَ نَذِيرِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5259"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mulk";
  dbp:text "And already had those before them denied, and how [terrible] was My reproach.";
  <http://localhost:3333/has_arabic_text> "وَلَقَدْ كَذَّبَ ٱلَّذِينَ مِن قَبْلِهِمْ فَكَيْفَ كَانَ نَكِيرِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5260"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mulk";
  dbp:text "Do they not see the birds above them with wings outspread and [sometimes] folded in? None holds them [aloft] except the Most Merciful. Indeed He is, of all things, Seeing.";
  <http://localhost:3333/has_arabic_text> "أَوَلَمْ يَرَوْا۟ إِلَى ٱلطَّيْرِ فَوْقَهُمْ صَٰٓفَّٰتٍ وَيَقْبِضْنَ ۚ مَا يُمْسِكُهُنَّ إِلَّا ٱلرَّحْمَٰنُ ۚ إِنَّهُۥ بِكُلِّ شَىْءٍۭ بَصِيرٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbp:text "Or who is it that could be an army for you to aid you other than the Most Merciful? The disbelievers are not but in delusion.";
  dbo:ayah "5261"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mulk";
  <http://localhost:3333/has_arabic_text> "أَمَّنْ هَٰذَا ٱلَّذِى هُوَ جُندٌ لَّكُمْ يَنصُرُكُم مِّن دُونِ ٱلرَّحْمَٰنِ ۚ إِنِ ٱلْكَٰفِرُونَ إِلَّا فِى غُرُورٍ",
    "أَمَّنْ هَٰذَا ٱلَّذِى يَرْزُقُكُمْ إِنْ أَمْسَكَ رِزْقَهُۥ ۚ بَل لَّجُّوا۟ فِى عُتُوٍّ وَنُفُورٍ";
  dbo:ayah "5262"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "Or who is it that could provide for you if He withheld His provision? But they have persisted in insolence and aversion.";
  a dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Mulk";
  a dbr:Ayah;
  dbp:text "Then is one who walks fallen on his face better guided or one who walks erect on a straight path?";
  dbo:Surah "Al-Mulk";
  dbo:ayah "5263"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "أَفَمَن يَمْشِى مُكِبًّا عَلَىٰ وَجْهِهِۦٓ أَهْدَىٰٓ أَمَّن يَمْشِى سَوِيًّا عَلَىٰ صِرَٰطٍ مُّسْتَقِيمٍ";
  dbp:text "Say, \"It is He who has produced you and made for you hearing and vision and hearts; little are you grateful.\"";
  dbo:Surah "Al-Mulk";
  a dbr:Quranic_verse;
  dbo:ayah "5264"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "قُلْ هُوَ ٱلَّذِىٓ أَنشَأَكُمْ وَجَعَلَ لَكُمُ ٱلسَّمْعَ وَٱلْأَبْصَٰرَ وَٱلْأَفْـِٔدَةَ ۖ قَلِيلًا مَّا تَشْكُرُونَ";
  dbp:text "Say, \"It is He who has multiplied you throughout the earth, and to Him you will be gathered.\"";
  dbo:Surah "Al-Mulk";
  dbo:ayah "5265"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "قُلْ هُوَ ٱلَّذِى ذَرَأَكُمْ فِى ٱلْأَرْضِ وَإِلَيْهِ تُحْشَرُونَ";
  dbp:text "And they say, \"When is this promise, if you should be truthful?\"";
  dbo:Surah "Al-Mulk";
  dbo:ayah "5266"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "وَيَقُولُونَ مَتَىٰ هَٰذَا ٱلْوَعْدُ إِن كُنتُمْ صَٰدِقِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5267"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mulk";
  dbp:text "Say, \"The knowledge is only with Allah, and I am only a clear warner.\"";
  <http://localhost:3333/has_arabic_text> "قُلْ إِنَّمَا ٱلْعِلْمُ عِندَ ٱللَّهِ وَإِنَّمَآ أَنَا۠ نَذِيرٌ مُّبِينٌ",
    "فَلَمَّا رَأَوْهُ زُلْفَةً سِيٓـَٔتْ وُجُوهُ ٱلَّذِينَ كَفَرُوا۟ وَقِيلَ هَٰذَا ٱلَّذِى كُنتُم بِهِۦ تَدَّعُونَ";
  dbp:text "But when they see it approaching, the faces of those who disbelieve will be distressed, and it will be said, \"This is that for which you used to call.\"";
  a dbr:Ayat, dbr:Ayah;
  dbo:Surah "Al-Mulk";
  a dbr:Quranic_verse;
  dbo:ayah "5268"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "قُلْ أَرَءَيْتُمْ إِنْ أَهْلَكَنِىَ ٱللَّهُ وَمَن مَّعِىَ أَوْ رَحِمَنَا فَمَن يُجِيرُ ٱلْكَٰفِرِينَ مِنْ عَذَابٍ أَلِيمٍ";
  dbp:text "Say, [O Muhammad], \"Have you considered: whether Allah should cause my death and those with me or have mercy upon us, who can protect the disbelievers from a painful punishment?\"";
  a dbr:Ayat;
  dbo:Surah "Al-Mulk";
  a dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5269"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayah, dbr:Ayat;
  dbo:ayah "5270"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mulk";
  dbp:text "Say, \"He is the Most Merciful; we have believed in Him, and upon Him we have relied. And you will [come to] know who it is that is in clear error.\"";
  <http://localhost:3333/has_arabic_text> "قُلْ هُوَ ٱلرَّحْمَٰنُ ءَامَنَّا بِهِۦ وَعَلَيْهِ تَوَكَّلْنَا ۖ فَسَتَعْلَمُونَ مَنْ هُوَ فِى ضَلَٰلٍ مُّبِينٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5271"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mulk";
  dbp:text "Say, \"Have you considered: if your water was to become sunken [into the earth], then who could bring you flowing water?\"";
  <http://localhost:3333/has_arabic_text> "قُلْ أَرَءَيْتُمْ إِنْ أَصْبَحَ مَآؤُكُمْ غَوْرًا فَمَن يَأْتِيكُم بِمَآءٍ مَّعِينٍۭ",
    "ٱلَّذِى خَلَقَ سَبْعَ سَمَٰوَٰتٍ طِبَاقًا ۖ مَّا تَرَىٰ فِى خَلْقِ ٱلرَّحْمَٰنِ مِن تَفَٰوُتٍ ۖ فَٱرْجِعِ ٱلْبَصَرَ هَلْ تَرَىٰ مِن فُطُورٍ",
    "ٱلَّذِى خَلَقَ ٱلْمَوْتَ وَٱلْحَيَوٰةَ لِيَبْلُوَكُمْ أَيُّكُمْ أَحْسَنُ عَمَلًا ۚ وَهُوَ ٱلْعَزِيزُ ٱلْغَفُورُ" .

<http://localhost:3333/Al-Qalam> dbo:Surah "Al-Qalam";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5272"^^<http://www.w3.org/2001/XMLSchema#int>, "5273"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat, dbr:Ayah;
  dbo:Surah "Al-Qalam";
  dbp:text "Nun. By the pen and what they inscribe,", "You are not, [O Muhammad], by the favor of your Lord, a madman.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5274"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "And indeed, for you is a reward uninterrupted.";
  <http://localhost:3333/has_arabic_text> "مَآ أَنتَ بِنِعْمَةِ رَبِّكَ بِمَجْنُونٍ";
  dbo:Surah "Al-Qalam";
  a dbr:Quranic_verse, dbr:Ayah;
  dbp:text "And indeed, you are of a great moral character.";
  dbo:ayah "5275"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "نٓ ۚ وَٱلْقَلَمِ وَمَا يَسْطُرُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5276"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "So you will see and they will see";
  <http://localhost:3333/has_arabic_text> "فَسَتُبْصِرُ وَيُبْصِرُونَ";
  a dbr:Ayah, dbr:Quranic_verse;
  dbo:Surah "Al-Qalam";
  dbp:text "Which of you is the afflicted [by a devil].";
  a dbr:Ayat;
  dbo:ayah "5277"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "بِأَييِّكُمُ ٱلْمَفْتُونُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5278"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "Indeed, your Lord is most knowing of who has gone astray from His way, and He is most knowing of the [rightly] guided.";
  <http://localhost:3333/has_arabic_text> "إِنَّ رَبَّكَ هُوَ أَعْلَمُ بِمَن ضَلَّ عَن سَبِيلِهِۦ وَهُوَ أَعْلَمُ بِٱلْمُهْتَدِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5279"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "Then do not obey the deniers.";
  <http://localhost:3333/has_arabic_text> "فَلَا تُطِعِ ٱلْمُكَذِّبِينَ";
  dbo:Surah "Al-Qalam";
  a dbr:Quranic_verse;
  dbo:ayah "5280"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "They wish that you would soften [in your position], so they would soften [toward you].";
  a dbr:Ayat, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "وَإِنَّ لَكَ لَأَجْرًا غَيْرَ مَمْنُونٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5281"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "And do not obey every worthless habitual swearer";
  <http://localhost:3333/has_arabic_text> "وَإِنَّكَ لَعَلَىٰ خُلُقٍ عَظِيمٍ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5282"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "[And] scorner, going about with malicious gossip -";
  <http://localhost:3333/has_arabic_text> "هَمَّازٍ مَّشَّآءٍۭ بِنَمِيمٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5283"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "A preventer of good, transgressing and sinful,";
  <http://localhost:3333/has_arabic_text> "مَّنَّاعٍ لِّلْخَيْرِ مُعْتَدٍ أَثِيمٍ";
  dbo:Surah "Al-Qalam";
  dbp:text "Cruel, moreover, and an illegitimate pretender.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5284"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "عُتُلٍّۭ بَعْدَ ذَٰلِكَ زَنِيمٍ", "أَن كَانَ ذَا مَالٍ وَبَنِينَ";
  dbo:ayah "5285"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat;
  dbp:text "Because he is a possessor of wealth and children,";
  a dbr:Ayah;
  dbo:Surah "Al-Qalam";
  a dbr:Quranic_verse, dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5286"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "When Our verses are recited to him, he says, \"Legends of the former peoples.\"";
  <http://localhost:3333/has_arabic_text> "إِذَا تُتْلَىٰ عَلَيْهِ ءَايَٰتُنَا قَالَ أَسَٰطِيرُ ٱلْأَوَّلِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5287"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "We will brand him upon the snout.";
  <http://localhost:3333/has_arabic_text> "سَنَسِمُهُۥ عَلَى ٱلْخُرْطُومِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5288"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "Indeed, We have tried them as We tried the companions of the garden, when they swore to cut its fruit in the [early] morning";
  <http://localhost:3333/has_arabic_text> "إِنَّا بَلَوْنَٰهُمْ كَمَا بَلَوْنَآ أَصْحَٰبَ ٱلْجَنَّةِ إِذْ أَقْسَمُوا۟ لَيَصْرِمُنَّهَا مُصْبِحِينَ";
  dbo:Surah "Al-Qalam";
  a dbr:Ayah;
  dbo:ayah "5289"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "Without making exception.";
  a dbr:Ayat, dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "وَدُّوا۟ لَوْ تُدْهِنُ فَيُدْهِنُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5290"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "So there came upon the garden an affliction from your Lord while they were asleep.";
  <http://localhost:3333/has_arabic_text> "فَطَافَ عَلَيْهَا طَآئِفٌ مِّن رَّبِّكَ وَهُمْ نَآئِمُونَ",
    "فَأَصْبَحَتْ كَٱلصَّرِيمِ";
  dbo:Surah "Al-Qalam";
  dbp:text "And it became as though reaped.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5291"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5292"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "And they called one another at morning,";
  <http://localhost:3333/has_arabic_text> "فَتَنَادَوْا۟ مُصْبِحِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5293"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "[Saying], \"Go early to your crop if you would cut the fruit.\"";
  <http://localhost:3333/has_arabic_text> "أَنِ ٱغْدُوا۟ عَلَىٰ حَرْثِكُمْ إِن كُنتُمْ صَٰرِمِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5294"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "So they set out, while lowering their voices,";
  <http://localhost:3333/has_arabic_text> "فَٱنطَلَقُوا۟ وَهُمْ يَتَخَٰفَتُونَ";
  dbp:text "[Saying], \"There will surely not enter it today upon you [any] poor person.\"";
  dbo:Surah "Al-Qalam";
  dbo:ayah "5295"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "أَن لَّا يَدْخُلَنَّهَا ٱلْيَوْمَ عَلَيْكُم مِّسْكِينٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5296"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "And they went early in determination, [assuming themselves] able.";
  <http://localhost:3333/has_arabic_text> "وَغَدَوْا۟ عَلَىٰ حَرْدٍ قَٰدِرِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5297"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "But when they saw it, they said, \"Indeed, we are lost;";
  <http://localhost:3333/has_arabic_text> "فَلَمَّا رَأَوْهَا قَالُوٓا۟ إِنَّا لَضَآلُّونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5298"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "Rather, we have been deprived.\"";
  <http://localhost:3333/has_arabic_text> "بَلْ نَحْنُ مَحْرُومُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5299"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "The most moderate of them said, \"Did I not say to you, 'Why do you not exalt [Allah]?' \"";
  <http://localhost:3333/has_arabic_text> "قَالَ أَوْسَطُهُمْ أَلَمْ أَقُل لَّكُمْ لَوْلَا تُسَبِّحُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5300"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "They said, \"Exalted is our Lord! Indeed, we were wrongdoers.\"";
  <http://localhost:3333/has_arabic_text> "قَالُوا۟ سُبْحَٰنَ رَبِّنَآ إِنَّا كُنَّا ظَٰلِمِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5301"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "Then they approached one another, blaming each other.";
  <http://localhost:3333/has_arabic_text> "فَأَقْبَلَ بَعْضُهُمْ عَلَىٰ بَعْضٍ يَتَلَٰوَمُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5302"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "They said, \"O woe to us; indeed we were transgressors.";
  <http://localhost:3333/has_arabic_text> "قَالُوا۟ يَٰوَيْلَنَآ إِنَّا كُنَّا طَٰغِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5303"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "Perhaps our Lord will substitute for us [one] better than it. Indeed, we are toward our Lord desirous.\"";
  <http://localhost:3333/has_arabic_text> "عَسَىٰ رَبُّنَآ أَن يُبْدِلَنَا خَيْرًا مِّنْهَآ إِنَّآ إِلَىٰ رَبِّنَا رَٰغِبُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5304"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "Such is the punishment [of this world]. And the punishment of the Hereafter is greater, if they only knew.";
  <http://localhost:3333/has_arabic_text> "كَذَٰلِكَ ٱلْعَذَابُ ۖ وَلَعَذَابُ ٱلْءَاخِرَةِ أَكْبَرُ ۚ لَوْ كَانُوا۟ يَعْلَمُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5305"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "Indeed, for the righteous with their Lord are the Gardens of Pleasure.";
  <http://localhost:3333/has_arabic_text> "إِنَّ لِلْمُتَّقِينَ عِندَ رَبِّهِمْ جَنَّٰتِ ٱلنَّعِيمِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5306"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "Then will We treat the Muslims like the criminals?";
  <http://localhost:3333/has_arabic_text> "أَفَنَجْعَلُ ٱلْمُسْلِمِينَ كَٱلْمُجْرِمِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5307"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "What is [the matter] with you? How do you judge?";
  <http://localhost:3333/has_arabic_text> "مَا لَكُمْ كَيْفَ تَحْكُمُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5308"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "Or do you have a scripture in which you learn";
  <http://localhost:3333/has_arabic_text> "أَمْ لَكُمْ كِتَٰبٌ فِيهِ تَدْرُسُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5309"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "That indeed for you is whatever you choose?";
  <http://localhost:3333/has_arabic_text> "إِنَّ لَكُمْ فِيهِ لَمَا تَخَيَّرُونَ", "أَمْ لَكُمْ أَيْمَٰنٌ عَلَيْنَا بَٰلِغَةٌ إِلَىٰ يَوْمِ ٱلْقِيَٰمَةِ ۙ إِنَّ لَكُمْ لَمَا تَحْكُمُونَ";
  a dbr:Ayat, dbr:Ayah;
  dbo:Surah "Al-Qalam";
  a dbr:Quranic_verse;
  dbp:text "Or do you have oaths [binding] upon Us, extending until the Day of Resurrection, that indeed for you is whatever you judge?";
  dbo:ayah "5310"^^<http://www.w3.org/2001/XMLSchema#int>, "5311"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbp:text "Ask them which of them, for that [claim], is responsible.";
  dbo:Surah "Al-Qalam";
  <http://localhost:3333/has_arabic_text> "سَلْهُمْ أَيُّهُم بِذَٰلِكَ زَعِيمٌ";
  dbo:Surah "Al-Qalam";
  dbo:ayah "5312"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "Or do they have partners? Then let them bring their partners, if they should be truthful.";
  a dbr:Ayat, dbr:Ayah, dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "أَمْ لَهُمْ شُرَكَآءُ فَلْيَأْتُوا۟ بِشُرَكَآئِهِمْ إِن كَانُوا۟ صَٰدِقِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5313"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "The Day the shin will be uncovered and they are invited to prostration but the disbelievers will not be able,";
  <http://localhost:3333/has_arabic_text> "وَلَا تُطِعْ كُلَّ حَلَّافٍ مَّهِينٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5314"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "Their eyes humbled, humiliation will cover them. And they used to be invited to prostration while they were sound.";
  <http://localhost:3333/has_arabic_text> "خَٰشِعَةً أَبْصَٰرُهُمْ تَرْهَقُهُمْ ذِلَّةٌ ۖ وَقَدْ كَانُوا۟ يُدْعَوْنَ إِلَى ٱلسُّجُودِ وَهُمْ سَٰلِمُونَ",
    "فَذَرْنِى وَمَن يُكَذِّبُ بِهَٰذَا ٱلْحَدِيثِ ۖ سَنَسْتَدْرِجُهُم مِّنْ حَيْثُ لَا يَعْلَمُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbp:text "So leave Me, [O Muhammad], with [the matter of] whoever denies the Qur'an. We will progressively lead them [to punishment] from where they do not know.";
  dbo:ayah "5315"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "And I will give them time. Indeed, My plan is firm.";
  dbo:Surah "Al-Qalam";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5316"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "وَأُمْلِى لَهُمْ ۚ إِنَّ كَيْدِى مَتِينٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5317"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "Or do you ask of them a payment, so they are by debt burdened down?";
  <http://localhost:3333/has_arabic_text> "أَمْ تَسْـَٔلُهُمْ أَجْرًا فَهُم مِّن مَّغْرَمٍ مُّثْقَلُونَ",
    "أَمْ عِندَهُمُ ٱلْغَيْبُ فَهُمْ يَكْتُبُونَ";
  a dbr:Ayat;
  dbo:ayah "5318"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "Or have they [knowledge of] the unseen, so they write [it] down?";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5319"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "Then be patient for the decision of your Lord, [O Muhammad], and be not like the companion of the fish when he called out while he was distressed.";
  <http://localhost:3333/has_arabic_text> "فَٱصْبِرْ لِحُكْمِ رَبِّكَ وَلَا تَكُن كَصَاحِبِ ٱلْحُوتِ إِذْ نَادَىٰ وَهُوَ مَكْظُومٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5320"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "If not that a favor from his Lord overtook him, he would have been thrown onto the naked shore while he was censured.";
  <http://localhost:3333/has_arabic_text> "لَّوْلَآ أَن تَدَٰرَكَهُۥ نِعْمَةٌ مِّن رَّبِّهِۦ لَنُبِذَ بِٱلْعَرَآءِ وَهُوَ مَذْمُومٌ";
  dbo:Surah "Al-Qalam";
  a dbr:Ayah;
  dbo:ayah "5321"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat;
  dbp:text "And his Lord chose him and made him of the righteous.";
  <http://localhost:3333/has_arabic_text> "فَٱجْتَبَٰهُ رَبُّهُۥ فَجَعَلَهُۥ مِنَ ٱلصَّٰلِحِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5322"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qalam";
  dbp:text "And indeed, those who disbelieve would almost make you slip with their eyes when they hear the message, and they say, \"Indeed, he is mad.\"";
  <http://localhost:3333/has_arabic_text> "وَإِن يَكَادُ ٱلَّذِينَ كَفَرُوا۟ لَيُزْلِقُونَكَ بِأَبْصَٰرِهِمْ لَمَّا سَمِعُوا۟ ٱلذِّكْرَ وَيَقُولُونَ إِنَّهُۥ لَمَجْنُونٌ";
  a dbr:Quranic_verse;
  dbp:text "But it is not except a reminder to the worlds.";
  a dbr:Ayat, dbr:Ayah;
  dbo:Surah "Al-Qalam";
  dbo:ayah "5323"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "وَلَا يَسْتَثْنُونَ", "وَمَا هُوَ إِلَّا ذِكْرٌ لِّلْعَٰلَمِينَ",
    "يَوْمَ يُكْشَفُ عَن سَاقٍ وَيُدْعَوْنَ إِلَى ٱلسُّجُودِ فَلَا يَسْتَطِيعُونَ" .

<http://localhost:3333/Al-Haqqah> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Haqqah";
  dbo:ayah "5324"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5325"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "The Inevitable Reality -", "What is the Inevitable Reality?";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5326"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "And what can make you know what is the Inevitable Reality?";
  <http://localhost:3333/has_arabic_text> "مَا ٱلْحَآقَّةُ";
  a dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5327"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  a dbr:Ayah;
  dbp:text "Thamud and 'Aad denied the Striking Calamity.";
  <http://localhost:3333/has_arabic_text> "كَذَّبَتْ ثَمُودُ وَعَادٌۢ بِٱلْقَارِعَةِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5328"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "So as for Thamud, they were destroyed by the overpowering [blast].";
  <http://localhost:3333/has_arabic_text> "فَأَمَّا ثَمُودُ فَأُهْلِكُوا۟ بِٱلطَّاغِيَةِ";
  a dbr:Ayah;
  dbo:ayah "5329"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Haqqah";
  dbp:text "And as for 'Aad, they were destroyed by a screaming, violent wind";
  <http://localhost:3333/has_arabic_text> "وَأَمَّا عَادٌ فَأُهْلِكُوا۟ بِرِيحٍ صَرْصَرٍ عَاتِيَةٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5330"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "Which Allah imposed upon them for seven nights and eight days in succession, so you would see the people therein fallen as if they were hollow trunks of palm trees.";
  <http://localhost:3333/has_arabic_text> "سَخَّرَهَا عَلَيْهِمْ سَبْعَ لَيَالٍ وَثَمَٰنِيَةَ أَيَّامٍ حُسُومًا فَتَرَى ٱلْقَوْمَ فِيهَا صَرْعَىٰ كَأَنَّهُمْ أَعْجَازُ نَخْلٍ خَاوِيَةٍ";
  dbo:ayah "5331"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:Surah "Al-Haqqah";
  dbp:text "Then do you see of them any remains?";
  <http://localhost:3333/has_arabic_text> "فَهَلْ تَرَىٰ لَهُم مِّنۢ بَاقِيَةٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5332"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "And there came Pharaoh and those before him and the overturned cities with sin.";
  <http://localhost:3333/has_arabic_text> "وَجَآءَ فِرْعَوْنُ وَمَن قَبْلَهُۥ وَٱلْمُؤْتَفِكَٰتُ بِٱلْخَاطِئَةِ";
  a dbr:Ayat;
  dbo:Surah "Al-Haqqah";
  a dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5333"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "And they disobeyed the messenger of their Lord, so He seized them with a seizure exceeding [in severity].";
  <http://localhost:3333/has_arabic_text> "فَعَصَوْا۟ رَسُولَ رَبِّهِمْ فَأَخَذَهُمْ أَخْذَةً رَّابِيَةً";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5334"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "Indeed, when the water overflowed, We carried your ancestors in the sailing ship";
  <http://localhost:3333/has_arabic_text> "إِنَّا لَمَّا طَغَا ٱلْمَآءُ حَمَلْنَٰكُمْ فِى ٱلْجَارِيَةِ";
  dbo:Surah "Al-Haqqah";
  a dbr:Ayat;
  dbo:ayah "5335"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah;
  dbp:text "That We might make it for you a reminder and [that] a conscious ear would be conscious of it.";
  a dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "لِنَجْعَلَهَا لَكُمْ تَذْكِرَةً وَتَعِيَهَآ أُذُنٌ وَٰعِيَةٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5336"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "Then when the Horn is blown with one blast";
  <http://localhost:3333/has_arabic_text> "فَإِذَا نُفِخَ فِى ٱلصُّورِ نَفْخَةٌ وَٰحِدَةٌ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5337"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "And the earth and the mountains are lifted and leveled with one blow -";
  <http://localhost:3333/has_arabic_text> "وَحُمِلَتِ ٱلْأَرْضُ وَٱلْجِبَالُ فَدُكَّتَا دَكَّةً وَٰحِدَةً";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5338"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "Then on that Day, the Resurrection will occur,";
  <http://localhost:3333/has_arabic_text> "فَيَوْمَئِذٍ وَقَعَتِ ٱلْوَاقِعَةُ";
  a dbr:Ayah;
  dbo:Surah "Al-Haqqah";
  dbo:ayah "5339"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat;
  dbp:text "And the heaven will split [open], for that Day it is infirm.";
  <http://localhost:3333/has_arabic_text> "وَمَآ أَدْرَىٰكَ مَا ٱلْحَآقَّةُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5340"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "And the angels are at its edges. And there will bear the Throne of your Lord above them, that Day, eight [of them].";
  <http://localhost:3333/has_arabic_text> "وَٱلْمَلَكُ عَلَىٰٓ أَرْجَآئِهَا ۚ وَيَحْمِلُ عَرْشَ رَبِّكَ فَوْقَهُمْ يَوْمَئِذٍ ثَمَٰنِيَةٌ";
  dbo:ayah "5341"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse;
  dbp:text "That Day, you will be exhibited [for judgement]; not hidden among you is anything concealed.";
  dbo:Surah "Al-Haqqah";
  a dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "وَٱنشَقَّتِ ٱلسَّمَآءُ فَهِىَ يَوْمَئِذٍ وَاهِيَةٌ";
  a dbr:Quranic_verse, dbr:Ayah, dbr:Ayat;
  dbo:ayah "5342"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "So as for he who is given his record in his right hand, he will say, \"Here, read my record!";
  <http://localhost:3333/has_arabic_text> "فَأَمَّا مَنْ أُوتِىَ كِتَٰبَهُۥ بِيَمِينِهِۦ فَيَقُولُ هَآؤُمُ ٱقْرَءُوا۟ كِتَٰبِيَهْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5343"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "Indeed, I was certain that I would be meeting my account.\"";
  <http://localhost:3333/has_arabic_text> "إِنِّى ظَنَنتُ أَنِّى مُلَٰقٍ حِسَابِيَهْ";
  dbo:Surah "Al-Haqqah";
  dbo:ayah "5344"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "So he will be in a pleasant life -";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "فَهُوَ فِى عِيشَةٍ رَّاضِيَةٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5345"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "In an elevated garden,";
  <http://localhost:3333/has_arabic_text> "فِى جَنَّةٍ عَالِيَةٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5346"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "Its [fruit] to be picked hanging near.";
  <http://localhost:3333/has_arabic_text> "قُطُوفُهَا دَانِيَةٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5347"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "[They will be told], \"Eat and drink in satisfaction for what you put forth in the days past.\"";
  <http://localhost:3333/has_arabic_text> "كُلُوا۟ وَٱشْرَبُوا۟ هَنِيٓـًٔۢا بِمَآ أَسْلَفْتُمْ فِى ٱلْأَيَّامِ ٱلْخَالِيَةِ";
  a dbr:Ayah;
  dbo:ayah "5348"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat;
  dbp:text "But as for he who is given his record in his left hand, he will say, \"Oh, I wish I had not been given my record";
  dbo:Surah "Al-Haqqah";
  <http://localhost:3333/has_arabic_text> "وَأَمَّا مَنْ أُوتِىَ كِتَٰبَهُۥ بِشِمَالِهِۦ فَيَقُولُ يَٰلَيْتَنِى لَمْ أُوتَ كِتَٰبِيَهْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5349"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "And had not known what is my account.";
  <http://localhost:3333/has_arabic_text> "وَلَمْ أَدْرِ مَا حِسَابِيَهْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5350"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "I wish my death had been the decisive one.";
  <http://localhost:3333/has_arabic_text> "يَوْمَئِذٍ تُعْرَضُونَ لَا تَخْفَىٰ مِنكُمْ خَافِيَةٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5351"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "My wealth has not availed me.";
  <http://localhost:3333/has_arabic_text> "مَآ أَغْنَىٰ عَنِّى مَالِيَهْ ۜ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5352"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "Gone from me is my authority.\"";
  <http://localhost:3333/has_arabic_text> "هَلَكَ عَنِّى سُلْطَٰنِيَهْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5353"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "[Allah will say], \"Seize him and shackle him.";
  <http://localhost:3333/has_arabic_text> "خُذُوهُ فَغُلُّوهُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5354"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "Then into Hellfire drive him.";
  <http://localhost:3333/has_arabic_text> "ثُمَّ ٱلْجَحِيمَ صَلُّوهُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5355"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "Then into a chain whose length is seventy cubits insert him.\"";
  <http://localhost:3333/has_arabic_text> "ثُمَّ فِى سِلْسِلَةٍ ذَرْعُهَا سَبْعُونَ ذِرَاعًا فَٱسْلُكُوهُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5356"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "Indeed, he did not used to believe in Allah, the Most Great,";
  <http://localhost:3333/has_arabic_text> "إِنَّهُۥ كَانَ لَا يُؤْمِنُ بِٱللَّهِ ٱلْعَظِيمِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5357"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "Nor did he encourage the feeding of the poor.";
  <http://localhost:3333/has_arabic_text> "وَلَا يَحُضُّ عَلَىٰ طَعَامِ ٱلْمِسْكِينِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5358"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "So there is not for him here this Day any devoted friend";
  <http://localhost:3333/has_arabic_text> "فَلَيْسَ لَهُ ٱلْيَوْمَ هَٰهُنَا حَمِيمٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5359"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "Nor any food except from the discharge of wounds;";
  <http://localhost:3333/has_arabic_text> "وَلَا طَعَامٌ إِلَّا مِنْ غِسْلِينٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5360"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "None will eat it except the sinners.";
  <http://localhost:3333/has_arabic_text> "لَّا يَأْكُلُهُۥٓ إِلَّا ٱلْخَٰطِـُٔونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5361"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "So I swear by what you see";
  <http://localhost:3333/has_arabic_text> "فَلَآ أُقْسِمُ بِمَا تُبْصِرُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5362"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "And what you do not see";
  <http://localhost:3333/has_arabic_text> "وَمَا لَا تُبْصِرُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5363"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "[That] indeed, the Qur'an is the word of a noble Messenger.";
  <http://localhost:3333/has_arabic_text> "إِنَّهُۥ لَقَوْلُ رَسُولٍ كَرِيمٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5364"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "And it is not the word of a poet; little do you believe.";
  <http://localhost:3333/has_arabic_text> "وَمَا هُوَ بِقَوْلِ شَاعِرٍ ۚ قَلِيلًا مَّا تُؤْمِنُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5365"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "Nor the word of a soothsayer; little do you remember.";
  <http://localhost:3333/has_arabic_text> "وَلَا بِقَوْلِ كَاهِنٍ ۚ قَلِيلًا مَّا تَذَكَّرُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5366"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "[It is] a revelation from the Lord of the worlds.";
  <http://localhost:3333/has_arabic_text> "تَنزِيلٌ مِّن رَّبِّ ٱلْعَٰلَمِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5367"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "And if Muhammad had made up about Us some [false] sayings,";
  <http://localhost:3333/has_arabic_text> "وَلَوْ تَقَوَّلَ عَلَيْنَا بَعْضَ ٱلْأَقَاوِيلِ";
  a dbr:Ayah;
  dbo:Surah "Al-Haqqah";
  a dbr:Quranic_verse;
  dbo:ayah "5368"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat;
  dbp:text "We would have seized him by the right hand;";
  <http://localhost:3333/has_arabic_text> "لَأَخَذْنَا مِنْهُ بِٱلْيَمِينِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5369"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "Then We would have cut from him the aorta.";
  <http://localhost:3333/has_arabic_text> "ثُمَّ لَقَطَعْنَا مِنْهُ ٱلْوَتِينَ";
  dbo:ayah "5370"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:Surah "Al-Haqqah";
  dbp:text "And there is no one of you who could prevent [Us] from him.";
  <http://localhost:3333/has_arabic_text> "فَمَا مِنكُم مِّنْ أَحَدٍ عَنْهُ حَٰجِزِينَ";
  dbp:text "And indeed, the Qur'an is a reminder for the righteous.";
  dbo:Surah "Al-Haqqah";
  dbo:ayah "5371"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "وَإِنَّهُۥ لَتَذْكِرَةٌ لِّلْمُتَّقِينَ",
    "وَإِنَّا لَنَعْلَمُ أَنَّ مِنكُم مُّكَذِّبِينَ";
  dbp:text "And indeed, We know that among you are deniers.";
  dbo:Surah "Al-Haqqah";
  dbo:ayah "5372"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "وَإِنَّهُۥ لَحَسْرَةٌ عَلَى ٱلْكَٰفِرِينَ";
  dbp:text "And indeed, it will be [a cause of] regret upon the disbelievers.";
  dbo:Surah "Al-Haqqah";
  dbo:ayah "5373"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Ayah, dbr:Quranic_verse;
  dbo:Surah "Al-Haqqah";
  dbo:ayah "5374"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbp:text "And indeed, it is the truth of certainty.";
  <http://localhost:3333/has_arabic_text> "وَإِنَّهُۥ لَحَقُّ ٱلْيَقِينِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5375"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Haqqah";
  dbp:text "So exalt the name of your Lord, the Most Great.";
  <http://localhost:3333/has_arabic_text> "فَسَبِّحْ بِٱسْمِ رَبِّكَ ٱلْعَظِيمِ", "يَٰلَيْتَهَا كَانَتِ ٱلْقَاضِيَةَ",
    "ٱلْحَآقَّةُ" .

<http://localhost:3333/Al-Ma'aarij> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Ma'aarij";
  dbo:ayah "5376"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat;
  dbp:text "A supplicant asked for a punishment bound to happen";
  dbo:ayah "5377"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ma'aarij";
  a dbr:Ayah;
  dbp:text "To the disbelievers; of it there is no preventer.", "[It is] from Allah, owner of the ways of ascent.";
  dbo:Surah "Al-Ma'aarij";
  dbo:ayah "5378"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "سَأَلَ سَآئِلٌۢ بِعَذَابٍ وَاقِعٍ";
  dbp:text "The angels and the Spirit will ascend to Him during a Day the extent of which is fifty thousand years.";
  dbo:Surah "Al-Ma'aarij";
  dbo:ayah "5379"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "تَعْرُجُ ٱلْمَلَٰٓئِكَةُ وَٱلرُّوحُ إِلَيْهِ فِى يَوْمٍ كَانَ مِقْدَارُهُۥ خَمْسِينَ أَلْفَ سَنَةٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5380"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ma'aarij";
  dbp:text "So be patient with gracious patience.";
  <http://localhost:3333/has_arabic_text> "فَٱصْبِرْ صَبْرًا جَمِيلًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5381"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "Indeed, they see it [as] distant,";
  dbo:Surah "Al-Ma'aarij";
  <http://localhost:3333/has_arabic_text> "إِنَّهُمْ يَرَوْنَهُۥ بَعِيدًا";
  dbp:text "But We see it [as] near.";
  dbo:Surah "Al-Ma'aarij";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5382"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "لِّلْكَٰفِرِينَ لَيْسَ لَهُۥ دَافِعٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5383"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ma'aarij";
  dbp:text "On the Day the sky will be like murky oil,";
  <http://localhost:3333/has_arabic_text> "مِّنَ ٱللَّهِ ذِى ٱلْمَعَارِجِ", "وَتَكُونُ ٱلْجِبَالُ كَٱلْعِهْنِ";
  dbo:Surah "Al-Ma'aarij";
  a dbr:Ayat, dbr:Quranic_verse;
  dbo:ayah "5384"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah;
  dbp:text "And the mountains will be like wool,";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5385"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ma'aarij";
  dbp:text "And no friend will ask [anything of] a friend,";
  <http://localhost:3333/has_arabic_text> "وَلَا يَسْـَٔلُ حَمِيمٌ حَمِيمًا";
  a dbr:Ayat, dbr:Quranic_verse;
  dbo:ayah "5386"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah;
  dbo:Surah "Al-Ma'aarij";
  dbp:text "They will be shown each other. The criminal will wish that he could be ransomed from the punishment of that Day by his children";
  <http://localhost:3333/has_arabic_text> "وَنَرَىٰهُ قَرِيبًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5387"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ma'aarij";
  dbp:text "And his wife and his brother";
  <http://localhost:3333/has_arabic_text> "وَصَٰحِبَتِهِۦ وَأَخِيهِ";
  a dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Ma'aarij";
  dbo:ayah "5388"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah;
  dbp:text "And his nearest kindred who shelter him";
  <http://localhost:3333/has_arabic_text> "وَفَصِيلَتِهِ ٱلَّتِى تُـْٔوِيهِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5389"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ma'aarij";
  dbp:text "And whoever is on earth entirely [so] then it could save him.";
  <http://localhost:3333/has_arabic_text> "وَمَن فِى ٱلْأَرْضِ جَمِيعًا ثُمَّ يُنجِيهِ";
  a dbr:Quranic_verse, dbr:Ayat, dbr:Ayah;
  dbo:ayah "5390"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ma'aarij";
  dbp:text "No! Indeed, it is the Flame [of Hell],";
  <http://localhost:3333/has_arabic_text> "كَلَّآ ۖ إِنَّهَا لَظَىٰ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5391"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ma'aarij";
  dbp:text "A remover of exteriors.";
  <http://localhost:3333/has_arabic_text> "نَزَّاعَةً لِّلشَّوَىٰ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5392"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ma'aarij";
  dbp:text "It invites he who turned his back [on truth] and went away [from obedience]";
  <http://localhost:3333/has_arabic_text> "تَدْعُوا۟ مَنْ أَدْبَرَ وَتَوَلَّىٰ";
  dbp:text "And collected [wealth] and hoarded.";
  a dbr:Ayat;
  dbo:Surah "Al-Ma'aarij";
  a dbr:Ayah;
  dbo:ayah "5393"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "وَجَمَعَ فَأَوْعَىٰٓ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5394"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ma'aarij";
  dbp:text "Indeed, mankind was created anxious:";
  <http://localhost:3333/has_arabic_text> "إِنَّ ٱلْإِنسَٰنَ خُلِقَ هَلُوعًا";
  dbo:Surah "Al-Ma'aarij";
  a dbr:Ayat;
  dbo:ayah "5395"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah;
  dbp:text "When evil touches him, impatient,";
  a dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "إِذَا مَسَّهُ ٱلشَّرُّ جَزُوعًا";
  dbo:ayah "5396"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Ayah, dbr:Quranic_verse;
  dbo:Surah "Al-Ma'aarij";
  dbp:text "And when good touches him, withholding [of it],";
  <http://localhost:3333/has_arabic_text> "وَإِذَا مَسَّهُ ٱلْخَيْرُ مَنُوعًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5397"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ma'aarij";
  dbp:text "Except the observers of prayer -";
  <http://localhost:3333/has_arabic_text> "إِلَّا ٱلْمُصَلِّينَ";
  dbo:Surah "Al-Ma'aarij";
  dbp:text "Those who are constant in their prayer";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5398"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "يَوْمَ تَكُونُ ٱلسَّمَآءُ كَٱلْمُهْلِ";
  a dbr:Ayah;
  dbp:text "And those within whose wealth is a known right";
  dbo:Surah "Al-Ma'aarij";
  dbo:ayah "5399"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "وَٱلَّذِينَ فِىٓ أَمْوَٰلِهِمْ حَقٌّ مَّعْلُومٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5400"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ma'aarij";
  dbp:text "For the petitioner and the deprived -";
  <http://localhost:3333/has_arabic_text> "لِّلسَّآئِلِ وَٱلْمَحْرُومِ";
  a dbr:Ayah;
  dbp:text "And those who believe in the Day of Recompense";
  dbo:ayah "5401"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ma'aarij";
  a dbr:Quranic_verse, dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "وَٱلَّذِينَ يُصَدِّقُونَ بِيَوْمِ ٱلدِّينِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5402"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ma'aarij";
  dbp:text "And those who are fearful of the punishment of their Lord -";
  <http://localhost:3333/has_arabic_text> "وَٱلَّذِينَ هُم مِّنْ عَذَابِ رَبِّهِم مُّشْفِقُونَ";
  a dbr:Ayat;
  dbo:ayah "5403"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "Indeed, the punishment of their Lord is not that from which one is safe -";
  a dbr:Ayah, dbr:Quranic_verse;
  dbo:Surah "Al-Ma'aarij";
  <http://localhost:3333/has_arabic_text> "إِنَّ عَذَابَ رَبِّهِمْ غَيْرُ مَأْمُونٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5404"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ma'aarij";
  dbp:text "And those who guard their private parts";
  <http://localhost:3333/has_arabic_text> "وَٱلَّذِينَ هُمْ لِفُرُوجِهِمْ حَٰفِظُونَ";
  a dbr:Quranic_verse;
  dbo:ayah "5405"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Ayah;
  dbo:Surah "Al-Ma'aarij";
  dbp:text "Except from their wives or those their right hands possess, for indeed, they are not to be blamed -";
  <http://localhost:3333/has_arabic_text> "إِلَّا عَلَىٰٓ أَزْوَٰجِهِمْ أَوْ مَا مَلَكَتْ أَيْمَٰنُهُمْ فَإِنَّهُمْ غَيْرُ مَلُومِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5406"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ma'aarij";
  dbp:text "But whoever seeks beyond that, then they are the transgressors -";
  <http://localhost:3333/has_arabic_text> "فَمَنِ ٱبْتَغَىٰ وَرَآءَ ذَٰلِكَ فَأُو۟لَٰٓئِكَ هُمُ ٱلْعَادُونَ";
  a dbr:Quranic_verse, dbr:Ayah, dbr:Ayat;
  dbo:ayah "5407"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ma'aarij";
  dbp:text "And those who are to their trusts and promises attentive";
  <http://localhost:3333/has_arabic_text> "وَٱلَّذِينَ هُمْ لِأَمَٰنَٰتِهِمْ وَعَهْدِهِمْ رَٰعُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5408"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ma'aarij";
  dbp:text "And those who are in their testimonies upright";
  <http://localhost:3333/has_arabic_text> "وَٱلَّذِينَ هُم بِشَهَٰدَٰتِهِمْ قَآئِمُونَ";
  a dbr:Quranic_verse, dbr:Ayat, dbr:Ayah;
  dbo:ayah "5409"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ma'aarij";
  dbp:text "And those who [carefully] maintain their prayer:";
  <http://localhost:3333/has_arabic_text> "وَٱلَّذِينَ هُمْ عَلَىٰ صَلَاتِهِمْ يُحَافِظُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5410"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ma'aarij";
  dbp:text "They will be in gardens, honored.";
  <http://localhost:3333/has_arabic_text> "أُو۟لَٰٓئِكَ فِى جَنَّٰتٍ مُّكْرَمُونَ";
  dbo:Surah "Al-Ma'aarij";
  dbp:text "So what is [the matter] with those who disbelieve, hastening [from] before you, [O Muhammad],";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5411"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "فَمَالِ ٱلَّذِينَ كَفَرُوا۟ قِبَلَكَ مُهْطِعِينَ";
  dbp:text "[To sit] on [your] right and [your] left in separate groups?";
  dbo:Surah "Al-Ma'aarij";
  dbo:ayah "5412"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "عَنِ ٱلْيَمِينِ وَعَنِ ٱلشِّمَالِ عِزِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5413"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ma'aarij";
  dbp:text "Does every person among them aspire to enter a garden of pleasure?";
  <http://localhost:3333/has_arabic_text> "أَيَطْمَعُ كُلُّ ٱمْرِئٍ مِّنْهُمْ أَن يُدْخَلَ جَنَّةَ نَعِيمٍ";
  dbp:text "No! Indeed, We have created them from that which they know.";
  a dbr:Quranic_verse, dbr:Ayah, dbr:Ayat;
  dbo:Surah "Al-Ma'aarij";
  dbo:ayah "5414"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "كَلَّآ ۖ إِنَّا خَلَقْنَٰهُم مِّمَّا يَعْلَمُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5415"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ma'aarij";
  dbp:text "So I swear by the Lord of [all] risings and settings that indeed We are able";
  <http://localhost:3333/has_arabic_text> "فَلَآ أُقْسِمُ بِرَبِّ ٱلْمَشَٰرِقِ وَٱلْمَغَٰرِبِ إِنَّا لَقَٰدِرُونَ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5416"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ma'aarij";
  dbp:text "To replace them with better than them; and We are not to be outdone.";
  <http://localhost:3333/has_arabic_text> "عَلَىٰٓ أَن نُّبَدِّلَ خَيْرًا مِّنْهُمْ وَمَا نَحْنُ بِمَسْبُوقِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5417"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ma'aarij";
  dbp:text "So leave them to converse vainly and amuse themselves until they meet their Day which they are promised -";
  <http://localhost:3333/has_arabic_text> "فَذَرْهُمْ يَخُوضُوا۟ وَيَلْعَبُوا۟ حَتَّىٰ يُلَٰقُوا۟ يَوْمَهُمُ ٱلَّذِى يُوعَدُونَ";
  a dbr:Quranic_verse, dbr:Ayah, dbr:Ayat;
  dbo:Surah "Al-Ma'aarij";
  dbo:ayah "5418"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "The Day they will emerge from the graves rapidly as if they were, toward an erected idol, hastening.";
  <http://localhost:3333/has_arabic_text> "يَوْمَ يَخْرُجُونَ مِنَ ٱلْأَجْدَاثِ سِرَاعًا كَأَنَّهُمْ إِلَىٰ نُصُبٍ يُوفِضُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5419"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ma'aarij";
  dbp:text "Their eyes humbled, humiliation will cover them. That is the Day which they had been promised.";
  <http://localhost:3333/has_arabic_text> "خَٰشِعَةً أَبْصَٰرُهُمْ تَرْهَقُهُمْ ذِلَّةٌ ۚ ذَٰلِكَ ٱلْيَوْمُ ٱلَّذِى كَانُوا۟ يُوعَدُونَ",
    "يُبَصَّرُونَهُمْ ۚ يَوَدُّ ٱلْمُجْرِمُ لَوْ يَفْتَدِى مِنْ عَذَابِ يَوْمِئِذٍۭ بِبَنِيهِ",
    "ٱلَّذِينَ هُمْ عَلَىٰ صَلَاتِهِمْ دَآئِمُونَ" .

<http://localhost:3333/Nuh> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Nuh";
  dbo:ayah "5420"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5421"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Nuh";
  dbp:text "He said, \"O my people, indeed I am to you a clear warner,", "Indeed, We sent Noah to his people, [saying], \"Warn your people before there comes to them a painful punishment.\"";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5422"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Nuh";
  dbp:text "[Saying], 'Worship Allah, fear Him and obey me.";
  <http://localhost:3333/has_arabic_text> "أَنِ ٱعْبُدُوا۟ ٱللَّهَ وَٱتَّقُوهُ وَأَطِيعُونِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5423"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Nuh";
  dbp:text "Allah will forgive you of your sins and delay you for a specified term. Indeed, the time [set by] Allah, when it comes, will not be delayed, if you only knew.' \"";
  <http://localhost:3333/has_arabic_text> "إِنَّآ أَرْسَلْنَا نُوحًا إِلَىٰ قَوْمِهِۦٓ أَنْ أَنذِرْ قَوْمَكَ مِن قَبْلِ أَن يَأْتِيَهُمْ عَذَابٌ أَلِيمٌ";
  dbo:ayah "5424"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat, dbr:Ayah;
  dbo:Surah "Nuh";
  dbp:text "He said, \"My Lord, indeed I invited my people [to truth] night and day.";
  <http://localhost:3333/has_arabic_text> "قَالَ رَبِّ إِنِّى دَعَوْتُ قَوْمِى لَيْلًا وَنَهَارًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5425"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Nuh";
  dbp:text "But my invitation increased them not except in flight.";
  <http://localhost:3333/has_arabic_text> "فَلَمْ يَزِدْهُمْ دُعَآءِىٓ إِلَّا فِرَارًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5426"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Nuh";
  dbp:text "And indeed, every time I invited them that You may forgive them, they put their fingers in their ears, covered themselves with their garments, persisted, and were arrogant with [great] arrogance.";
  <http://localhost:3333/has_arabic_text> "قَالَ يَٰقَوْمِ إِنِّى لَكُمْ نَذِيرٌ مُّبِينٌ",
    "ثُمَّ إِنِّى دَعَوْتُهُمْ جِهَارًا";
  dbp:text "Then I invited them publicly.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5427"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Nuh";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5428"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Nuh";
  dbp:text "Then I announced to them and [also] confided to them secretly";
  <http://localhost:3333/has_arabic_text> "ثُمَّ إِنِّىٓ أَعْلَنتُ لَهُمْ وَأَسْرَرْتُ لَهُمْ إِسْرَارًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5429"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Nuh";
  dbp:text "And said, 'Ask forgiveness of your Lord. Indeed, He is ever a Perpetual Forgiver.";
  <http://localhost:3333/has_arabic_text> "فَقُلْتُ ٱسْتَغْفِرُوا۟ رَبَّكُمْ إِنَّهُۥ كَانَ غَفَّارًا";
  dbo:ayah "5430"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse;
  dbp:text "He will send [rain from] the sky upon you in [continuing] showers";
  dbo:Surah "Nuh";
  a dbr:Ayat, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "وَإِنِّى كُلَّمَا دَعَوْتُهُمْ لِتَغْفِرَ لَهُمْ جَعَلُوٓا۟ أَصَٰبِعَهُمْ فِىٓ ءَاذَانِهِمْ وَٱسْتَغْشَوْا۟ ثِيَابَهُمْ وَأَصَرُّوا۟ وَٱسْتَكْبَرُوا۟ ٱسْتِكْبَارًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5431"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Nuh";
  dbp:text "And give you increase in wealth and children and provide for you gardens and provide for you rivers.";
  <http://localhost:3333/has_arabic_text> "وَيُمْدِدْكُم بِأَمْوَٰلٍ وَبَنِينَ وَيَجْعَل لَّكُمْ جَنَّٰتٍ وَيَجْعَل لَّكُمْ أَنْهَٰرًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5432"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Nuh";
  dbp:text "What is [the matter] with you that you do not attribute to Allah [due] grandeur";
  <http://localhost:3333/has_arabic_text> "مَّا لَكُمْ لَا تَرْجُونَ لِلَّهِ وَقَارًا";
  dbp:text "While He has created you in stages?";
  a dbr:Ayat;
  dbo:Surah "Nuh";
  a dbr:Ayah, dbr:Quranic_verse;
  dbo:ayah "5433"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "وَقَدْ خَلَقَكُمْ أَطْوَارًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5434"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Nuh";
  dbp:text "Do you not consider how Allah has created seven heavens in layers";
  <http://localhost:3333/has_arabic_text> "أَلَمْ تَرَوْا۟ كَيْفَ خَلَقَ ٱللَّهُ سَبْعَ سَمَٰوَٰتٍ طِبَاقًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5435"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Nuh";
  dbp:text "And made the moon therein a [reflected] light and made the sun a burning lamp?";
  <http://localhost:3333/has_arabic_text> "وَجَعَلَ ٱلْقَمَرَ فِيهِنَّ نُورًا وَجَعَلَ ٱلشَّمْسَ سِرَاجًا";
  a dbr:Quranic_verse, dbr:Ayat, dbr:Ayah;
  dbo:ayah "5436"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Nuh";
  dbp:text "And Allah has caused you to grow from the earth a [progressive] growth.";
  <http://localhost:3333/has_arabic_text> "وَٱللَّهُ أَنۢبَتَكُم مِّنَ ٱلْأَرْضِ نَبَاتًا";
  a dbr:Quranic_verse, dbr:Ayat;
  dbp:text "Then He will return you into it and extract you [another] extraction.";
  dbo:Surah "Nuh";
  a dbr:Ayah;
  dbo:ayah "5437"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "ثُمَّ يُعِيدُكُمْ فِيهَا وَيُخْرِجُكُمْ إِخْرَاجًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5438"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Nuh";
  dbp:text "And Allah has made for you the earth an expanse";
  <http://localhost:3333/has_arabic_text> "وَٱللَّهُ جَعَلَ لَكُمُ ٱلْأَرْضَ بِسَاطًا";
  dbo:Surah "Nuh";
  dbo:ayah "5439"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbp:text "That you may follow therein roads of passage.' \"";
  <http://localhost:3333/has_arabic_text> "لِّتَسْلُكُوا۟ مِنْهَا سُبُلًا فِجَاجًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5440"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Nuh";
  dbp:text "Noah said, \"My Lord, indeed they have disobeyed me and followed him whose wealth and children will not increase him except in loss.";
  <http://localhost:3333/has_arabic_text> "قَالَ نُوحٌ رَّبِّ إِنَّهُمْ عَصَوْنِى وَٱتَّبَعُوا۟ مَن لَّمْ يَزِدْهُ مَالُهُۥ وَوَلَدُهُۥٓ إِلَّا خَسَارًا";
  a dbr:Quranic_verse;
  dbp:text "And they conspired an immense conspiracy.";
  a dbr:Ayah;
  dbo:Surah "Nuh";
  dbo:ayah "5441"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "وَمَكَرُوا۟ مَكْرًا كُبَّارًا";
  a dbr:Quranic_verse, dbr:Ayat, dbr:Ayah;
  dbo:ayah "5442"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Nuh";
  dbp:text "And said, 'Never leave your gods and never leave Wadd or Suwa' or Yaghuth and Ya'uq and Nasr.";
  <http://localhost:3333/has_arabic_text> "وَقَالُوا۟ لَا تَذَرُنَّ ءَالِهَتَكُمْ وَلَا تَذَرُنَّ وَدًّا وَلَا سُوَاعًا وَلَا يَغُوثَ وَيَعُوقَ وَنَسْرًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5443"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Nuh";
  dbp:text "And already they have misled many. And, [my Lord], do not increase the wrongdoers except in error.\"";
  <http://localhost:3333/has_arabic_text> "وَقَدْ أَضَلُّوا۟ كَثِيرًا ۖ وَلَا تَزِدِ ٱلظَّٰلِمِينَ إِلَّا ضَلَٰلًا";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5444"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "Because of their sins they were drowned and put into the Fire, and they found not for themselves besides Allah [any] helpers.";
  dbo:Surah "Nuh";
  <http://localhost:3333/has_arabic_text> "مِّمَّا خَطِيٓـَٰٔتِهِمْ أُغْرِقُوا۟ فَأُدْخِلُوا۟ نَارًا فَلَمْ يَجِدُوا۟ لَهُم مِّن دُونِ ٱللَّهِ أَنصَارًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5445"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Nuh";
  dbp:text "And Noah said, \"My Lord, do not leave upon the earth from among the disbelievers an inhabitant.";
  <http://localhost:3333/has_arabic_text> "وَقَالَ نُوحٌ رَّبِّ لَا تَذَرْ عَلَى ٱلْأَرْضِ مِنَ ٱلْكَٰفِرِينَ دَيَّارًا";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5446"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Nuh";
  dbp:text "Indeed, if You leave them, they will mislead Your servants and not beget except [every] wicked one and [confirmed] disbeliever.";
  <http://localhost:3333/has_arabic_text> "إِنَّكَ إِن تَذَرْهُمْ يُضِلُّوا۟ عِبَادَكَ وَلَا يَلِدُوٓا۟ إِلَّا فَاجِرًا كَفَّارًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5447"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Nuh";
  dbp:text "My Lord, forgive me and my parents and whoever enters my house a believer and the believing men and believing women. And do not increase the wrongdoers except in destruction.\"";
  <http://localhost:3333/has_arabic_text> "رَّبِّ ٱغْفِرْ لِى وَلِوَٰلِدَىَّ وَلِمَن دَخَلَ بَيْتِىَ مُؤْمِنًا وَلِلْمُؤْمِنِينَ وَٱلْمُؤْمِنَٰتِ وَلَا تَزِدِ ٱلظَّٰلِمِينَ إِلَّا تَبَارًۢا",
    "يَغْفِرْ لَكُم مِّن ذُنُوبِكُمْ وَيُؤَخِّرْكُمْ إِلَىٰٓ أَجَلٍ مُّسَمًّى ۚ إِنَّ أَجَلَ ٱللَّهِ إِذَا جَآءَ لَا يُؤَخَّرُ ۖ لَوْ كُنتُمْ تَعْلَمُونَ",
    "يُرْسِلِ ٱلسَّمَآءَ عَلَيْكُم مِّدْرَارًا" .

<http://localhost:3333/Al-Jinn> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Jinn";
  dbo:ayah "5448"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Jinn";
  a dbr:Ayah;
  dbo:ayah "5449"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse;
  dbp:text "It guides to the right course, and we have believed in it. And we will never associate with our Lord anyone.",
    "Say, [O Muhammad], \"It has been revealed to me that a group of the jinn listened and said, 'Indeed, we have heard an amazing Qur'an.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5450"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Jinn";
  dbp:text "And [it teaches] that exalted is the nobleness of our Lord; He has not taken a wife or a son";
  <http://localhost:3333/has_arabic_text> "قُلْ أُوحِىَ إِلَىَّ أَنَّهُ ٱسْتَمَعَ نَفَرٌ مِّنَ ٱلْجِنِّ فَقَالُوٓا۟ إِنَّا سَمِعْنَا قُرْءَانًا عَجَبًا";
  a dbr:Ayat, dbr:Quranic_verse;
  dbo:ayah "5451"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah;
  dbo:Surah "Al-Jinn";
  dbp:text "And that our foolish one has been saying about Allah an excessive transgression.";
  <http://localhost:3333/has_arabic_text> "وَأَنَّهُۥ تَعَٰلَىٰ جَدُّ رَبِّنَا مَا ٱتَّخَذَ صَٰحِبَةً وَلَا وَلَدًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5452"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Jinn";
  dbp:text "And we had thought that mankind and the jinn would never speak about Allah a lie.";
  <http://localhost:3333/has_arabic_text> "وَأَنَّا ظَنَنَّآ أَن لَّن تَقُولَ ٱلْإِنسُ وَٱلْجِنُّ عَلَى ٱللَّهِ كَذِبًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5453"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Jinn";
  dbp:text "And there were men from mankind who sought refuge in men from the jinn, so they [only] increased them in burden.";
  <http://localhost:3333/has_arabic_text> "وَأَنَّهُۥ كَانَ رِجَالٌ مِّنَ ٱلْإِنسِ يَعُوذُونَ بِرِجَالٍ مِّنَ ٱلْجِنِّ فَزَادُوهُمْ رَهَقًا";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5454"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Jinn";
  dbp:text "And they had thought, as you thought, that Allah would never send anyone [as a messenger].";
  <http://localhost:3333/has_arabic_text> "وَأَنَّهُمْ ظَنُّوا۟ كَمَا ظَنَنتُمْ أَن لَّن يَبْعَثَ ٱللَّهُ أَحَدًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5455"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Jinn";
  dbp:text "And we have sought [to reach] the heaven but found it filled with powerful guards and burning flames.";
  <http://localhost:3333/has_arabic_text> "وَأَنَّا لَمَسْنَا ٱلسَّمَآءَ فَوَجَدْنَٰهَا مُلِئَتْ حَرَسًا شَدِيدًا وَشُهُبًا";
  dbo:Surah "Al-Jinn";
  a dbr:Ayat, dbr:Ayah;
  dbo:ayah "5456"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "And we used to sit therein in positions for hearing, but whoever listens now will find a burning flame lying in wait for him.";
  a dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "وَأَنَّا كُنَّا نَقْعُدُ مِنْهَا مَقَٰعِدَ لِلسَّمْعِ ۖ فَمَن يَسْتَمِعِ ٱلْءَانَ يَجِدْ لَهُۥ شِهَابًا رَّصَدًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5457"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Jinn";
  dbp:text "And we do not know [therefore] whether evil is intended for those on earth or whether their Lord intends for them a right course.";
  <http://localhost:3333/has_arabic_text> "وَأَنَّا لَا نَدْرِىٓ أَشَرٌّ أُرِيدَ بِمَن فِى ٱلْأَرْضِ أَمْ أَرَادَ بِهِمْ رَبُّهُمْ رَشَدًا";
  a dbr:Quranic_verse;
  dbo:Surah "Al-Jinn";
  dbo:ayah "5458"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Ayat;
  dbp:text "And among us are the righteous, and among us are [others] not so; we were [of] divided ways.";
  <http://localhost:3333/has_arabic_text> "وَأَنَّا مِنَّا ٱلصَّٰلِحُونَ وَمِنَّا دُونَ ذَٰلِكَ ۖ كُنَّا طَرَآئِقَ قِدَدًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5459"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Jinn";
  dbp:text "And we have become certain that we will never cause failure to Allah upon earth, nor can we escape Him by flight.";
  <http://localhost:3333/has_arabic_text> "وَأَنَّا ظَنَنَّآ أَن لَّن نُّعْجِزَ ٱللَّهَ فِى ٱلْأَرْضِ وَلَن نُّعْجِزَهُۥ هَرَبًا",
    "وَأَنَّا لَمَّا سَمِعْنَا ٱلْهُدَىٰٓ ءَامَنَّا بِهِۦ ۖ فَمَن يُؤْمِنۢ بِرَبِّهِۦ فَلَا يَخَافُ بَخْسًا وَلَا رَهَقًا";
  dbo:ayah "5460"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Jinn";
  dbp:text "And when we heard the guidance, we believed in it. And whoever believes in his Lord will not fear deprivation or burden.";
  a dbr:Ayah, dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5461"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Jinn";
  dbp:text "And among us are Muslims [in submission to Allah], and among us are the unjust. And whoever has become Muslim - those have sought out the right course.";
  <http://localhost:3333/has_arabic_text> "وَأَنَّا مِنَّا ٱلْمُسْلِمُونَ وَمِنَّا ٱلْقَٰسِطُونَ ۖ فَمَنْ أَسْلَمَ فَأُو۟لَٰٓئِكَ تَحَرَّوْا۟ رَشَدًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5462"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Jinn";
  dbp:text "But as for the unjust, they will be, for Hell, firewood.'";
  <http://localhost:3333/has_arabic_text> "وَأَمَّا ٱلْقَٰسِطُونَ فَكَانُوا۟ لِجَهَنَّمَ حَطَبًا";
  a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:ayah "5463"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "And [Allah revealed] that if they had remained straight on the way, We would have given them abundant provision";
  dbo:Surah "Al-Jinn";
  <http://localhost:3333/has_arabic_text> "وَأَلَّوِ ٱسْتَقَٰمُوا۟ عَلَى ٱلطَّرِيقَةِ لَأَسْقَيْنَٰهُم مَّآءً غَدَقًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5464"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Jinn";
  dbp:text "So We might test them therein. And whoever turns away from the remembrance of his Lord He will put into arduous punishment.";
  <http://localhost:3333/has_arabic_text> "لِّنَفْتِنَهُمْ فِيهِ ۚ وَمَن يُعْرِضْ عَن ذِكْرِ رَبِّهِۦ يَسْلُكْهُ عَذَابًا صَعَدًا";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5465"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Jinn";
  dbp:text "And [He revealed] that the masjids are for Allah, so do not invoke with Allah anyone.";
  <http://localhost:3333/has_arabic_text> "وَأَنَّ ٱلْمَسَٰجِدَ لِلَّهِ فَلَا تَدْعُوا۟ مَعَ ٱللَّهِ أَحَدًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5466"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Jinn";
  dbp:text "And that when the Servant of Allah stood up supplicating Him, they almost became about him a compacted mass.\"";
  <http://localhost:3333/has_arabic_text> "وَأَنَّهُۥ كَانَ يَقُولُ سَفِيهُنَا عَلَى ٱللَّهِ شَطَطًا";
  a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:ayah "5467"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Jinn";
  dbp:text "Say, [O Muhammad], \"I only invoke my Lord and do not associate with Him anyone.\"";
  <http://localhost:3333/has_arabic_text> "قُلْ إِنَّمَآ أَدْعُوا۟ رَبِّى وَلَآ أُشْرِكُ بِهِۦٓ أَحَدًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5468"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Jinn";
  dbp:text "Say, \"Indeed, I do not possess for you [the power of] harm or right direction.\"";
  <http://localhost:3333/has_arabic_text> "قُلْ إِنِّى لَآ أَمْلِكُ لَكُمْ ضَرًّا وَلَا رَشَدًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5469"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Jinn";
  dbp:text "Say, \"Indeed, there will never protect me from Allah anyone [if I should disobey], nor will I find in other than Him a refuge.";
  <http://localhost:3333/has_arabic_text> "قُلْ إِنِّى لَن يُجِيرَنِى مِنَ ٱللَّهِ أَحَدٌ وَلَنْ أَجِدَ مِن دُونِهِۦ مُلْتَحَدًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5470"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Jinn";
  dbp:text "But [I have for you] only notification from Allah, and His messages.\" And whoever disobeys Allah and His Messenger - then indeed, for him is the fire of Hell; they will abide therein forever.";
  <http://localhost:3333/has_arabic_text> "إِلَّا بَلَٰغًا مِّنَ ٱللَّهِ وَرِسَٰلَٰتِهِۦ ۚ وَمَن يَعْصِ ٱللَّهَ وَرَسُولَهُۥ فَإِنَّ لَهُۥ نَارَ جَهَنَّمَ خَٰلِدِينَ فِيهَآ أَبَدًا";
  dbo:ayah "5471"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Ayah;
  dbo:Surah "Al-Jinn";
  dbp:text "[The disbelievers continue] until, when they see that which they are promised, then they will know who is weaker in helpers and less in number.";
  a dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "حَتَّىٰٓ إِذَا رَأَوْا۟ مَا يُوعَدُونَ فَسَيَعْلَمُونَ مَنْ أَضْعَفُ نَاصِرًا وَأَقَلُّ عَدَدًا";
  dbo:ayah "5472"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:Surah "Al-Jinn";
  dbp:text "Say, \"I do not know if what you are promised is near or if my Lord will grant for it a [long] period.\"";
  <http://localhost:3333/has_arabic_text> "قُلْ إِنْ أَدْرِىٓ أَقَرِيبٌ مَّا تُوعَدُونَ أَمْ يَجْعَلُ لَهُۥ رَبِّىٓ أَمَدًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5473"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Jinn";
  dbp:text "[He is] Knower of the unseen, and He does not disclose His [knowledge of the] unseen to anyone";
  <http://localhost:3333/has_arabic_text> "عَٰلِمُ ٱلْغَيْبِ فَلَا يُظْهِرُ عَلَىٰ غَيْبِهِۦٓ أَحَدًا";
  dbp:text "Except whom He has approved of messengers, and indeed, He sends before each messenger and behind him observers";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5474"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Jinn";
  <http://localhost:3333/has_arabic_text> "إِلَّا مَنِ ٱرْتَضَىٰ مِن رَّسُولٍ فَإِنَّهُۥ يَسْلُكُ مِنۢ بَيْنِ يَدَيْهِ وَمِنْ خَلْفِهِۦ رَصَدًا";
  a dbr:Ayat, dbr:Ayah;
  dbo:Surah "Al-Jinn";
  dbo:ayah "5475"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse;
  dbp:text "That he may know that they have conveyed the messages of their Lord; and He has encompassed whatever is with them and has enumerated all things in number.";
  <http://localhost:3333/has_arabic_text> "لِّيَعْلَمَ أَن قَدْ أَبْلَغُوا۟ رِسَٰلَٰتِ رَبِّهِمْ وَأَحَاطَ بِمَا لَدَيْهِمْ وَأَحْصَىٰ كُلَّ شَىْءٍ عَدَدًۢا",
    "وَأَنَّهُۥ لَمَّا قَامَ عَبْدُ ٱللَّهِ يَدْعُوهُ كَادُوا۟ يَكُونُونَ عَلَيْهِ لِبَدًا",
    "يَهْدِىٓ إِلَى ٱلرُّشْدِ فَـَٔامَنَّا بِهِۦ ۖ وَلَن نُّشْرِكَ بِرَبِّنَآ أَحَدًا" .

<http://localhost:3333/Al-Muzzammil> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Muzzammil";
  dbo:ayah "5476"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Ayat;
  dbo:Surah "Al-Muzzammil";
  dbp:text "Arise [to pray] the night, except for a little -";
  a dbr:Quranic_verse;
  dbo:ayah "5477"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "O you who wraps himself [in clothing],";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5478"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muzzammil";
  dbp:text "Half of it - or subtract from it a little";
  <http://localhost:3333/has_arabic_text> "قُمِ ٱلَّيْلَ إِلَّا قَلِيلًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5479"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muzzammil";
  dbp:text "Or add to it, and recite the Qur'an with measured recitation.";
  <http://localhost:3333/has_arabic_text> "أَوْ زِدْ عَلَيْهِ وَرَتِّلِ ٱلْقُرْءَانَ تَرْتِيلًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5480"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muzzammil";
  dbp:text "Indeed, We will cast upon you a heavy word.";
  <http://localhost:3333/has_arabic_text> "إِنَّا سَنُلْقِى عَلَيْكَ قَوْلًا ثَقِيلًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5481"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muzzammil";
  dbp:text "Indeed, the hours of the night are more effective for concurrence [of heart and tongue] and more suitable for words.";
  <http://localhost:3333/has_arabic_text> "إِنَّ نَاشِئَةَ ٱلَّيْلِ هِىَ أَشَدُّ وَطْـًٔا وَأَقْوَمُ قِيلًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5482"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muzzammil";
  dbp:text "Indeed, for you by day is prolonged occupation.";
  <http://localhost:3333/has_arabic_text> "إِنَّ لَكَ فِى ٱلنَّهَارِ سَبْحًا طَوِيلًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5483"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muzzammil";
  dbp:text "And remember the name of your Lord and devote yourself to Him with [complete] devotion.";
  <http://localhost:3333/has_arabic_text> "نِّصْفَهُۥٓ أَوِ ٱنقُصْ مِنْهُ قَلِيلًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5484"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muzzammil";
  dbp:text "[He is] the Lord of the East and the West; there is no deity except Him, so take Him as Disposer of [your] affairs.";
  <http://localhost:3333/has_arabic_text> "رَّبُّ ٱلْمَشْرِقِ وَٱلْمَغْرِبِ لَآ إِلَٰهَ إِلَّا هُوَ فَٱتَّخِذْهُ وَكِيلًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5485"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muzzammil";
  dbp:text "And be patient over what they say and avoid them with gracious avoidance.";
  <http://localhost:3333/has_arabic_text> "وَٱذْكُرِ ٱسْمَ رَبِّكَ وَتَبَتَّلْ إِلَيْهِ تَبْتِيلًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5486"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muzzammil";
  dbp:text "And leave Me with [the matter of] the deniers, those of ease [in life], and allow them respite a little.";
  <http://localhost:3333/has_arabic_text> "وَذَرْنِى وَٱلْمُكَذِّبِينَ أُو۟لِى ٱلنَّعْمَةِ وَمَهِّلْهُمْ قَلِيلًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5487"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muzzammil";
  dbp:text "Indeed, with Us [for them] are shackles and burning fire";
  <http://localhost:3333/has_arabic_text> "إِنَّ لَدَيْنَآ أَنكَالًا وَجَحِيمًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5488"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muzzammil";
  dbp:text "And food that chokes and a painful punishment -";
  <http://localhost:3333/has_arabic_text> "وَطَعَامًا ذَا غُصَّةٍ وَعَذَابًا أَلِيمًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5489"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muzzammil";
  dbp:text "On the Day the earth and the mountains will convulse and the mountains will become a heap of sand pouring down.";
  <http://localhost:3333/has_arabic_text> "وَٱصْبِرْ عَلَىٰ مَا يَقُولُونَ وَٱهْجُرْهُمْ هَجْرًا جَمِيلًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5490"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muzzammil";
  dbp:text "Indeed, We have sent to you a Messenger as a witness upon you just as We sent to Pharaoh a messenger.";
  <http://localhost:3333/has_arabic_text> "إِنَّآ أَرْسَلْنَآ إِلَيْكُمْ رَسُولًا شَٰهِدًا عَلَيْكُمْ كَمَآ أَرْسَلْنَآ إِلَىٰ فِرْعَوْنَ رَسُولًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5491"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muzzammil";
  dbp:text "But Pharaoh disobeyed the messenger, so We seized him with a ruinous seizure.";
  <http://localhost:3333/has_arabic_text> "فَعَصَىٰ فِرْعَوْنُ ٱلرَّسُولَ فَأَخَذْنَٰهُ أَخْذًا وَبِيلًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5492"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muzzammil";
  dbp:text "Then how can you fear, if you disbelieve, a Day that will make the children white- haired?";
  <http://localhost:3333/has_arabic_text> "فَكَيْفَ تَتَّقُونَ إِن كَفَرْتُمْ يَوْمًا يَجْعَلُ ٱلْوِلْدَٰنَ شِيبًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5493"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muzzammil";
  dbp:text "The heaven will break apart therefrom; ever is His promise fulfilled.";
  <http://localhost:3333/has_arabic_text> "يَوْمَ تَرْجُفُ ٱلْأَرْضُ وَٱلْجِبَالُ وَكَانَتِ ٱلْجِبَالُ كَثِيبًا مَّهِيلًا";
  dbo:Surah "Al-Muzzammil";
  a dbr:Ayah;
  dbo:ayah "5494"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse;
  dbp:text "Indeed, this is a reminder, so whoever wills may take to his Lord a way.";
  a dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "إِنَّ هَٰذِهِۦ تَذْكِرَةٌ ۖ فَمَن شَآءَ ٱتَّخَذَ إِلَىٰ رَبِّهِۦ سَبِيلًا";
  a dbr:Quranic_verse, dbr:Ayah;
  dbp:text "Indeed, your Lord knows, [O Muhammad], that you stand [in prayer] almost two thirds of the night or half of it or a third of it, and [so do] a group of those with you. And Allah determines [the extent of] the night and the day. He has known that you [Muslims] will not be able to do it and has turned to you in forgiveness, so recite what is easy [for you] of the Qur'an. He has known that there will be among you those who are ill and others traveling throughout the land seeking [something] of the bounty of Allah and others fighting for the cause of Allah. So recite what is easy from it and establish prayer and give zakah and loan Allah a goodly loan. And whatever good you put forward for yourselves - you will find it with Allah. It is better and greater in reward. And seek forgiveness of Allah. Indeed, Allah is Forgiving and Merciful.";
  dbo:Surah "Al-Muzzammil";
  dbo:ayah "5495"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "إِنَّ رَبَّكَ يَعْلَمُ أَنَّكَ تَقُومُ أَدْنَىٰ مِن ثُلُثَىِ ٱلَّيْلِ وَنِصْفَهُۥ وَثُلُثَهُۥ وَطَآئِفَةٌ مِّنَ ٱلَّذِينَ مَعَكَ ۚ وَٱللَّهُ يُقَدِّرُ ٱلَّيْلَ وَٱلنَّهَارَ ۚ عَلِمَ أَن لَّن تُحْصُوهُ فَتَابَ عَلَيْكُمْ ۖ فَٱقْرَءُوا۟ مَا تَيَسَّرَ مِنَ ٱلْقُرْءَانِ ۚ عَلِمَ أَن سَيَكُونُ مِنكُم مَّرْضَىٰ ۙ وَءَاخَرُونَ يَضْرِبُونَ فِى ٱلْأَرْضِ يَبْتَغُونَ مِن فَضْلِ ٱللَّهِ ۙ وَءَاخَرُونَ يُقَٰتِلُونَ فِى سَبِيلِ ٱللَّهِ ۖ فَٱقْرَءُوا۟ مَا تَيَسَّرَ مِنْهُ ۚ وَأَقِيمُوا۟ ٱلصَّلَوٰةَ وَءَاتُوا۟ ٱلزَّكَوٰةَ وَأَقْرِضُوا۟ ٱللَّهَ قَرْضًا حَسَنًا ۚ وَمَا تُقَدِّمُوا۟ لِأَنفُسِكُم مِّنْ خَيْرٍ تَجِدُوهُ عِندَ ٱللَّهِ هُوَ خَيْرًا وَأَعْظَمَ أَجْرًا ۚ وَٱسْتَغْفِرُوا۟ ٱللَّهَ ۖ إِنَّ ٱللَّهَ غَفُورٌ رَّحِيمٌۢ",
    "يَٰٓأَيُّهَا ٱلْمُزَّمِّلُ", "ٱلسَّمَآءُ مُنفَطِرٌۢ بِهِۦ ۚ كَانَ وَعْدُهُۥ مَفْعُولًا" .

<http://localhost:3333/Al-Muddathir> dbo:Surah "Al-Muddathir";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5496"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5497"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "Arise and warn", "O you who covers himself [with a garment],";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5498"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "And your Lord glorify";
  <http://localhost:3333/has_arabic_text> "قُمْ فَأَنذِرْ";
  dbp:text "And your clothing purify";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5499"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  <http://localhost:3333/has_arabic_text> "وَثِيَابَكَ فَطَهِّرْ";
  a dbr:Ayah;
  dbo:ayah "5500"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "And uncleanliness avoid";
  a dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Muddathir";
  <http://localhost:3333/has_arabic_text> "وَرَبَّكَ فَكَبِّرْ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5501"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "And do not confer favor to acquire more";
  <http://localhost:3333/has_arabic_text> "وَلَا تَمْنُن تَسْتَكْثِرُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5502"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "But for your Lord be patient.";
  <http://localhost:3333/has_arabic_text> "وَلِرَبِّكَ فَٱصْبِرْ";
  dbp:text "And when the trumpet is blown,";
  a dbr:Quranic_verse, dbr:Ayat;
  dbo:Surah "Al-Muddathir";
  dbo:ayah "5503"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "فَإِذَا نُقِرَ فِى ٱلنَّاقُورِ";
  dbo:ayah "5504"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:Surah "Al-Muddathir";
  dbp:text "That Day will be a difficult day";
  <http://localhost:3333/has_arabic_text> "فَذَٰلِكَ يَوْمَئِذٍ يَوْمٌ عَسِيرٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5505"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "For the disbelievers - not easy.";
  <http://localhost:3333/has_arabic_text> "عَلَى ٱلْكَٰفِرِينَ غَيْرُ يَسِيرٍ";
  dbo:Surah "Al-Muddathir";
  dbo:ayah "5506"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbp:text "Leave Me with the one I created alone";
  <http://localhost:3333/has_arabic_text> "ذَرْنِى وَمَنْ خَلَقْتُ وَحِيدًا";
  a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:ayah "5507"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "And to whom I granted extensive wealth";
  <http://localhost:3333/has_arabic_text> "وَجَعَلْتُ لَهُۥ مَالًا مَّمْدُودًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5508"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "And children present [with him]";
  <http://localhost:3333/has_arabic_text> "وَبَنِينَ شُهُودًا";
  dbo:ayah "5509"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "And spread [everything] before him, easing [his life].";
  a dbr:Ayat, dbr:Ayah, dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "وَمَهَّدتُّ لَهُۥ تَمْهِيدًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5510"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "Then he desires that I should add more.";
  <http://localhost:3333/has_arabic_text> "ثُمَّ يَطْمَعُ أَنْ أَزِيدَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5511"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "No! Indeed, he has been toward Our verses obstinate.";
  <http://localhost:3333/has_arabic_text> "كَلَّآ ۖ إِنَّهُۥ كَانَ لِءَايَٰتِنَا عَنِيدًا";
  dbp:text "I will cover him with arduous torment.";
  dbo:Surah "Al-Muddathir";
  a dbr:Ayat, dbr:Ayah, dbr:Quranic_verse;
  dbo:ayah "5512"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "سَأُرْهِقُهُۥ صَعُودًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5513"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "Indeed, he thought and deliberated.";
  <http://localhost:3333/has_arabic_text> "إِنَّهُۥ فَكَّرَ وَقَدَّرَ";
  a dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "فَقُتِلَ كَيْفَ قَدَّرَ";
  dbo:Surah "Al-Muddathir";
  dbp:text "So may he be destroyed [for] how he deliberated.";
  a dbr:Quranic_verse;
  dbo:ayah "5514"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5515"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "Then may he be destroyed [for] how he deliberated.";
  <http://localhost:3333/has_arabic_text> "ثُمَّ قُتِلَ كَيْفَ قَدَّرَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5516"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "Then he considered [again];";
  <http://localhost:3333/has_arabic_text> "ثُمَّ نَظَرَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5517"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "Then he frowned and scowled;";
  <http://localhost:3333/has_arabic_text> "ثُمَّ عَبَسَ وَبَسَرَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5518"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "Then he turned back and was arrogant";
  <http://localhost:3333/has_arabic_text> "ثُمَّ أَدْبَرَ وَٱسْتَكْبَرَ", "فَقَالَ إِنْ هَٰذَآ إِلَّا سِحْرٌ يُؤْثَرُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbp:text "And said, \"This is not but magic imitated [from others].";
  dbo:ayah "5519"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbo:ayah "5520"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:Surah "Al-Muddathir";
  dbp:text "This is not but the word of a human being.\"";
  <http://localhost:3333/has_arabic_text> "إِنْ هَٰذَآ إِلَّا قَوْلُ ٱلْبَشَرِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5521"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "I will drive him into Saqar.";
  <http://localhost:3333/has_arabic_text> "سَأُصْلِيهِ سَقَرَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5522"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "And what can make you know what is Saqar?";
  <http://localhost:3333/has_arabic_text> "وَمَآ أَدْرَىٰكَ مَا سَقَرُ";
  dbo:ayah "5523"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "It lets nothing remain and leaves nothing [unburned],";
  dbo:Surah "Al-Muddathir";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "لَا تُبْقِى وَلَا تَذَرُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5524"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "Blackening the skins.";
  <http://localhost:3333/has_arabic_text> "لَوَّاحَةٌ لِّلْبَشَرِ";
  a dbr:Ayat;
  dbo:Surah "Al-Muddathir";
  dbo:ayah "5525"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse;
  dbp:text "Over it are nineteen [angels].";
  <http://localhost:3333/has_arabic_text> "عَلَيْهَا تِسْعَةَ عَشَرَ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5526"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "And We have not made the keepers of the Fire except angels. And We have not made their number except as a trial for those who disbelieve - that those who were given the Scripture will be convinced and those who have believed will increase in faith and those who were given the Scripture and the believers will not doubt and that those in whose hearts is hypocrisy and the disbelievers will say, \"What does Allah intend by this as an example?\" Thus does Allah leave astray whom He wills and guides whom He wills. And none knows the soldiers of your Lord except Him. And mention of the Fire is not but a reminder to humanity.";
  <http://localhost:3333/has_arabic_text> "وَمَا جَعَلْنَآ أَصْحَٰبَ ٱلنَّارِ إِلَّا مَلَٰٓئِكَةً ۙ وَمَا جَعَلْنَا عِدَّتَهُمْ إِلَّا فِتْنَةً لِّلَّذِينَ كَفَرُوا۟ لِيَسْتَيْقِنَ ٱلَّذِينَ أُوتُوا۟ ٱلْكِتَٰبَ وَيَزْدَادَ ٱلَّذِينَ ءَامَنُوٓا۟ إِيمَٰنًا ۙ وَلَا يَرْتَابَ ٱلَّذِينَ أُوتُوا۟ ٱلْكِتَٰبَ وَٱلْمُؤْمِنُونَ ۙ وَلِيَقُولَ ٱلَّذِينَ فِى قُلُوبِهِم مَّرَضٌ وَٱلْكَٰفِرُونَ مَاذَآ أَرَادَ ٱللَّهُ بِهَٰذَا مَثَلًا ۚ كَذَٰلِكَ يُضِلُّ ٱللَّهُ مَن يَشَآءُ وَيَهْدِى مَن يَشَآءُ ۚ وَمَا يَعْلَمُ جُنُودَ رَبِّكَ إِلَّا هُوَ ۚ وَمَا هِىَ إِلَّا ذِكْرَىٰ لِلْبَشَرِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5527"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "No! By the moon";
  <http://localhost:3333/has_arabic_text> "كَلَّا وَٱلْقَمَرِ";
  a dbr:Quranic_verse, dbr:Ayat;
  dbo:Surah "Al-Muddathir";
  dbo:ayah "5528"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah;
  dbp:text "And [by] the night when it departs";
  <http://localhost:3333/has_arabic_text> "وَٱلرُّجْزَ فَٱهْجُرْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5529"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "And [by] the morning when it brightens,";
  <http://localhost:3333/has_arabic_text> "وَٱلصُّبْحِ إِذَآ أَسْفَرَ";
  dbp:text "Indeed, the Fire is of the greatest [afflictions]";
  dbo:Surah "Al-Muddathir";
  a dbr:Quranic_verse, dbr:Ayat, dbr:Ayah;
  dbo:ayah "5530"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "إِنَّهَا لَإِحْدَى ٱلْكُبَرِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5531"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "As a warning to humanity -";
  <http://localhost:3333/has_arabic_text> "نَذِيرًا لِّلْبَشَرِ";
  dbp:text "To whoever wills among you to proceed or stay behind.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5532"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  <http://localhost:3333/has_arabic_text> "لِمَن شَآءَ مِنكُمْ أَن يَتَقَدَّمَ أَوْ يَتَأَخَّرَ";
  dbo:Surah "Al-Muddathir";
  a dbr:Ayah;
  dbo:ayah "5533"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse;
  dbp:text "Every soul, for what it has earned, will be retained";
  <http://localhost:3333/has_arabic_text> "كُلُّ نَفْسٍۭ بِمَا كَسَبَتْ رَهِينَةٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5534"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "Except the companions of the right,";
  <http://localhost:3333/has_arabic_text> "إِلَّآ أَصْحَٰبَ ٱلْيَمِينِ";
  a dbr:Ayah, dbr:Quranic_verse;
  dbp:text "[Who will be] in gardens, questioning each other";
  dbo:ayah "5535"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat;
  dbo:Surah "Al-Muddathir";
  <http://localhost:3333/has_arabic_text> "فِى جَنَّٰتٍ يَتَسَآءَلُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5536"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "About the criminals,";
  <http://localhost:3333/has_arabic_text> "عَنِ ٱلْمُجْرِمِينَ";
  a dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Muddathir";
  dbo:ayah "5537"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah;
  dbp:text "[And asking them], \"What put you into Saqar?\"";
  <http://localhost:3333/has_arabic_text> "مَا سَلَكَكُمْ فِى سَقَرَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5538"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "They will say, \"We were not of those who prayed,";
  <http://localhost:3333/has_arabic_text> "قَالُوا۟ لَمْ نَكُ مِنَ ٱلْمُصَلِّينَ";
  a dbr:Quranic_verse;
  dbo:Surah "Al-Muddathir";
  dbo:ayah "5539"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Ayat;
  dbp:text "Nor did we used to feed the poor.";
  <http://localhost:3333/has_arabic_text> "وَلَمْ نَكُ نُطْعِمُ ٱلْمِسْكِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5540"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "And we used to enter into vain discourse with those who engaged [in it],";
  <http://localhost:3333/has_arabic_text> "وَكُنَّا نَخُوضُ مَعَ ٱلْخَآئِضِينَ";
  dbo:Surah "Al-Muddathir";
  dbo:ayah "5541"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbp:text "And we used to deny the Day of Recompense";
  <http://localhost:3333/has_arabic_text> "وَكُنَّا نُكَذِّبُ بِيَوْمِ ٱلدِّينِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5542"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "Until there came to us the certainty.\"";
  <http://localhost:3333/has_arabic_text> "حَتَّىٰٓ أَتَىٰنَا ٱلْيَقِينُ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:Surah "Al-Muddathir";
  dbo:ayah "5543"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "So there will not benefit them the intercession of [any] intercessors.";
  <http://localhost:3333/has_arabic_text> "فَمَا تَنفَعُهُمْ شَفَٰعَةُ ٱلشَّٰفِعِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5544"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "Then what is [the matter] with them that they are, from the reminder, turning away";
  <http://localhost:3333/has_arabic_text> "فَمَا لَهُمْ عَنِ ٱلتَّذْكِرَةِ مُعْرِضِينَ";
  dbo:ayah "5545"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat;
  dbo:Surah "Al-Muddathir";
  dbp:text "As if they were alarmed donkeys";
  a dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "كَأَنَّهُمْ حُمُرٌ مُّسْتَنفِرَةٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5546"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "Fleeing from a lion?";
  <http://localhost:3333/has_arabic_text> "فَرَّتْ مِن قَسْوَرَةٍۭ";
  a dbr:Ayat;
  dbo:ayah "5547"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "Rather, every person among them desires that he would be given scriptures spread about.";
  dbo:Surah "Al-Muddathir";
  a dbr:Ayah, dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "بَلْ يُرِيدُ كُلُّ ٱمْرِئٍ مِّنْهُمْ أَن يُؤْتَىٰ صُحُفًا مُّنَشَّرَةً";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5548"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "No! But they do not fear the Hereafter.";
  <http://localhost:3333/has_arabic_text> "كَلَّا ۖ بَل لَّا يَخَافُونَ ٱلْءَاخِرَةَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5549"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "No! Indeed, the Qur'an is a reminder";
  <http://localhost:3333/has_arabic_text> "كَلَّآ إِنَّهُۥ تَذْكِرَةٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5550"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "Then whoever wills will remember it.";
  <http://localhost:3333/has_arabic_text> "فَمَن شَآءَ ذَكَرَهُۥ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5551"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Muddathir";
  dbp:text "And they will not remember except that Allah wills. He is worthy of fear and adequate for [granting] forgiveness.";
  <http://localhost:3333/has_arabic_text> "وَمَا يَذْكُرُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ ۚ هُوَ أَهْلُ ٱلتَّقْوَىٰ وَأَهْلُ ٱلْمَغْفِرَةِ",
    "وَٱلَّيْلِ إِذْ أَدْبَرَ", "يَٰٓأَيُّهَا ٱلْمُدَّثِّرُ" .

<http://localhost:3333/Al-Qiyamah> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Qiyamah";
  dbo:ayah "5552"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5553"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qiyamah";
  dbp:text "And I swear by the reproaching soul [to the certainty of resurrection].",
    "I swear by the Day of Resurrection";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5554"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qiyamah";
  dbp:text "Does man think that We will not assemble his bones?";
  <http://localhost:3333/has_arabic_text> "أَيَحْسَبُ ٱلْإِنسَٰنُ أَلَّن نَّجْمَعَ عِظَامَهُۥ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5555"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qiyamah";
  dbp:text "Yes. [We are] Able [even] to proportion his fingertips.";
  <http://localhost:3333/has_arabic_text> "بَلَىٰ قَٰدِرِينَ عَلَىٰٓ أَن نُّسَوِّىَ بَنَانَهُۥ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5556"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qiyamah";
  dbp:text "But man desires to continue in sin.";
  <http://localhost:3333/has_arabic_text> "بَلْ يُرِيدُ ٱلْإِنسَٰنُ لِيَفْجُرَ أَمَامَهُۥ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5557"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qiyamah";
  dbp:text "He asks, \"When is the Day of Resurrection?\"";
  <http://localhost:3333/has_arabic_text> "لَآ أُقْسِمُ بِيَوْمِ ٱلْقِيَٰمَةِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5558"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qiyamah";
  dbp:text "So when vision is dazzled";
  <http://localhost:3333/has_arabic_text> "فَإِذَا بَرِقَ ٱلْبَصَرُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5559"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qiyamah";
  dbp:text "And the moon darkens";
  <http://localhost:3333/has_arabic_text> "وَخَسَفَ ٱلْقَمَرُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5560"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qiyamah";
  dbp:text "And the sun and the moon are joined,";
  <http://localhost:3333/has_arabic_text> "وَجُمِعَ ٱلشَّمْسُ وَٱلْقَمَرُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5561"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qiyamah";
  dbp:text "Man will say on that Day, \"Where is the [place of] escape?\"";
  <http://localhost:3333/has_arabic_text> "وَلَآ أُقْسِمُ بِٱلنَّفْسِ ٱللَّوَّامَةِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5562"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qiyamah";
  dbp:text "No! There is no refuge.";
  <http://localhost:3333/has_arabic_text> "كَلَّا لَا وَزَرَ";
  dbo:ayah "5563"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "إِلَىٰ رَبِّكَ يَوْمَئِذٍ ٱلْمُسْتَقَرُّ";
  dbo:Surah "Al-Qiyamah";
  dbp:text "To your Lord, that Day, is the [place of] permanence.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "يَسْـَٔلُ أَيَّانَ يَوْمُ ٱلْقِيَٰمَةِ";
  a dbr:Ayat;
  dbp:text "Man will be informed that Day of what he sent ahead and kept back.";
  a dbr:Quranic_verse;
  dbo:Surah "Al-Qiyamah";
  a dbr:Ayah;
  dbo:ayah "5564"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayah;
  dbp:text "Rather, man, against himself, will be a witness,";
  dbo:Surah "Al-Qiyamah";
  dbo:ayah "5565"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "بَلِ ٱلْإِنسَٰنُ عَلَىٰ نَفْسِهِۦ بَصِيرَةٌ",
    "وَلَوْ أَلْقَىٰ مَعَاذِيرَهُۥ";
  dbp:text "Even if he presents his excuses.";
  dbo:Surah "Al-Qiyamah";
  dbo:ayah "5566"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah, dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5567"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qiyamah";
  dbp:text "Move not your tongue with it, [O Muhammad], to hasten with recitation of the Qur'an.";
  <http://localhost:3333/has_arabic_text> "لَا تُحَرِّكْ بِهِۦ لِسَانَكَ لِتَعْجَلَ بِهِۦٓ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5568"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qiyamah";
  dbp:text "Indeed, upon Us is its collection [in your heart] and [to make possible] its recitation.";
  <http://localhost:3333/has_arabic_text> "إِنَّ عَلَيْنَا جَمْعَهُۥ وَقُرْءَانَهُۥ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5569"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qiyamah";
  dbp:text "So when We have recited it [through Gabriel], then follow its recitation.";
  <http://localhost:3333/has_arabic_text> "فَإِذَا قَرَأْنَٰهُ فَٱتَّبِعْ قُرْءَانَهُۥ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5570"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qiyamah";
  dbp:text "Then upon Us is its clarification [to you].";
  <http://localhost:3333/has_arabic_text> "ثُمَّ إِنَّ عَلَيْنَا بَيَانَهُۥ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5571"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qiyamah";
  dbp:text "No! But you love the immediate";
  <http://localhost:3333/has_arabic_text> "كَلَّا بَلْ تُحِبُّونَ ٱلْعَاجِلَةَ", "وَتَذَرُونَ ٱلْءَاخِرَةَ";
  a dbr:Ayah, dbr:Ayat;
  dbo:Surah "Al-Qiyamah";
  a dbr:Quranic_verse;
  dbo:ayah "5572"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "And leave the Hereafter.", "[Some] faces, that Day, will be radiant,";
  dbo:Surah "Al-Qiyamah";
  dbo:ayah "5573"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Ayah, dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "وُجُوهٌ يَوْمَئِذٍ نَّاضِرَةٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5574"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qiyamah";
  dbp:text "Looking at their Lord.";
  <http://localhost:3333/has_arabic_text> "إِلَىٰ رَبِّهَا نَاظِرَةٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5575"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qiyamah";
  dbp:text "And [some] faces, that Day, will be contorted,";
  <http://localhost:3333/has_arabic_text> "وَوُجُوهٌ يَوْمَئِذٍۭ بَاسِرَةٌ", "تَظُنُّ أَن يُفْعَلَ بِهَا فَاقِرَةٌ";
  a dbr:Ayat;
  dbo:Surah "Al-Qiyamah";
  dbo:ayah "5576"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah;
  dbp:text "Expecting that there will be done to them [something] backbreaking.";
  a dbr:Quranic_verse;
  dbo:Surah "Al-Qiyamah";
  a dbr:Ayah;
  dbo:ayah "5577"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse;
  dbp:text "No! When the soul has reached the collar bones";
  <http://localhost:3333/has_arabic_text> "كَلَّآ إِذَا بَلَغَتِ ٱلتَّرَاقِىَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5578"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qiyamah";
  dbp:text "And it is said, \"Who will cure [him]?\"";
  <http://localhost:3333/has_arabic_text> "وَقِيلَ مَنْ ۜ رَاقٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5579"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qiyamah";
  dbp:text "And the dying one is certain that it is the [time of] separation";
  <http://localhost:3333/has_arabic_text> "وَظَنَّ أَنَّهُ ٱلْفِرَاقُ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5580"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qiyamah";
  dbp:text "And the leg is wound about the leg,";
  <http://localhost:3333/has_arabic_text> "وَٱلْتَفَّتِ ٱلسَّاقُ بِٱلسَّاقِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5581"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qiyamah";
  dbp:text "To your Lord, that Day, will be the procession.";
  <http://localhost:3333/has_arabic_text> "إِلَىٰ رَبِّكَ يَوْمَئِذٍ ٱلْمَسَاقُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5582"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qiyamah";
  dbp:text "And the disbeliever had not believed, nor had he prayed.";
  <http://localhost:3333/has_arabic_text> "فَلَا صَدَّقَ وَلَا صَلَّىٰ", "وَلَٰكِن كَذَّبَ وَتَوَلَّىٰ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:Surah "Al-Qiyamah";
  dbo:ayah "5583"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "But [instead], he denied and turned away.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5584"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qiyamah";
  dbp:text "And then he went to his people, swaggering [in pride].";
  <http://localhost:3333/has_arabic_text> "ثُمَّ ذَهَبَ إِلَىٰٓ أَهْلِهِۦ يَتَمَطَّىٰٓ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5585"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qiyamah";
  dbp:text "Woe to you, and woe!";
  <http://localhost:3333/has_arabic_text> "أَوْلَىٰ لَكَ فَأَوْلَىٰ";
  a dbr:Ayah;
  dbo:ayah "5586"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat;
  dbo:Surah "Al-Qiyamah";
  dbp:text "Then woe to you, and woe!";
  <http://localhost:3333/has_arabic_text> "ثُمَّ أَوْلَىٰ لَكَ فَأَوْلَىٰٓ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5587"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qiyamah";
  dbp:text "Does man think that he will be left neglected?";
  <http://localhost:3333/has_arabic_text> "أَيَحْسَبُ ٱلْإِنسَٰنُ أَن يُتْرَكَ سُدًى";
  dbo:ayah "5588"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "Had he not been a sperm from semen emitted?";
  a dbr:Ayat, dbr:Ayah, dbr:Quranic_verse;
  dbo:Surah "Al-Qiyamah";
  <http://localhost:3333/has_arabic_text> "أَلَمْ يَكُ نُطْفَةً مِّن مَّنِىٍّ يُمْنَىٰ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5589"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qiyamah";
  dbp:text "Then he was a clinging clot, and [Allah] created [his form] and proportioned [him]";
  <http://localhost:3333/has_arabic_text> "ثُمَّ كَانَ عَلَقَةً فَخَلَقَ فَسَوَّىٰ";
  dbo:ayah "5590"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse;
  dbo:Surah "Al-Qiyamah";
  dbp:text "And made of him two mates, the male and the female.";
  a dbr:Ayat, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "فَجَعَلَ مِنْهُ ٱلزَّوْجَيْنِ ٱلذَّكَرَ وَٱلْأُنثَىٰٓ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5591"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qiyamah";
  dbp:text "Is not that [Creator] Able to give life to the dead?";
  <http://localhost:3333/has_arabic_text> "أَلَيْسَ ذَٰلِكَ بِقَٰدِرٍ عَلَىٰٓ أَن يُحْۦِىَ ٱلْمَوْتَىٰ",
    "يَقُولُ ٱلْإِنسَٰنُ يَوْمَئِذٍ أَيْنَ ٱلْمَفَرُّ", "يُنَبَّؤُا۟ ٱلْإِنسَٰنُ يَوْمَئِذٍۭ بِمَا قَدَّمَ وَأَخَّرَ" .

<http://localhost:3333/Al-Insan> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Insan";
  dbo:ayah "5592"^^<http://www.w3.org/2001/XMLSchema#int>, "5593"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Insan";
  dbp:text "Has there [not] come upon man a period of time when he was not a thing [even] mentioned?",
    "Indeed, We created man from a sperm-drop mixture that We may try him; and We made him hearing and seeing.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5594"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Insan";
  dbp:text "Indeed, We guided him to the way, be he grateful or be he ungrateful.";
  <http://localhost:3333/has_arabic_text> "إِنَّا خَلَقْنَا ٱلْإِنسَٰنَ مِن نُّطْفَةٍ أَمْشَاجٍ نَّبْتَلِيهِ فَجَعَلْنَٰهُ سَمِيعًۢا بَصِيرًا",
    "إِنَّا هَدَيْنَٰهُ ٱلسَّبِيلَ إِمَّا شَاكِرًا وَإِمَّا كَفُورًا";
  dbp:text "Indeed, We have prepared for the disbelievers chains and shackles and a blaze.";
  a dbr:Ayah, dbr:Quranic_verse;
  dbo:ayah "5595"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Insan";
  a dbr:Ayat, dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5596"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Insan";
  dbp:text "Indeed, the righteous will drink from a cup [of wine] whose mixture is of Kafur,";
  <http://localhost:3333/has_arabic_text> "إِنَّ ٱلْأَبْرَارَ يَشْرَبُونَ مِن كَأْسٍ كَانَ مِزَاجُهَا كَافُورًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5597"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Insan";
  dbp:text "A spring of which the [righteous] servants of Allah will drink; they will make it gush forth in force [and abundance].";
  <http://localhost:3333/has_arabic_text> "إِنَّآ أَعْتَدْنَا لِلْكَٰفِرِينَ سَلَٰسِلَا۟ وَأَغْلَٰلًا وَسَعِيرًا";
  dbo:ayah "5598"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:Surah "Al-Insan";
  dbp:text "They [are those who] fulfill [their] vows and fear a Day whose evil will be widespread.";
  <http://localhost:3333/has_arabic_text> "عَيْنًا يَشْرَبُ بِهَا عِبَادُ ٱللَّهِ يُفَجِّرُونَهَا تَفْجِيرًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5599"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Insan";
  dbp:text "And they give food in spite of love for it to the needy, the orphan, and the captive,";
  <http://localhost:3333/has_arabic_text> "هَلْ أَتَىٰ عَلَى ٱلْإِنسَٰنِ حِينٌ مِّنَ ٱلدَّهْرِ لَمْ يَكُن شَيْـًٔا مَّذْكُورًا";
  a dbr:Ayat, dbr:Quranic_verse;
  dbp:text "[Saying], \"We feed you only for the countenance of Allah. We wish not from you reward or gratitude.";
  a dbr:Ayah;
  dbo:ayah "5600"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Insan";
  <http://localhost:3333/has_arabic_text> "إِنَّمَا نُطْعِمُكُمْ لِوَجْهِ ٱللَّهِ لَا نُرِيدُ مِنكُمْ جَزَآءً وَلَا شُكُورًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5601"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Insan";
  dbp:text "Indeed, We fear from our Lord a Day austere and distressful.\"";
  <http://localhost:3333/has_arabic_text> "إِنَّا نَخَافُ مِن رَّبِّنَا يَوْمًا عَبُوسًا قَمْطَرِيرًا";
  dbp:text "So Allah will protect them from the evil of that Day and give them radiance and happiness";
  dbo:Surah "Al-Insan";
  a dbr:Ayat, dbr:Quranic_verse;
  dbo:ayah "5602"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "فَوَقَىٰهُمُ ٱللَّهُ شَرَّ ذَٰلِكَ ٱلْيَوْمِ وَلَقَّىٰهُمْ نَضْرَةً وَسُرُورًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5603"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Insan";
  dbp:text "And will reward them for what they patiently endured [with] a garden [in Paradise] and silk [garments].";
  <http://localhost:3333/has_arabic_text> "وَجَزَىٰهُم بِمَا صَبَرُوا۟ جَنَّةً وَحَرِيرًا";
  a dbr:Quranic_verse, dbr:Ayat, dbr:Ayah;
  dbo:ayah "5604"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Insan";
  dbp:text "[They will be] reclining therein on adorned couches. They will not see therein any [burning] sun or [freezing] cold.";
  <http://localhost:3333/has_arabic_text> "مُّتَّكِـِٔينَ فِيهَا عَلَى ٱلْأَرَآئِكِ ۖ لَا يَرَوْنَ فِيهَا شَمْسًا وَلَا زَمْهَرِيرًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5605"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Insan";
  dbp:text "And near above them are its shades, and its [fruit] to be picked will be lowered in compliance.";
  <http://localhost:3333/has_arabic_text> "وَدَانِيَةً عَلَيْهِمْ ظِلَٰلُهَا وَذُلِّلَتْ قُطُوفُهَا تَذْلِيلًا";
  dbo:ayah "5606"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat, dbr:Ayah;
  dbo:Surah "Al-Insan";
  dbp:text "And there will be circulated among them vessels of silver and cups having been [created] clear [as glass],";
  <http://localhost:3333/has_arabic_text> "وَيُطَافُ عَلَيْهِم بِـَٔانِيَةٍ مِّن فِضَّةٍ وَأَكْوَابٍ كَانَتْ قَوَارِيرَا۠";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5607"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Insan";
  dbp:text "Clear glasses [made] from silver of which they have determined the measure.";
  <http://localhost:3333/has_arabic_text> "قَوَارِيرَا۟ مِن فِضَّةٍ قَدَّرُوهَا تَقْدِيرًا";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5608"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Insan";
  dbp:text "And they will be given to drink a cup [of wine] whose mixture is of ginger";
  <http://localhost:3333/has_arabic_text> "وَيُسْقَوْنَ فِيهَا كَأْسًا كَانَ مِزَاجُهَا زَنجَبِيلًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5609"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Insan";
  dbp:text "[From] a fountain within Paradise named Salsabeel.";
  <http://localhost:3333/has_arabic_text> "عَيْنًا فِيهَا تُسَمَّىٰ سَلْسَبِيلًا";
  a dbr:Quranic_verse, dbr:Ayat;
  dbo:Surah "Al-Insan";
  dbo:ayah "5610"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah;
  dbp:text "There will circulate among them young boys made eternal. When you see them, you would think them [as beautiful as] scattered pearls.";
  <http://localhost:3333/has_arabic_text> "وَيَطُوفُ عَلَيْهِمْ وِلْدَٰنٌ مُّخَلَّدُونَ إِذَا رَأَيْتَهُمْ حَسِبْتَهُمْ لُؤْلُؤًا مَّنثُورًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5611"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Insan";
  dbp:text "And when you look there [in Paradise], you will see pleasure and great dominion.";
  <http://localhost:3333/has_arabic_text> "وَإِذَا رَأَيْتَ ثَمَّ رَأَيْتَ نَعِيمًا وَمُلْكًا كَبِيرًا";
  a dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5612"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Insan";
  a dbr:Ayat;
  dbp:text "Upon the inhabitants will be green garments of fine silk and brocade. And they will be adorned with bracelets of silver, and their Lord will give them a purifying drink.";
  <http://localhost:3333/has_arabic_text> "عَٰلِيَهُمْ ثِيَابُ سُندُسٍ خُضْرٌ وَإِسْتَبْرَقٌ ۖ وَحُلُّوٓا۟ أَسَاوِرَ مِن فِضَّةٍ وَسَقَىٰهُمْ رَبُّهُمْ شَرَابًا طَهُورًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5613"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Insan";
  dbp:text "[And it will be said], \"Indeed, this is for you a reward, and your effort has been appreciated.\"";
  <http://localhost:3333/has_arabic_text> "إِنَّ هَٰذَا كَانَ لَكُمْ جَزَآءً وَكَانَ سَعْيُكُم مَّشْكُورًا";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5614"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Insan";
  dbp:text "Indeed, it is We who have sent down to you, [O Muhammad], the Qur'an progressively.";
  <http://localhost:3333/has_arabic_text> "إِنَّا نَحْنُ نَزَّلْنَا عَلَيْكَ ٱلْقُرْءَانَ تَنزِيلًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5615"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Insan";
  dbp:text "So be patient for the decision of your Lord and do not obey from among them a sinner or ungrateful [disbeliever].";
  <http://localhost:3333/has_arabic_text> "فَٱصْبِرْ لِحُكْمِ رَبِّكَ وَلَا تُطِعْ مِنْهُمْ ءَاثِمًا أَوْ كَفُورًا";
  dbo:ayah "5616"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Ayah;
  dbp:text "And mention the name of your Lord [in prayer] morning and evening";
  a dbr:Quranic_verse;
  dbo:Surah "Al-Insan";
  <http://localhost:3333/has_arabic_text> "وَيُطْعِمُونَ ٱلطَّعَامَ عَلَىٰ حُبِّهِۦ مِسْكِينًا وَيَتِيمًا وَأَسِيرًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5617"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Insan";
  dbp:text "And during the night prostrate to Him and exalt Him a long [part of the] night.";
  <http://localhost:3333/has_arabic_text> "وَمِنَ ٱلَّيْلِ فَٱسْجُدْ لَهُۥ وَسَبِّحْهُ لَيْلًا طَوِيلًا";
  dbo:ayah "5618"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:Surah "Al-Insan";
  dbp:text "Indeed, these [disbelievers] love the immediate and leave behind them a grave Day.";
  <http://localhost:3333/has_arabic_text> "إِنَّ هَٰٓؤُلَآءِ يُحِبُّونَ ٱلْعَاجِلَةَ وَيَذَرُونَ وَرَآءَهُمْ يَوْمًا ثَقِيلًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5619"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Insan";
  dbp:text "We have created them and strengthened their forms, and when We will, We can change their likenesses with [complete] alteration.";
  <http://localhost:3333/has_arabic_text> "نَّحْنُ خَلَقْنَٰهُمْ وَشَدَدْنَآ أَسْرَهُمْ ۖ وَإِذَا شِئْنَا بَدَّلْنَآ أَمْثَٰلَهُمْ تَبْدِيلًا";
  a dbr:Ayah;
  dbo:ayah "5620"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Insan";
  dbp:text "Indeed, this is a reminder, so he who wills may take to his Lord a way.";
  <http://localhost:3333/has_arabic_text> "إِنَّ هَٰذِهِۦ تَذْكِرَةٌ ۖ فَمَن شَآءَ ٱتَّخَذَ إِلَىٰ رَبِّهِۦ سَبِيلًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5621"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Insan";
  dbp:text "And you do not will except that Allah wills. Indeed, Allah is ever Knowing and Wise.";
  <http://localhost:3333/has_arabic_text> "وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ ۚ إِنَّ ٱللَّهَ كَانَ عَلِيمًا حَكِيمًا";
  dbo:Surah "Al-Insan";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5622"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "He admits whom He wills into His mercy; but the wrongdoers - He has prepared for them a painful punishment.";
  <http://localhost:3333/has_arabic_text> "وَٱذْكُرِ ٱسْمَ رَبِّكَ بُكْرَةً وَأَصِيلًا",
    "يُدْخِلُ مَن يَشَآءُ فِى رَحْمَتِهِۦ ۚ وَٱلظَّٰلِمِينَ أَعَدَّ لَهُمْ عَذَابًا أَلِيمًۢا",
    "يُوفُونَ بِٱلنَّذْرِ وَيَخَافُونَ يَوْمًا كَانَ شَرُّهُۥ مُسْتَطِيرًا" .

<http://localhost:3333/Al-Mursalat> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Mursalat";
  dbo:ayah "5623"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5624"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "And the winds that blow violently", "By those [winds] sent forth in gusts";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5625"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "And [by] the winds that spread [clouds]";
  <http://localhost:3333/has_arabic_text> "فَٱلْعَٰصِفَٰتِ عَصْفًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5626"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "And those [angels] who bring criterion";
  <http://localhost:3333/has_arabic_text> "فَٱلْفَٰرِقَٰتِ فَرْقًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5627"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "And those [angels] who deliver a message";
  <http://localhost:3333/has_arabic_text> "فَٱلْمُلْقِيَٰتِ ذِكْرًا";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5628"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "As justification or warning,";
  <http://localhost:3333/has_arabic_text> "عُذْرًا أَوْ نُذْرًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5629"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "Indeed, what you are promised is to occur.";
  <http://localhost:3333/has_arabic_text> "إِنَّمَا تُوعَدُونَ لَوَٰقِعٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5630"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "So when the stars are obliterated";
  <http://localhost:3333/has_arabic_text> "فَإِذَا ٱلنُّجُومُ طُمِسَتْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5631"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "And when the heaven is opened";
  <http://localhost:3333/has_arabic_text> "وَإِذَا ٱلسَّمَآءُ فُرِجَتْ";
  a dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5632"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat;
  dbo:Surah "Al-Mursalat";
  dbp:text "And when the mountains are blown away";
  <http://localhost:3333/has_arabic_text> "وَإِذَا ٱلْجِبَالُ نُسِفَتْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5633"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "And when the messengers' time has come...";
  <http://localhost:3333/has_arabic_text> "وَإِذَا ٱلرُّسُلُ أُقِّتَتْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5634"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "For what Day was it postponed?";
  <http://localhost:3333/has_arabic_text> "لِأَىِّ يَوْمٍ أُجِّلَتْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5635"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "For the Day of Judgement.";
  <http://localhost:3333/has_arabic_text> "لِيَوْمِ ٱلْفَصْلِ";
  dbo:ayah "5636"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat;
  dbo:Surah "Al-Mursalat";
  a dbr:Ayah;
  dbp:text "And what can make you know what is the Day of Judgement?";
  <http://localhost:3333/has_arabic_text> "وَمَآ أَدْرَىٰكَ مَا يَوْمُ ٱلْفَصْلِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5637"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "Woe, that Day, to the deniers.";
  <http://localhost:3333/has_arabic_text> "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5638"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "Did We not destroy the former peoples?";
  <http://localhost:3333/has_arabic_text> "أَلَمْ نُهْلِكِ ٱلْأَوَّلِينَ";
  dbp:text "Then We will follow them with the later ones.";
  a dbr:Ayah;
  dbo:Surah "Al-Mursalat";
  dbo:ayah "5639"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "ثُمَّ نُتْبِعُهُمُ ٱلْءَاخِرِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5640"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "Thus do We deal with the criminals.";
  <http://localhost:3333/has_arabic_text> "كَذَٰلِكَ نَفْعَلُ بِٱلْمُجْرِمِينَ";
  dbo:ayah "5641"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse;
  dbo:Surah "Al-Mursalat";
  dbp:text "Woe, that Day, to the deniers.";
  a dbr:Ayat, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5642"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "Did We not create you from a liquid disdained?";
  <http://localhost:3333/has_arabic_text> "أَلَمْ نَخْلُقكُّم مِّن مَّآءٍ مَّهِينٍ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:Surah "Al-Mursalat";
  dbo:ayah "5643"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "And We placed it in a firm lodging";
  <http://localhost:3333/has_arabic_text> "فَجَعَلْنَٰهُ فِى قَرَارٍ مَّكِينٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5644"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "For a known extent.";
  <http://localhost:3333/has_arabic_text> "إِلَىٰ قَدَرٍ مَّعْلُومٍ";
  dbo:ayah "5645"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat, dbr:Ayah;
  dbo:Surah "Al-Mursalat";
  dbp:text "And We determined [it], and excellent [are We] to determine.";
  <http://localhost:3333/has_arabic_text> "فَقَدَرْنَا فَنِعْمَ ٱلْقَٰدِرُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5646"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "Woe, that Day, to the deniers.";
  <http://localhost:3333/has_arabic_text> "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5647"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "Have We not made the earth a container";
  dbo:Surah "Al-Mursalat";
  <http://localhost:3333/has_arabic_text> "أَلَمْ نَجْعَلِ ٱلْأَرْضَ كِفَاتًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5648"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "Of the living and the dead?";
  <http://localhost:3333/has_arabic_text> "أَحْيَآءً وَأَمْوَٰتًا";
  dbp:text "And We placed therein lofty, firmly set mountains and have given you to drink sweet water.";
  a dbr:Ayat;
  dbo:Surah "Al-Mursalat";
  dbo:ayah "5649"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "وَجَعَلْنَا فِيهَا رَوَٰسِىَ شَٰمِخَٰتٍ وَأَسْقَيْنَٰكُم مَّآءً فُرَاتًا";
  a dbr:Quranic_verse;
  dbo:Surah "Al-Mursalat";
  a dbr:Ayat, dbr:Ayah;
  dbo:ayah "5650"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "Woe, that Day, to the deniers.";
  <http://localhost:3333/has_arabic_text> "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5651"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "[They will be told], \"Proceed to that which you used to deny.";
  <http://localhost:3333/has_arabic_text> "وَٱلنَّٰشِرَٰتِ نَشْرًا";
  dbo:Surah "Al-Mursalat";
  dbo:ayah "5652"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbp:text "Proceed to a shadow [of smoke] having three columns";
  <http://localhost:3333/has_arabic_text> "وَٱلْمُرْسَلَٰتِ عُرْفًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5653"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "[But having] no cool shade and availing not against the flame.\"";
  <http://localhost:3333/has_arabic_text> "لَّا ظَلِيلٍ وَلَا يُغْنِى مِنَ ٱللَّهَبِ";
  a dbr:Ayat, dbr:Ayah, dbr:Quranic_verse;
  dbo:ayah "5654"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "Indeed, it throws sparks [as huge] as a fortress,";
  <http://localhost:3333/has_arabic_text> "إِنَّهَا تَرْمِى بِشَرَرٍ كَٱلْقَصْرِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5655"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "As if they were yellowish [black] camels.";
  <http://localhost:3333/has_arabic_text> "كَأَنَّهُۥ جِمَٰلَتٌ صُفْرٌ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbp:text "Woe, that Day, to the deniers.";
  dbo:Surah "Al-Mursalat";
  dbo:ayah "5656"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5657"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "This is a Day they will not speak,";
  <http://localhost:3333/has_arabic_text> "هَٰذَا يَوْمُ لَا يَنطِقُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5658"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "Nor will it be permitted for them to make an excuse.";
  <http://localhost:3333/has_arabic_text> "وَلَا يُؤْذَنُ لَهُمْ فَيَعْتَذِرُونَ";
  dbo:Surah "Al-Mursalat";
  a dbr:Ayah;
  dbo:ayah "5659"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse;
  dbp:text "Woe, that Day, to the deniers.";
  <http://localhost:3333/has_arabic_text> "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5660"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "This is the Day of Judgement; We will have assembled you and the former peoples.";
  <http://localhost:3333/has_arabic_text> "هَٰذَا يَوْمُ ٱلْفَصْلِ ۖ جَمَعْنَٰكُمْ وَٱلْأَوَّلِينَ";
  a dbr:Quranic_verse;
  dbo:ayah "5661"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah;
  dbp:text "So if you have a plan, then plan against Me.";
  a dbr:Ayat;
  dbo:Surah "Al-Mursalat";
  <http://localhost:3333/has_arabic_text> "فَإِن كَانَ لَكُمْ كَيْدٌ فَكِيدُونِ";
  a dbr:Ayat, dbr:Ayah, dbr:Quranic_verse;
  dbo:ayah "5662"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "Woe, that Day, to the deniers.";
  <http://localhost:3333/has_arabic_text> "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5663"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "Indeed, the righteous will be among shades and springs";
  <http://localhost:3333/has_arabic_text> "إِنَّ ٱلْمُتَّقِينَ فِى ظِلَٰلٍ وَعُيُونٍ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5664"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "And fruits from whatever they desire,";
  <http://localhost:3333/has_arabic_text> "وَفَوَٰكِهَ مِمَّا يَشْتَهُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5665"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "[Being told], \"Eat and drink in satisfaction for what you used to do.\"";
  <http://localhost:3333/has_arabic_text> "كُلُوا۟ وَٱشْرَبُوا۟ هَنِيٓـًٔۢا بِمَا كُنتُمْ تَعْمَلُونَ",
    "إِنَّا كَذَٰلِكَ نَجْزِى ٱلْمُحْسِنِينَ";
  a dbr:Ayat;
  dbo:ayah "5666"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "Indeed, We thus reward the doers of good.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5667"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "Woe, that Day, to the deniers.";
  <http://localhost:3333/has_arabic_text> "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5668"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "[O disbelievers], eat and enjoy yourselves a little; indeed, you are criminals.";
  <http://localhost:3333/has_arabic_text> "كُلُوا۟ وَتَمَتَّعُوا۟ قَلِيلًا إِنَّكُم مُّجْرِمُونَ",
    "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ";
  dbo:Surah "Al-Mursalat";
  a dbr:Ayah;
  dbo:ayah "5669"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "Woe, that Day, to the deniers.";
  a dbr:Quranic_verse, dbr:Ayat, dbr:Quranic_verse, dbr:Ayah, dbr:Ayat;
  dbo:ayah "5670"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "And when it is said to them, \"Bow [in prayer],\" they do not bow.";
  <http://localhost:3333/has_arabic_text> "وَإِذَا قِيلَ لَهُمُ ٱرْكَعُوا۟ لَا يَرْكَعُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5671"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mursalat";
  dbp:text "Woe, that Day, to the deniers.";
  <http://localhost:3333/has_arabic_text> "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ";
  a dbr:Quranic_verse;
  dbo:ayah "5672"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Ayah;
  dbo:Surah "Al-Mursalat";
  dbp:text "Then in what statement after the Qur'an will they believe?";
  <http://localhost:3333/has_arabic_text> "فَبِأَىِّ حَدِيثٍۭ بَعْدَهُۥ يُؤْمِنُونَ",
    "ٱنطَلِقُوٓا۟ إِلَىٰ ظِلٍّ ذِى ثَلَٰثِ شُعَبٍ", "ٱنطَلِقُوٓا۟ إِلَىٰ مَا كُنتُم بِهِۦ تُكَذِّبُونَ" .

<http://localhost:3333/An-Naba'> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "An-Naba'";
  dbo:ayah "5673"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5674"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "About the great news -", "About what are they asking one another?";
  a dbr:Ayat;
  dbp:text "That over which they are in disagreement.";
  dbo:Surah "An-Naba'";
  a dbr:Ayah;
  dbo:ayah "5675"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "عَمَّ يَتَسَآءَلُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5676"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "No! They are going to know.";
  <http://localhost:3333/has_arabic_text> "عَنِ ٱلنَّبَإِ ٱلْعَظِيمِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5677"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "Then, no! They are going to know.";
  <http://localhost:3333/has_arabic_text> "ثُمَّ كَلَّا سَيَعْلَمُونَ";
  dbo:ayah "5678"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "Have We not made the earth a resting place?";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:Surah "An-Naba'";
  <http://localhost:3333/has_arabic_text> "أَلَمْ نَجْعَلِ ٱلْأَرْضَ مِهَٰدًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5679"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "And the mountains as stakes?";
  <http://localhost:3333/has_arabic_text> "كَلَّا سَيَعْلَمُونَ";
  a dbr:Ayat, dbr:Ayah, dbr:Quranic_verse;
  dbo:ayah "5680"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "And We created you in pairs";
  <http://localhost:3333/has_arabic_text> "وَخَلَقْنَٰكُمْ أَزْوَٰجًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5681"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "And made your sleep [a means for] rest";
  <http://localhost:3333/has_arabic_text> "وَجَعَلْنَا نَوْمَكُمْ سُبَاتًا";
  a dbr:Quranic_verse, dbr:Ayat, dbr:Ayah;
  dbo:ayah "5682"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "And made the night as clothing";
  <http://localhost:3333/has_arabic_text> "وَجَعَلْنَا ٱلَّيْلَ لِبَاسًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5683"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "And made the day for livelihood";
  <http://localhost:3333/has_arabic_text> "وَجَعَلْنَا ٱلنَّهَارَ مَعَاشًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5684"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "And constructed above you seven strong [heavens]";
  <http://localhost:3333/has_arabic_text> "وَبَنَيْنَا فَوْقَكُمْ سَبْعًا شِدَادًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5685"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "And made [therein] a burning lamp";
  <http://localhost:3333/has_arabic_text> "وَجَعَلْنَا سِرَاجًا وَهَّاجًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5686"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "And sent down, from the rain clouds, pouring water";
  <http://localhost:3333/has_arabic_text> "وَأَنزَلْنَا مِنَ ٱلْمُعْصِرَٰتِ مَآءً ثَجَّاجًا";
  dbp:text "That We may bring forth thereby grain and vegetation";
  a dbr:Ayat, dbr:Ayah;
  dbo:Surah "An-Naba'";
  a dbr:Quranic_verse;
  dbo:ayah "5687"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "لِّنُخْرِجَ بِهِۦ حَبًّا وَنَبَاتًا";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5688"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "And gardens of entwined growth.";
  <http://localhost:3333/has_arabic_text> "وَجَنَّٰتٍ أَلْفَافًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5689"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "Indeed, the Day of Judgement is an appointed time -";
  <http://localhost:3333/has_arabic_text> "إِنَّ يَوْمَ ٱلْفَصْلِ كَانَ مِيقَٰتًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5690"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "The Day the Horn is blown and you will come forth in multitudes";
  <http://localhost:3333/has_arabic_text> "وَٱلْجِبَالَ أَوْتَادًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5691"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "And the heaven is opened and will become gateways";
  <http://localhost:3333/has_arabic_text> "وَفُتِحَتِ ٱلسَّمَآءُ فَكَانَتْ أَبْوَٰبًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5692"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "And the mountains are removed and will be [but] a mirage.";
  <http://localhost:3333/has_arabic_text> "وَسُيِّرَتِ ٱلْجِبَالُ فَكَانَتْ سَرَابًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5693"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "Indeed, Hell has been lying in wait";
  <http://localhost:3333/has_arabic_text> "إِنَّ جَهَنَّمَ كَانَتْ مِرْصَادًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5694"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "For the transgressors, a place of return,";
  <http://localhost:3333/has_arabic_text> "لِّلطَّٰغِينَ مَـَٔابًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5695"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "In which they will remain for ages [unending].";
  <http://localhost:3333/has_arabic_text> "لَّٰبِثِينَ فِيهَآ أَحْقَابًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5696"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "They will not taste therein [any] coolness or drink";
  <http://localhost:3333/has_arabic_text> "لَّا يَذُوقُونَ فِيهَا بَرْدًا وَلَا شَرَابًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5697"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "Except scalding water and [foul] purulence -";
  <http://localhost:3333/has_arabic_text> "إِلَّا حَمِيمًا وَغَسَّاقًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5698"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "An appropriate recompense.";
  <http://localhost:3333/has_arabic_text> "جَزَآءً وِفَاقًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5699"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "Indeed, they were not expecting an account";
  <http://localhost:3333/has_arabic_text> "إِنَّهُمْ كَانُوا۟ لَا يَرْجُونَ حِسَابًا";
  a dbr:Quranic_verse, dbr:Ayah, dbr:Ayat;
  dbo:ayah "5700"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "And denied Our verses with [emphatic] denial.";
  <http://localhost:3333/has_arabic_text> "وَكَذَّبُوا۟ بِـَٔايَٰتِنَا كِذَّابًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5701"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "But all things We have enumerated in writing.";
  <http://localhost:3333/has_arabic_text> "وَكُلَّ شَىْءٍ أَحْصَيْنَٰهُ كِتَٰبًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5702"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "\"So taste [the penalty], and never will We increase you except in torment.\"";
  <http://localhost:3333/has_arabic_text> "فَذُوقُوا۟ فَلَن نَّزِيدَكُمْ إِلَّا عَذَابًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5703"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "Indeed, for the righteous is attainment -";
  <http://localhost:3333/has_arabic_text> "إِنَّ لِلْمُتَّقِينَ مَفَازًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5704"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "Gardens and grapevines";
  <http://localhost:3333/has_arabic_text> "حَدَآئِقَ وَأَعْنَٰبًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5705"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "And full-breasted [companions] of equal age";
  <http://localhost:3333/has_arabic_text> "وَكَوَاعِبَ أَتْرَابًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5706"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "And a full cup.";
  <http://localhost:3333/has_arabic_text> "وَكَأْسًا دِهَاقًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5707"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "No ill speech will they hear therein or any falsehood -";
  <http://localhost:3333/has_arabic_text> "لَّا يَسْمَعُونَ فِيهَا لَغْوًا وَلَا كِذَّٰبًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5708"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "[As] reward from your Lord, [a generous] gift [made due by] account,";
  <http://localhost:3333/has_arabic_text> "جَزَآءً مِّن رَّبِّكَ عَطَآءً حِسَابًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5709"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "[From] the Lord of the heavens and the earth and whatever is between them, the Most Merciful. They possess not from Him [authority for] speech.";
  <http://localhost:3333/has_arabic_text> "رَّبِّ ٱلسَّمَٰوَٰتِ وَٱلْأَرْضِ وَمَا بَيْنَهُمَا ٱلرَّحْمَٰنِ ۖ لَا يَمْلِكُونَ مِنْهُ خِطَابًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5710"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "The Day that the Spirit and the angels will stand in rows, they will not speak except for one whom the Most Merciful permits, and he will say what is correct.";
  <http://localhost:3333/has_arabic_text> "يَوْمَ يَقُومُ ٱلرُّوحُ وَٱلْمَلَٰٓئِكَةُ صَفًّا ۖ لَّا يَتَكَلَّمُونَ إِلَّا مَنْ أَذِنَ لَهُ ٱلرَّحْمَٰنُ وَقَالَ صَوَابًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5711"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "That is the True Day; so he who wills may take to his Lord a [way of] return.";
  <http://localhost:3333/has_arabic_text> "ذَٰلِكَ ٱلْيَوْمُ ٱلْحَقُّ ۖ فَمَن شَآءَ ٱتَّخَذَ إِلَىٰ رَبِّهِۦ مَـَٔابًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5712"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Naba'";
  dbp:text "Indeed, We have warned you of a near punishment on the Day when a man will observe what his hands have put forth and the disbeliever will say, \"Oh, I wish that I were dust!\"";
  <http://localhost:3333/has_arabic_text> "إِنَّآ أَنذَرْنَٰكُمْ عَذَابًا قَرِيبًا يَوْمَ يَنظُرُ ٱلْمَرْءُ مَا قَدَّمَتْ يَدَاهُ وَيَقُولُ ٱلْكَافِرُ يَٰلَيْتَنِى كُنتُ تُرَٰبًۢا",
    "يَوْمَ يُنفَخُ فِى ٱلصُّورِ فَتَأْتُونَ أَفْوَاجًا", "ٱلَّذِى هُمْ فِيهِ مُخْتَلِفُونَ" .

<http://localhost:3333/An-Nazi'at> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "An-Nazi'at";
  dbo:ayah "5713"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "By those [angels] who extract with violence";
  a dbr:Ayat;
  dbo:ayah "5714"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "And [by] those who remove with ease";
  dbo:Surah "An-Nazi'at";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5715"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "And [by] those who glide [as if] swimming";
  <http://localhost:3333/has_arabic_text> "وَٱلسَّٰبِحَٰتِ سَبْحًا";
  dbo:Surah "An-Nazi'at";
  a dbr:Ayah, dbr:Quranic_verse;
  dbo:ayah "5716"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat;
  dbp:text "And those who race each other in a race";
  <http://localhost:3333/has_arabic_text> "فَٱلسَّٰبِقَٰتِ سَبْقًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5717"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "And those who arrange [each] matter,";
  <http://localhost:3333/has_arabic_text> "فَٱلْمُدَبِّرَٰتِ أَمْرًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5718"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "On the Day the blast [of the Horn] will convulse [creation],";
  <http://localhost:3333/has_arabic_text> "وَٱلنَّٰزِعَٰتِ غَرْقًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5719"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "There will follow it the subsequent [one].";
  <http://localhost:3333/has_arabic_text> "تَتْبَعُهَا ٱلرَّادِفَةُ", "قُلُوبٌ يَوْمَئِذٍ وَاجِفَةٌ";
  a dbr:Ayat, dbr:Quranic_verse;
  dbp:text "Hearts, that Day, will tremble,";
  a dbr:Ayah;
  dbo:Surah "An-Nazi'at";
  dbo:ayah "5720"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5721"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "Their eyes humbled.";
  <http://localhost:3333/has_arabic_text> "أَبْصَٰرُهَا خَٰشِعَةٌ";
  a dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "وَٱلنَّٰشِطَٰتِ نَشْطًا";
  a dbr:Ayah;
  dbp:text "They are [presently] saying, \"Will we indeed be returned to [our] former state [of life]?";
  dbo:Surah "An-Nazi'at";
  dbo:ayah "5722"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbp:text "Even if we should be decayed bones?";
  dbo:Surah "An-Nazi'at";
  dbo:ayah "5723"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "أَءِذَا كُنَّا عِظَٰمًا نَّخِرَةً";
  dbo:ayah "5724"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:Surah "An-Nazi'at";
  dbp:text "They say, \"That, then, would be a losing return.\"";
  <http://localhost:3333/has_arabic_text> "قَالُوا۟ تِلْكَ إِذًا كَرَّةٌ خَاسِرَةٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5725"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "Indeed, it will be but one shout,";
  <http://localhost:3333/has_arabic_text> "فَإِنَّمَا هِىَ زَجْرَةٌ وَٰحِدَةٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5726"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "And suddenly they will be [alert] upon the earth's surface.";
  <http://localhost:3333/has_arabic_text> "فَإِذَا هُم بِٱلسَّاهِرَةِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5727"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "Has there reached you the story of Moses? -";
  <http://localhost:3333/has_arabic_text> "هَلْ أَتَىٰكَ حَدِيثُ مُوسَىٰٓ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5728"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "When his Lord called to him in the sacred valley of Tuwa,";
  <http://localhost:3333/has_arabic_text> "إِذْ نَادَىٰهُ رَبُّهُۥ بِٱلْوَادِ ٱلْمُقَدَّسِ طُوًى";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5729"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "\"Go to Pharaoh. Indeed, he has transgressed.";
  <http://localhost:3333/has_arabic_text> "يَقُولُونَ أَءِنَّا لَمَرْدُودُونَ فِى ٱلْحَافِرَةِ";
  a dbr:Ayah, dbr:Ayat;
  dbo:ayah "5730"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  a dbr:Quranic_verse;
  dbp:text "And say to him, 'Would you [be willing to] purify yourself";
  <http://localhost:3333/has_arabic_text> "فَقُلْ هَل لَّكَ إِلَىٰٓ أَن تَزَكَّىٰ";
  dbo:Surah "An-Nazi'at";
  dbp:text "And let me guide you to your Lord so you would fear [Him]?'\"";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5731"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "وَأَهْدِيَكَ إِلَىٰ رَبِّكَ فَتَخْشَىٰ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5732"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "And he showed him the greatest sign,";
  <http://localhost:3333/has_arabic_text> "فَأَرَىٰهُ ٱلْءَايَةَ ٱلْكُبْرَىٰ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5733"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "But Pharaoh denied and disobeyed.";
  <http://localhost:3333/has_arabic_text> "فَكَذَّبَ وَعَصَىٰ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5734"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "Then he turned his back, striving.";
  <http://localhost:3333/has_arabic_text> "ثُمَّ أَدْبَرَ يَسْعَىٰ";
  a dbr:Ayat;
  dbo:Surah "An-Nazi'at";
  a dbr:Ayah;
  dbp:text "And he gathered [his people] and called out";
  a dbr:Quranic_verse;
  dbo:ayah "5735"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "فَحَشَرَ فَنَادَىٰ";
  dbo:ayah "5736"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat;
  dbp:text "And said, \"I am your most exalted lord.\"";
  a dbr:Ayah;
  dbo:Surah "An-Nazi'at";
  <http://localhost:3333/has_arabic_text> "فَقَالَ أَنَا۠ رَبُّكُمُ ٱلْأَعْلَىٰ";
  a dbr:Quranic_verse;
  dbp:text "So Allah seized him in exemplary punishment for the last and the first [transgression].";
  a dbr:Ayat;
  dbo:Surah "An-Nazi'at";
  dbo:ayah "5737"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "فَأَخَذَهُ ٱللَّهُ نَكَالَ ٱلْءَاخِرَةِ وَٱلْأُولَىٰٓ",
    "إِنَّ فِى ذَٰلِكَ لَعِبْرَةً لِّمَن يَخْشَىٰٓ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbp:text "Indeed in that is a warning for whoever would fear [Allah].";
  dbo:Surah "An-Nazi'at";
  dbo:ayah "5738"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5739"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "Are you a more difficult creation or is the heaven? Allah constructed it.";
  <http://localhost:3333/has_arabic_text> "ءَأَنتُمْ أَشَدُّ خَلْقًا أَمِ ٱلسَّمَآءُ ۚ بَنَىٰهَا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5740"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "He raised its ceiling and proportioned it.";
  <http://localhost:3333/has_arabic_text> "رَفَعَ سَمْكَهَا فَسَوَّىٰهَا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5741"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "And He darkened its night and extracted its brightness.";
  <http://localhost:3333/has_arabic_text> "وَأَغْطَشَ لَيْلَهَا وَأَخْرَجَ ضُحَىٰهَا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5742"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "And after that He spread the earth.";
  <http://localhost:3333/has_arabic_text> "وَٱلْأَرْضَ بَعْدَ ذَٰلِكَ دَحَىٰهَآ", "أَخْرَجَ مِنْهَا مَآءَهَا وَمَرْعَىٰهَا";
  a dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5743"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "He extracted from it its water and its pasture,";
  a dbr:Ayat;
  dbo:Surah "An-Nazi'at";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5744"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "And the mountains He set firmly";
  <http://localhost:3333/has_arabic_text> "وَٱلْجِبَالَ أَرْسَىٰهَا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5745"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "As provision for you and your grazing livestock.";
  <http://localhost:3333/has_arabic_text> "مَتَٰعًا لَّكُمْ وَلِأَنْعَٰمِكُمْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:Surah "An-Nazi'at";
  dbp:text "But when there comes the greatest Overwhelming Calamity -";
  dbo:ayah "5746"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "فَإِذَا جَآءَتِ ٱلطَّآمَّةُ ٱلْكُبْرَىٰ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5747"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "The Day when man will remember that for which he strove,";
  <http://localhost:3333/has_arabic_text> "يَوْمَ تَرْجُفُ ٱلرَّاجِفَةُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5748"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "And Hellfire will be exposed for [all] those who see -";
  <http://localhost:3333/has_arabic_text> "وَبُرِّزَتِ ٱلْجَحِيمُ لِمَن يَرَىٰ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5749"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "So as for he who transgressed";
  dbo:Surah "An-Nazi'at";
  <http://localhost:3333/has_arabic_text> "فَأَمَّا مَن طَغَىٰ";
  dbo:ayah "5750"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:Surah "An-Nazi'at";
  dbp:text "And preferred the life of the world,";
  <http://localhost:3333/has_arabic_text> "وَءَاثَرَ ٱلْحَيَوٰةَ ٱلدُّنْيَا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5751"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "Then indeed, Hellfire will be [his] refuge.";
  <http://localhost:3333/has_arabic_text> "فَإِنَّ ٱلْجَحِيمَ هِىَ ٱلْمَأْوَىٰ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5752"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "But as for he who feared the position of his Lord and prevented the soul from [unlawful] inclination,";
  <http://localhost:3333/has_arabic_text> "وَأَمَّا مَنْ خَافَ مَقَامَ رَبِّهِۦ وَنَهَى ٱلنَّفْسَ عَنِ ٱلْهَوَىٰ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5753"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "Then indeed, Paradise will be [his] refuge.";
  <http://localhost:3333/has_arabic_text> "فَإِنَّ ٱلْجَنَّةَ هِىَ ٱلْمَأْوَىٰ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5754"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "They ask you, [O Muhammad], about the Hour: when is its arrival?";
  <http://localhost:3333/has_arabic_text> "يَسْـَٔلُونَكَ عَنِ ٱلسَّاعَةِ أَيَّانَ مُرْسَىٰهَا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5755"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "In what [position] are you that you should mention it?";
  <http://localhost:3333/has_arabic_text> "فِيمَ أَنتَ مِن ذِكْرَىٰهَآ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5756"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "To your Lord is its finality.";
  <http://localhost:3333/has_arabic_text> "إِلَىٰ رَبِّكَ مُنتَهَىٰهَآ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5757"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "You are only a warner for those who fear it.";
  <http://localhost:3333/has_arabic_text> "إِنَّمَآ أَنتَ مُنذِرُ مَن يَخْشَىٰهَا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5758"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nazi'at";
  dbp:text "It will be, on the Day they see it, as though they had not remained [in the world] except for an afternoon or a morning thereof.";
  <http://localhost:3333/has_arabic_text> "كَأَنَّهُمْ يَوْمَ يَرَوْنَهَا لَمْ يَلْبَثُوٓا۟ إِلَّا عَشِيَّةً أَوْ ضُحَىٰهَا",
    "يَوْمَ يَتَذَكَّرُ ٱلْإِنسَٰنُ مَا سَعَىٰ", "ٱذْهَبْ إِلَىٰ فِرْعَوْنَ إِنَّهُۥ طَغَىٰ" .

<http://localhost:3333/Abasa> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Abasa";
  dbo:ayah "5759"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5760"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  dbp:text "Because there came to him the blind man, [interrupting].", "The Prophet frowned and turned away",
    "But what would make you perceive, [O Muhammad], that perhaps he might be purified";
  dbo:Surah "Abasa";
  dbo:ayah "5761"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "أَن جَآءَهُ ٱلْأَعْمَىٰ";
  a dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "أَوْ يَذَّكَّرُ فَتَنفَعَهُ ٱلذِّكْرَىٰٓ";
  a dbr:Ayat;
  dbp:text "Or be reminded and the remembrance would benefit him?";
  dbo:Surah "Abasa";
  a dbr:Ayah;
  dbo:ayah "5762"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Ayah;
  dbp:text "As for he who thinks himself without need,";
  dbo:Surah "Abasa";
  dbo:ayah "5763"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "أَمَّا مَنِ ٱسْتَغْنَىٰ";
  dbo:ayah "5764"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Abasa";
  dbp:text "To him you give attention.";
  <http://localhost:3333/has_arabic_text> "عَبَسَ وَتَوَلَّىٰٓ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5765"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  dbp:text "And not upon you [is any blame] if he will not be purified.";
  <http://localhost:3333/has_arabic_text> "فَأَنتَ لَهُۥ تَصَدَّىٰ";
  a dbr:Quranic_verse, dbr:Ayat;
  dbp:text "But as for he who came to you striving [for knowledge]";
  dbo:ayah "5766"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  a dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "وَأَمَّا مَن جَآءَكَ يَسْعَىٰ";
  a dbr:Ayah;
  dbo:ayah "5767"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "While he fears [Allah],";
  a dbr:Ayat;
  dbo:Surah "Abasa";
  a dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "وَمَا عَلَيْكَ أَلَّا يَزَّكَّىٰ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5768"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  dbp:text "From him you are distracted.";
  <http://localhost:3333/has_arabic_text> "فَأَنتَ عَنْهُ تَلَهَّىٰ";
  dbp:text "No! Indeed, these verses are a reminder;";
  dbo:Surah "Abasa";
  a dbr:Ayah, dbr:Quranic_verse;
  dbo:ayah "5769"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "كَلَّآ إِنَّهَا تَذْكِرَةٌ";
  dbp:text "So whoever wills may remember it.";
  a dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5770"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  a dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "فَمَن شَآءَ ذَكَرَهُۥ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5771"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  dbp:text "[It is recorded] in honored sheets,";
  <http://localhost:3333/has_arabic_text> "فِى صُحُفٍ مُّكَرَّمَةٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5772"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  dbp:text "Exalted and purified,";
  <http://localhost:3333/has_arabic_text> "مَّرْفُوعَةٍ مُّطَهَّرَةٍۭ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5773"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  dbp:text "[Carried] by the hands of messenger-angels,";
  <http://localhost:3333/has_arabic_text> "بِأَيْدِى سَفَرَةٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5774"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  dbp:text "Noble and dutiful.";
  <http://localhost:3333/has_arabic_text> "كِرَامٍۭ بَرَرَةٍ";
  a dbr:Quranic_verse, dbr:Ayah;
  dbp:text "Cursed is man; how disbelieving is he.";
  dbo:ayah "5775"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  a dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "قُتِلَ ٱلْإِنسَٰنُ مَآ أَكْفَرَهُۥ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5776"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  dbp:text "From what substance did He create him?";
  <http://localhost:3333/has_arabic_text> "مِنْ أَىِّ شَىْءٍ خَلَقَهُۥ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5777"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  dbp:text "From a sperm-drop He created him and destined for him;";
  <http://localhost:3333/has_arabic_text> "مِن نُّطْفَةٍ خَلَقَهُۥ فَقَدَّرَهُۥ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5778"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  dbp:text "Then He eased the way for him;";
  <http://localhost:3333/has_arabic_text> "ثُمَّ ٱلسَّبِيلَ يَسَّرَهُۥ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5779"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  dbp:text "Then He causes his death and provides a grave for him.";
  <http://localhost:3333/has_arabic_text> "ثُمَّ أَمَاتَهُۥ فَأَقْبَرَهُۥ";
  a dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5780"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  a dbr:Ayah;
  dbp:text "Then when He wills, He will resurrect him.";
  <http://localhost:3333/has_arabic_text> "ثُمَّ إِذَا شَآءَ أَنشَرَهُۥ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5781"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  dbp:text "No! Man has not yet accomplished what He commanded him.";
  <http://localhost:3333/has_arabic_text> "كَلَّا لَمَّا يَقْضِ مَآ أَمَرَهُۥ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5782"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  dbp:text "Then let mankind look at his food -";
  <http://localhost:3333/has_arabic_text> "فَلْيَنظُرِ ٱلْإِنسَٰنُ إِلَىٰ طَعَامِهِۦٓ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5783"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  dbp:text "How We poured down water in torrents,";
  <http://localhost:3333/has_arabic_text> "أَنَّا صَبَبْنَا ٱلْمَآءَ صَبًّا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5784"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  dbp:text "Then We broke open the earth, splitting [it with sprouts],";
  <http://localhost:3333/has_arabic_text> "ثُمَّ شَقَقْنَا ٱلْأَرْضَ شَقًّا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5785"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  dbp:text "And caused to grow within it grain";
  <http://localhost:3333/has_arabic_text> "فَأَنۢبَتْنَا فِيهَا حَبًّا";
  a dbr:Quranic_verse, dbr:Ayah, dbr:Ayat;
  dbo:ayah "5786"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  dbp:text "And grapes and herbage";
  <http://localhost:3333/has_arabic_text> "وَعِنَبًا وَقَضْبًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5787"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  dbp:text "And olive and palm trees";
  <http://localhost:3333/has_arabic_text> "وَزَيْتُونًا وَنَخْلًا";
  a dbr:Quranic_verse, dbr:Ayah, dbr:Ayat;
  dbo:ayah "5788"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  dbp:text "And gardens of dense shrubbery";
  <http://localhost:3333/has_arabic_text> "وَحَدَآئِقَ غُلْبًا", "وَفَٰكِهَةً وَأَبًّا";
  dbp:text "And fruit and grass -";
  a dbr:Ayah;
  dbo:Surah "Abasa";
  a dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5789"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5790"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  dbp:text "[As] enjoyment for you and your grazing livestock.";
  <http://localhost:3333/has_arabic_text> "مَّتَٰعًا لَّكُمْ وَلِأَنْعَٰمِكُمْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5791"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  dbp:text "But when there comes the Deafening Blast";
  <http://localhost:3333/has_arabic_text> "فَإِذَا جَآءَتِ ٱلصَّآخَّةُ";
  dbo:ayah "5792"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "On the Day a man will flee from his brother";
  dbo:Surah "Abasa";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "وَمَا يُدْرِيكَ لَعَلَّهُۥ يَزَّكَّىٰٓ";
  a dbr:Ayat;
  dbo:Surah "Abasa";
  a dbr:Quranic_verse;
  dbo:ayah "5793"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah;
  dbp:text "And his mother and his father";
  <http://localhost:3333/has_arabic_text> "وَأُمِّهِۦ وَأَبِيهِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5794"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  dbp:text "And his wife and his children,";
  <http://localhost:3333/has_arabic_text> "وَصَٰحِبَتِهِۦ وَبَنِيهِ";
  a dbr:Ayah, dbr:Quranic_verse;
  dbp:text "For every man, that Day, will be a matter adequate for him.";
  dbo:Surah "Abasa";
  a dbr:Ayat;
  dbo:ayah "5795"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "لِكُلِّ ٱمْرِئٍ مِّنْهُمْ يَوْمَئِذٍ شَأْنٌ يُغْنِيهِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5796"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  dbp:text "[Some] faces, that Day, will be bright -";
  <http://localhost:3333/has_arabic_text> "وَهُوَ يَخْشَىٰ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5797"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  dbp:text "Laughing, rejoicing at good news.";
  <http://localhost:3333/has_arabic_text> "ضَاحِكَةٌ مُّسْتَبْشِرَةٌ", "وَوُجُوهٌ يَوْمَئِذٍ عَلَيْهَا غَبَرَةٌ";
  a dbr:Ayat;
  dbp:text "And [other] faces, that Day, will have upon them dust.";
  dbo:Surah "Abasa";
  dbo:ayah "5798"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse;
  dbo:ayah "5799"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:Surah "Abasa";
  dbp:text "Blackness will cover them.";
  <http://localhost:3333/has_arabic_text> "تَرْهَقُهَا قَتَرَةٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5800"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Abasa";
  dbp:text "Those are the disbelievers, the wicked ones.";
  <http://localhost:3333/has_arabic_text> "أُو۟لَٰٓئِكَ هُمُ ٱلْكَفَرَةُ ٱلْفَجَرَةُ",
    "وُجُوهٌ يَوْمَئِذٍ مُّسْفِرَةٌ", "يَوْمَ يَفِرُّ ٱلْمَرْءُ مِنْ أَخِيهِ" .

<http://localhost:3333/At-Takweer> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "At-Takweer";
  dbo:ayah "5801"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5802"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takweer";
  dbp:text "And when the stars fall, dispersing,", "When the sun is wrapped up [in darkness]",
    "And when the mountains are removed";
  dbo:Surah "At-Takweer";
  a dbr:Ayah, dbr:Quranic_verse;
  dbo:ayah "5803"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "إِذَا ٱلشَّمْسُ كُوِّرَتْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5804"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takweer";
  dbp:text "And when full-term she-camels are neglected";
  <http://localhost:3333/has_arabic_text> "وَإِذَا ٱلنُّجُومُ ٱنكَدَرَتْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5805"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takweer";
  dbp:text "And when the wild beasts are gathered";
  <http://localhost:3333/has_arabic_text> "وَإِذَا ٱلْجِبَالُ سُيِّرَتْ";
  a dbr:Ayat, dbr:Ayah, dbr:Quranic_verse;
  dbo:ayah "5806"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takweer";
  dbp:text "And when the seas are filled with flame";
  <http://localhost:3333/has_arabic_text> "وَإِذَا ٱلْبِحَارُ سُجِّرَتْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5807"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takweer";
  dbp:text "And when the souls are paired";
  <http://localhost:3333/has_arabic_text> "وَإِذَا ٱلنُّفُوسُ زُوِّجَتْ", "وَإِذَا ٱلْعِشَارُ عُطِّلَتْ";
  a dbr:Quranic_verse, dbr:Ayat;
  dbp:text "And when the girl [who was] buried alive is asked";
  dbo:Surah "At-Takweer";
  dbo:ayah "5808"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat, dbr:Ayah;
  dbo:ayah "5809"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takweer";
  dbp:text "For what sin she was killed";
  <http://localhost:3333/has_arabic_text> "بِأَىِّ ذَنۢبٍ قُتِلَتْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5810"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takweer";
  dbp:text "And when the pages are made public";
  <http://localhost:3333/has_arabic_text> "وَإِذَا ٱلصُّحُفُ نُشِرَتْ";
  dbo:ayah "5811"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "And when the sky is stripped away";
  a dbr:Quranic_verse, dbr:Ayah;
  dbo:Surah "At-Takweer";
  a dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "وَإِذَا ٱلسَّمَآءُ كُشِطَتْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5812"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takweer";
  dbp:text "And when Hellfire is set ablaze";
  <http://localhost:3333/has_arabic_text> "وَإِذَا ٱلْجَحِيمُ سُعِّرَتْ";
  a dbr:Ayat, dbr:Ayah, dbr:Quranic_verse;
  dbo:ayah "5813"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takweer";
  dbp:text "And when Paradise is brought near,";
  <http://localhost:3333/has_arabic_text> "وَإِذَا ٱلْجَنَّةُ أُزْلِفَتْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5814"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takweer";
  dbp:text "A soul will [then] know what it has brought [with it].";
  <http://localhost:3333/has_arabic_text> "عَلِمَتْ نَفْسٌ مَّآ أَحْضَرَتْ";
  dbo:ayah "5815"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayah, dbr:Ayat;
  dbo:Surah "At-Takweer";
  dbp:text "So I swear by the retreating stars -";
  <http://localhost:3333/has_arabic_text> "فَلَآ أُقْسِمُ بِٱلْخُنَّسِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5816"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takweer";
  dbp:text "Those that run [their courses] and disappear -";
  <http://localhost:3333/has_arabic_text> "وَإِذَا ٱلْمَوْءُۥدَةُ سُئِلَتْ";
  a dbr:Ayah;
  dbo:ayah "5817"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takweer";
  a dbr:Ayat, dbr:Quranic_verse;
  dbp:text "And by the night as it closes in";
  <http://localhost:3333/has_arabic_text> "وَإِذَا ٱلْوُحُوشُ حُشِرَتْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5818"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takweer";
  dbp:text "And by the dawn when it breathes";
  <http://localhost:3333/has_arabic_text> "وَٱلصُّبْحِ إِذَا تَنَفَّسَ";
  a dbr:Ayah;
  dbo:ayah "5819"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "At-Takweer";
  dbp:text "[That] indeed, the Qur'an is a word [conveyed by] a noble messenger";
  <http://localhost:3333/has_arabic_text> "إِنَّهُۥ لَقَوْلُ رَسُولٍ كَرِيمٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5820"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takweer";
  dbp:text "[Who is] possessed of power and with the Owner of the Throne, secure [in position],";
  <http://localhost:3333/has_arabic_text> "ذِى قُوَّةٍ عِندَ ذِى ٱلْعَرْشِ مَكِينٍ";
  dbo:Surah "At-Takweer";
  dbo:ayah "5821"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbp:text "Obeyed there [in the heavens] and trustworthy.";
  <http://localhost:3333/has_arabic_text> "مُّطَاعٍ ثَمَّ أَمِينٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5822"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takweer";
  dbp:text "And your companion is not [at all] mad.";
  <http://localhost:3333/has_arabic_text> "وَمَا صَاحِبُكُم بِمَجْنُونٍ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5823"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takweer";
  dbp:text "And he has already seen Gabriel in the clear horizon.";
  <http://localhost:3333/has_arabic_text> "وَلَقَدْ رَءَاهُ بِٱلْأُفُقِ ٱلْمُبِينِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5824"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takweer";
  dbp:text "And Muhammad is not a withholder of [knowledge of] the unseen.";
  <http://localhost:3333/has_arabic_text> "وَمَا هُوَ عَلَى ٱلْغَيْبِ بِضَنِينٍ";
  dbo:ayah "5825"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat, dbr:Ayah;
  dbo:Surah "At-Takweer";
  dbp:text "And the Qur'an is not the word of a devil, expelled [from the heavens].";
  <http://localhost:3333/has_arabic_text> "وَمَا هُوَ بِقَوْلِ شَيْطَٰنٍ رَّجِيمٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5826"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takweer";
  dbp:text "So where are you going?";
  <http://localhost:3333/has_arabic_text> "فَأَيْنَ تَذْهَبُونَ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5827"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takweer";
  dbp:text "It is not except a reminder to the worlds";
  <http://localhost:3333/has_arabic_text> "إِنْ هُوَ إِلَّا ذِكْرٌ لِّلْعَٰلَمِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5828"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takweer";
  dbp:text "For whoever wills among you to take a right course.";
  <http://localhost:3333/has_arabic_text> "لِمَن شَآءَ مِنكُمْ أَن يَسْتَقِيمَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5829"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takweer";
  dbp:text "And you do not will except that Allah wills - Lord of the worlds.";
  <http://localhost:3333/has_arabic_text> "وَمَا تَشَآءُونَ إِلَّآ أَن يَشَآءَ ٱللَّهُ رَبُّ ٱلْعَٰلَمِينَ",
    "وَٱلَّيْلِ إِذَا عَسْعَسَ", "ٱلْجَوَارِ ٱلْكُنَّسِ" .

<http://localhost:3333/Al-Infitar> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Infitar";
  dbo:ayah "5830"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "And when the stars fall, scattering,";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5831"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Infitar";
  dbp:text "When the sky breaks apart";
  dbo:ayah "5832"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:Surah "Al-Infitar";
  dbp:text "And when the seas are erupted";
  <http://localhost:3333/has_arabic_text> "إِذَا ٱلسَّمَآءُ ٱنفَطَرَتْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5833"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Infitar";
  dbp:text "And when the [contents of] graves are scattered,";
  <http://localhost:3333/has_arabic_text> "وَإِذَا ٱلْبِحَارُ فُجِّرَتْ";
  a dbr:Ayat;
  dbo:ayah "5834"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Infitar";
  dbp:text "A soul will [then] know what it has put forth and kept back.";
  a dbr:Ayah, dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "عَلِمَتْ نَفْسٌ مَّا قَدَّمَتْ وَأَخَّرَتْ";
  dbo:ayah "5835"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:Surah "Al-Infitar";
  dbp:text "O mankind, what has deceived you concerning your Lord, the Generous,";
  <http://localhost:3333/has_arabic_text> "وَإِذَا ٱلْقُبُورُ بُعْثِرَتْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5836"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Infitar";
  dbp:text "Who created you, proportioned you, and balanced you?";
  <http://localhost:3333/has_arabic_text> "وَإِذَا ٱلْكَوَاكِبُ ٱنتَثَرَتْ";
  a dbr:Ayah;
  dbo:ayah "5837"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Infitar";
  dbp:text "In whatever form He willed has He assembled you.";
  <http://localhost:3333/has_arabic_text> "فِىٓ أَىِّ صُورَةٍ مَّا شَآءَ رَكَّبَكَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5838"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Infitar";
  dbp:text "No! But you deny the Recompense.";
  <http://localhost:3333/has_arabic_text> "كَلَّا بَلْ تُكَذِّبُونَ بِٱلدِّينِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5839"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Infitar";
  dbp:text "And indeed, [appointed] over you are keepers,";
  <http://localhost:3333/has_arabic_text> "وَإِنَّ عَلَيْكُمْ لَحَٰفِظِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5840"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Infitar";
  dbp:text "Noble and recording;";
  <http://localhost:3333/has_arabic_text> "كِرَامًا كَٰتِبِينَ", "يَعْلَمُونَ مَا تَفْعَلُونَ";
  dbp:text "They know whatever you do.";
  dbo:ayah "5841"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Infitar";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah, dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5842"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Infitar";
  dbp:text "Indeed, the righteous will be in pleasure,";
  <http://localhost:3333/has_arabic_text> "إِنَّ ٱلْأَبْرَارَ لَفِى نَعِيمٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5843"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Infitar";
  dbp:text "And indeed, the wicked will be in Hellfire.";
  <http://localhost:3333/has_arabic_text> "وَإِنَّ ٱلْفُجَّارَ لَفِى جَحِيمٍ";
  dbo:ayah "5844"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Infitar";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbp:text "They will [enter to] burn therein on the Day of Recompense,";
  <http://localhost:3333/has_arabic_text> "يَصْلَوْنَهَا يَوْمَ ٱلدِّينِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5845"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Infitar";
  dbp:text "And never therefrom will they be absent.";
  <http://localhost:3333/has_arabic_text> "وَمَا هُمْ عَنْهَا بِغَآئِبِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5846"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Infitar";
  dbp:text "And what can make you know what is the Day of Recompense?";
  <http://localhost:3333/has_arabic_text> "وَمَآ أَدْرَىٰكَ مَا يَوْمُ ٱلدِّينِ";
  dbp:text "Then, what can make you know what is the Day of Recompense?";
  dbo:ayah "5847"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse;
  dbo:Surah "Al-Infitar";
  a dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "ثُمَّ مَآ أَدْرَىٰكَ مَا يَوْمُ ٱلدِّينِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5848"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Infitar";
  dbp:text "It is the Day when a soul will not possess for another soul [power to do] a thing; and the command, that Day, is [entirely] with Allah.";
  <http://localhost:3333/has_arabic_text> "يَوْمَ لَا تَمْلِكُ نَفْسٌ لِّنَفْسٍ شَيْـًٔا ۖ وَٱلْأَمْرُ يَوْمَئِذٍ لِّلَّهِ",
    "يَٰٓأَيُّهَا ٱلْإِنسَٰنُ مَا غَرَّكَ بِرَبِّكَ ٱلْكَرِيمِ", "ٱلَّذِى خَلَقَكَ فَسَوَّىٰكَ فَعَدَلَكَ" .

<http://localhost:3333/Al-Mutaffifeen> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Mutaffifeen";
  dbo:ayah "5849"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mutaffifeen";
  dbo:ayah "5850"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbp:text "Who, when they take a measure from people, take in full.", "Woe to those who give less [than due],";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5851"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mutaffifeen";
  dbp:text "But if they give by measure or by weight to them, they cause loss.";
  <http://localhost:3333/has_arabic_text> "وَإِذَا كَالُوهُمْ أَو وَّزَنُوهُمْ يُخْسِرُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5852"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mutaffifeen";
  dbp:text "Do they not think that they will be resurrected";
  <http://localhost:3333/has_arabic_text> "أَلَا يَظُنُّ أُو۟لَٰٓئِكَ أَنَّهُم مَّبْعُوثُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5853"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mutaffifeen";
  dbp:text "For a tremendous Day -";
  <http://localhost:3333/has_arabic_text> "لِيَوْمٍ عَظِيمٍ";
  a dbr:Ayat;
  dbp:text "The Day when mankind will stand before the Lord of the worlds?";
  a dbr:Quranic_verse;
  dbo:ayah "5854"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mutaffifeen";
  a dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "وَيْلٌ لِّلْمُطَفِّفِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5855"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mutaffifeen";
  dbp:text "No! Indeed, the record of the wicked is in sijjeen.";
  <http://localhost:3333/has_arabic_text> "كَلَّآ إِنَّ كِتَٰبَ ٱلْفُجَّارِ لَفِى سِجِّينٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5856"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mutaffifeen";
  dbp:text "And what can make you know what is sijjeen?";
  <http://localhost:3333/has_arabic_text> "وَمَآ أَدْرَىٰكَ مَا سِجِّينٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5857"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mutaffifeen";
  dbp:text "It is [their destination recorded in] a register inscribed.";
  <http://localhost:3333/has_arabic_text> "كِتَٰبٌ مَّرْقُومٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5858"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mutaffifeen";
  dbp:text "Woe, that Day, to the deniers,";
  <http://localhost:3333/has_arabic_text> "وَيْلٌ يَوْمَئِذٍ لِّلْمُكَذِّبِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5859"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mutaffifeen";
  dbp:text "Who deny the Day of Recompense.";
  <http://localhost:3333/has_arabic_text> "يَوْمَ يَقُومُ ٱلنَّاسُ لِرَبِّ ٱلْعَٰلَمِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5860"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mutaffifeen";
  dbp:text "And none deny it except every sinful transgressor.";
  <http://localhost:3333/has_arabic_text> "وَمَا يُكَذِّبُ بِهِۦٓ إِلَّا كُلُّ مُعْتَدٍ أَثِيمٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5861"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mutaffifeen";
  dbp:text "When Our verses are recited to him, he says, \"Legends of the former peoples.\"";
  <http://localhost:3333/has_arabic_text> "إِذَا تُتْلَىٰ عَلَيْهِ ءَايَٰتُنَا قَالَ أَسَٰطِيرُ ٱلْأَوَّلِينَ";
  a dbr:Ayat;
  dbp:text "No! Rather, the stain has covered their hearts of that which they were earning.";
  a dbr:Ayah;
  dbo:ayah "5862"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse;
  dbo:Surah "Al-Mutaffifeen";
  <http://localhost:3333/has_arabic_text> "كَلَّا ۖ بَلْ ۜ رَانَ عَلَىٰ قُلُوبِهِم مَّا كَانُوا۟ يَكْسِبُونَ",
    "كَلَّآ إِنَّهُمْ عَن رَّبِّهِمْ يَوْمَئِذٍ لَّمَحْجُوبُونَ";
  dbp:text "No! Indeed, from their Lord, that Day, they will be partitioned.";
  dbo:Surah "Al-Mutaffifeen";
  dbo:ayah "5863"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbp:text "Then indeed, they will [enter and] burn in Hellfire.";
  dbo:Surah "Al-Mutaffifeen";
  dbo:ayah "5864"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "ثُمَّ إِنَّهُمْ لَصَالُوا۟ ٱلْجَحِيمِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5865"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mutaffifeen";
  dbp:text "Then it will be said [to them], \"This is what you used to deny.\"";
  <http://localhost:3333/has_arabic_text> "ثُمَّ يُقَالُ هَٰذَا ٱلَّذِى كُنتُم بِهِۦ تُكَذِّبُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5866"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mutaffifeen";
  dbp:text "No! Indeed, the record of the righteous is in 'illiyyun.";
  <http://localhost:3333/has_arabic_text> "كَلَّآ إِنَّ كِتَٰبَ ٱلْأَبْرَارِ لَفِى عِلِّيِّينَ";
  a dbr:Ayah;
  dbp:text "And what can make you know what is 'illiyyun?";
  dbo:Surah "Al-Mutaffifeen";
  a dbr:Quranic_verse;
  dbo:ayah "5867"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "وَمَآ أَدْرَىٰكَ مَا عِلِّيُّونَ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5868"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mutaffifeen";
  dbp:text "It is [their destination recorded in] a register inscribed";
  <http://localhost:3333/has_arabic_text> "كِتَٰبٌ مَّرْقُومٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5869"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mutaffifeen";
  dbp:text "Which is witnessed by those brought near [to Allah].";
  <http://localhost:3333/has_arabic_text> "يَشْهَدُهُ ٱلْمُقَرَّبُونَ", "إِنَّ ٱلْأَبْرَارَ لَفِى نَعِيمٍ";
  dbp:text "Indeed, the righteous will be in pleasure";
  dbo:Surah "Al-Mutaffifeen";
  dbo:ayah "5870"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah, dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5871"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mutaffifeen";
  dbp:text "On adorned couches, observing.";
  <http://localhost:3333/has_arabic_text> "عَلَى ٱلْأَرَآئِكِ يَنظُرُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5872"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mutaffifeen";
  dbp:text "You will recognize in their faces the radiance of pleasure.";
  <http://localhost:3333/has_arabic_text> "تَعْرِفُ فِى وُجُوهِهِمْ نَضْرَةَ ٱلنَّعِيمِ";
  dbp:text "They will be given to drink [pure] wine [which was] sealed.";
  a dbr:Quranic_verse;
  dbo:Surah "Al-Mutaffifeen";
  dbo:ayah "5873"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "يُسْقَوْنَ مِن رَّحِيقٍ مَّخْتُومٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5874"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mutaffifeen";
  dbp:text "The last of it is musk. So for this let the competitors compete.";
  <http://localhost:3333/has_arabic_text> "خِتَٰمُهُۥ مِسْكٌ ۚ وَفِى ذَٰلِكَ فَلْيَتَنَافَسِ ٱلْمُتَنَٰفِسُونَ";
  dbp:text "And its mixture is of Tasneem,";
  a dbr:Ayah;
  dbo:Surah "Al-Mutaffifeen";
  dbo:ayah "5875"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "وَمِزَاجُهُۥ مِن تَسْنِيمٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5876"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mutaffifeen";
  dbp:text "A spring from which those near [to Allah] drink.";
  <http://localhost:3333/has_arabic_text> "عَيْنًا يَشْرَبُ بِهَا ٱلْمُقَرَّبُونَ";
  a dbr:Quranic_verse;
  dbo:Surah "Al-Mutaffifeen";
  a dbr:Ayat, dbr:Ayah;
  dbo:ayah "5877"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "Indeed, those who committed crimes used to laugh at those who believed.";
  <http://localhost:3333/has_arabic_text> "إِنَّ ٱلَّذِينَ أَجْرَمُوا۟ كَانُوا۟ مِنَ ٱلَّذِينَ ءَامَنُوا۟ يَضْحَكُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5878"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mutaffifeen";
  dbp:text "And when they passed by them, they would exchange derisive glances.";
  <http://localhost:3333/has_arabic_text> "وَإِذَا مَرُّوا۟ بِهِمْ يَتَغَامَزُونَ";
  dbo:Surah "Al-Mutaffifeen";
  a dbr:Ayah;
  dbo:ayah "5879"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat;
  dbp:text "And when they returned to their people, they would return jesting.";
  <http://localhost:3333/has_arabic_text> "وَإِذَا ٱنقَلَبُوٓا۟ إِلَىٰٓ أَهْلِهِمُ ٱنقَلَبُوا۟ فَكِهِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5880"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mutaffifeen";
  dbp:text "And when they saw them, they would say, \"Indeed, those are truly lost.\"";
  <http://localhost:3333/has_arabic_text> "وَإِذَا رَأَوْهُمْ قَالُوٓا۟ إِنَّ هَٰٓؤُلَآءِ لَضَآلُّونَ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5881"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mutaffifeen";
  dbp:text "But they had not been sent as guardians over them.";
  <http://localhost:3333/has_arabic_text> "وَمَآ أُرْسِلُوا۟ عَلَيْهِمْ حَٰفِظِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5882"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mutaffifeen";
  dbp:text "So Today those who believed are laughing at the disbelievers,";
  <http://localhost:3333/has_arabic_text> "فَٱلْيَوْمَ ٱلَّذِينَ ءَامَنُوا۟ مِنَ ٱلْكُفَّارِ يَضْحَكُونَ";
  dbo:Surah "Al-Mutaffifeen";
  a dbr:Ayah;
  dbo:ayah "5883"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse;
  dbp:text "On adorned couches, observing.";
  <http://localhost:3333/has_arabic_text> "عَلَى ٱلْأَرَآئِكِ يَنظُرُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5884"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Mutaffifeen";
  dbp:text "Have the disbelievers [not] been rewarded [this Day] for what they used to do?";
  <http://localhost:3333/has_arabic_text> "هَلْ ثُوِّبَ ٱلْكُفَّارُ مَا كَانُوا۟ يَفْعَلُونَ",
    "ٱلَّذِينَ إِذَا ٱكْتَالُوا۟ عَلَى ٱلنَّاسِ يَسْتَوْفُونَ", "ٱلَّذِينَ يُكَذِّبُونَ بِيَوْمِ ٱلدِّينِ" .

<http://localhost:3333/Al-Inshiqaq> dbo:Surah "Al-Inshiqaq";
  a dbr:Quranic_verse, dbr:Ayat, dbr:Ayah;
  dbo:ayah "5885"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5886"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Inshiqaq";
  dbp:text "And has responded to its Lord and was obligated [to do so]", "When the sky has split [open]";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5887"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Inshiqaq";
  dbp:text "And when the earth has been extended";
  <http://localhost:3333/has_arabic_text> "إِذَا ٱلسَّمَآءُ ٱنشَقَّتْ", "وَأَذِنَتْ لِرَبِّهَا وَحُقَّتْ";
  dbo:ayah "5888"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbp:text "And has cast out that within it and relinquished [it]";
  dbo:Surah "Al-Inshiqaq";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5889"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Inshiqaq";
  dbp:text "And has responded to its Lord and was obligated [to do so] -";
  <http://localhost:3333/has_arabic_text> "وَأَذِنَتْ لِرَبِّهَا وَحُقَّتْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5890"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Inshiqaq";
  dbp:text "O mankind, indeed you are laboring toward your Lord with [great] exertion and will meet it.";
  <http://localhost:3333/has_arabic_text> "وَأَلْقَتْ مَا فِيهَا وَتَخَلَّتْ";
  dbo:Surah "Al-Inshiqaq";
  dbo:ayah "5891"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "Then as for he who is given his record in his right hand,";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "فَأَمَّا مَنْ أُوتِىَ كِتَٰبَهُۥ بِيَمِينِهِۦ";
  a dbr:Ayah;
  dbo:Surah "Al-Inshiqaq";
  a dbr:Quranic_verse;
  dbo:ayah "5892"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat;
  dbp:text "He will be judged with an easy account";
  <http://localhost:3333/has_arabic_text> "فَسَوْفَ يُحَاسَبُ حِسَابًا يَسِيرًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5893"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Inshiqaq";
  dbp:text "And return to his people in happiness.";
  <http://localhost:3333/has_arabic_text> "وَإِذَا ٱلْأَرْضُ مُدَّتْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5894"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Inshiqaq";
  dbp:text "But as for he who is given his record behind his back,";
  <http://localhost:3333/has_arabic_text> "وَأَمَّا مَنْ أُوتِىَ كِتَٰبَهُۥ وَرَآءَ ظَهْرِهِۦ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5895"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Inshiqaq";
  dbp:text "He will cry out for destruction";
  <http://localhost:3333/has_arabic_text> "فَسَوْفَ يَدْعُوا۟ ثُبُورًا", "وَيَصْلَىٰ سَعِيرًا";
  dbo:ayah "5896"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Inshiqaq";
  dbp:text "And [enter to] burn in a Blaze.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat, dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5897"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Inshiqaq";
  dbp:text "Indeed, he had [once] been among his people in happiness;";
  <http://localhost:3333/has_arabic_text> "إِنَّهُۥ كَانَ فِىٓ أَهْلِهِۦ مَسْرُورًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5898"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Inshiqaq";
  dbp:text "Indeed, he had thought he would never return [to Allah].";
  <http://localhost:3333/has_arabic_text> "إِنَّهُۥ ظَنَّ أَن لَّن يَحُورَ";
  dbo:ayah "5899"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayah, dbr:Ayat;
  dbo:Surah "Al-Inshiqaq";
  dbp:text "But yes! Indeed, his Lord was ever of him, Seeing.";
  <http://localhost:3333/has_arabic_text> "بَلَىٰٓ إِنَّ رَبَّهُۥ كَانَ بِهِۦ بَصِيرًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5900"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Inshiqaq";
  dbp:text "So I swear by the twilight glow";
  <http://localhost:3333/has_arabic_text> "فَلَآ أُقْسِمُ بِٱلشَّفَقِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5901"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Inshiqaq";
  dbp:text "And [by] the night and what it envelops";
  <http://localhost:3333/has_arabic_text> "وَيَنقَلِبُ إِلَىٰٓ أَهْلِهِۦ مَسْرُورًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5902"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Inshiqaq";
  dbp:text "And [by] the moon when it becomes full";
  <http://localhost:3333/has_arabic_text> "وَٱلَّيْلِ وَمَا وَسَقَ";
  dbp:text "[That] you will surely experience state after state.";
  dbo:Surah "Al-Inshiqaq";
  a dbr:Ayah, dbr:Quranic_verse;
  dbo:ayah "5903"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "لَتَرْكَبُنَّ طَبَقًا عَن طَبَقٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5904"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Inshiqaq";
  dbp:text "So what is [the matter] with them [that] they do not believe,";
  <http://localhost:3333/has_arabic_text> "فَمَا لَهُمْ لَا يُؤْمِنُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5905"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Inshiqaq";
  dbp:text "And when the Qur'an is recited to them, they do not prostrate [to Allah]?";
  <http://localhost:3333/has_arabic_text> "وَإِذَا قُرِئَ عَلَيْهِمُ ٱلْقُرْءَانُ لَا يَسْجُدُونَ ۩",
    "بَلِ ٱلَّذِينَ كَفَرُوا۟ يُكَذِّبُونَ";
  dbo:ayah "5906"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse;
  dbp:text "But those who have disbelieved deny,";
  a dbr:Ayat;
  dbo:Surah "Al-Inshiqaq";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5907"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Inshiqaq";
  dbp:text "And Allah is most knowing of what they keep within themselves.";
  <http://localhost:3333/has_arabic_text> "وَٱللَّهُ أَعْلَمُ بِمَا يُوعُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5908"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Inshiqaq";
  dbp:text "So give them tidings of a painful punishment,";
  <http://localhost:3333/has_arabic_text> "فَبَشِّرْهُم بِعَذَابٍ أَلِيمٍ";
  dbp:text "Except for those who believe and do righteous deeds. For them is a reward uninterrupted.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:Surah "Al-Inshiqaq";
  dbo:ayah "5909"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "إِلَّا ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّٰلِحَٰتِ لَهُمْ أَجْرٌ غَيْرُ مَمْنُونٍۭ",
    "وَٱلْقَمَرِ إِذَا ٱتَّسَقَ", "يَٰٓأَيُّهَا ٱلْإِنسَٰنُ إِنَّكَ كَادِحٌ إِلَىٰ رَبِّكَ كَدْحًا فَمُلَٰقِيهِ" .

<http://localhost:3333/Al-Burooj> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Burooj";
  dbo:ayah "5910"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5911"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Burooj";
  dbp:text "And [by] the promised Day", "By the sky containing great stars", "And [by] the witness and what is witnessed,";
  a dbr:Quranic_verse, dbr:Ayat;
  dbo:Surah "Al-Burooj";
  dbo:ayah "5912"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "وَشَاهِدٍ وَمَشْهُودٍ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5913"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Burooj";
  dbp:text "Cursed were the companions of the trench";
  <http://localhost:3333/has_arabic_text> "قُتِلَ أَصْحَٰبُ ٱلْأُخْدُودِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5914"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Burooj";
  dbp:text "[Containing] the fire full of fuel,";
  <http://localhost:3333/has_arabic_text> "وَٱلسَّمَآءِ ذَاتِ ٱلْبُرُوجِ", "إِذْ هُمْ عَلَيْهَا قُعُودٌ";
  a dbr:Ayah;
  dbp:text "When they were sitting near it";
  dbo:Surah "Al-Burooj";
  a dbr:Quranic_verse;
  dbo:ayah "5915"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5916"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Burooj";
  dbp:text "And they, to what they were doing against the believers, were witnesses.";
  <http://localhost:3333/has_arabic_text> "وَهُمْ عَلَىٰ مَا يَفْعَلُونَ بِٱلْمُؤْمِنِينَ شُهُودٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5917"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Burooj";
  dbp:text "And they resented them not except because they believed in Allah, the Exalted in Might, the Praiseworthy,";
  <http://localhost:3333/has_arabic_text> "وَمَا نَقَمُوا۟ مِنْهُمْ إِلَّآ أَن يُؤْمِنُوا۟ بِٱللَّهِ ٱلْعَزِيزِ ٱلْحَمِيدِ";
  dbo:Surah "Al-Burooj";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5918"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "To whom belongs the dominion of the heavens and the earth. And Allah, over all things, is Witness.";
  <http://localhost:3333/has_arabic_text> "وَٱلْيَوْمِ ٱلْمَوْعُودِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5919"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Burooj";
  dbp:text "Indeed, those who have tortured the believing men and believing women and then have not repented will have the punishment of Hell, and they will have the punishment of the Burning Fire.";
  <http://localhost:3333/has_arabic_text> "إِنَّ ٱلَّذِينَ فَتَنُوا۟ ٱلْمُؤْمِنِينَ وَٱلْمُؤْمِنَٰتِ ثُمَّ لَمْ يَتُوبُوا۟ فَلَهُمْ عَذَابُ جَهَنَّمَ وَلَهُمْ عَذَابُ ٱلْحَرِيقِ";
  a dbr:Ayat, dbr:Ayah;
  dbp:text "Indeed, those who have believed and done righteous deeds will have gardens beneath which rivers flow. That is the great attainment.";
  a dbr:Quranic_verse;
  dbo:Surah "Al-Burooj";
  dbo:ayah "5920"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "إِنَّ ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّٰلِحَٰتِ لَهُمْ جَنَّٰتٌ تَجْرِى مِن تَحْتِهَا ٱلْأَنْهَٰرُ ۚ ذَٰلِكَ ٱلْفَوْزُ ٱلْكَبِيرُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5921"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Burooj";
  dbp:text "Indeed, the vengeance of your Lord is severe.";
  <http://localhost:3333/has_arabic_text> "إِنَّ بَطْشَ رَبِّكَ لَشَدِيدٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5922"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Burooj";
  dbp:text "Indeed, it is He who originates [creation] and repeats.";
  <http://localhost:3333/has_arabic_text> "إِنَّهُۥ هُوَ يُبْدِئُ وَيُعِيدُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5923"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Burooj";
  dbp:text "And He is the Forgiving, the Affectionate,";
  <http://localhost:3333/has_arabic_text> "وَهُوَ ٱلْغَفُورُ ٱلْوَدُودُ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5924"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Burooj";
  dbp:text "Honorable Owner of the Throne,";
  <http://localhost:3333/has_arabic_text> "ذُو ٱلْعَرْشِ ٱلْمَجِيدُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5925"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Burooj";
  dbp:text "Effecter of what He intends.";
  <http://localhost:3333/has_arabic_text> "فَعَّالٌ لِّمَا يُرِيدُ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5926"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Burooj";
  dbp:text "Has there reached you the story of the soldiers -";
  <http://localhost:3333/has_arabic_text> "هَلْ أَتَىٰكَ حَدِيثُ ٱلْجُنُودِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5927"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Burooj";
  dbp:text "[Those of] Pharaoh and Thamud?";
  <http://localhost:3333/has_arabic_text> "فِرْعَوْنَ وَثَمُودَ";
  a dbr:Quranic_verse, dbr:Ayah, dbr:Ayat;
  dbo:ayah "5928"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Burooj";
  dbp:text "But they who disbelieve are in [persistent] denial,";
  <http://localhost:3333/has_arabic_text> "بَلِ ٱلَّذِينَ كَفَرُوا۟ فِى تَكْذِيبٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5929"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Burooj";
  dbp:text "While Allah encompasses them from behind.";
  <http://localhost:3333/has_arabic_text> "وَٱللَّهُ مِن وَرَآئِهِم مُّحِيطٌۢ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5930"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Burooj";
  dbp:text "But this is an honored Qur'an";
  <http://localhost:3333/has_arabic_text> "بَلْ هُوَ قُرْءَانٌ مَّجِيدٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5931"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Burooj";
  dbp:text "[Inscribed] in a Preserved Slate.";
  <http://localhost:3333/has_arabic_text> "فِى لَوْحٍ مَّحْفُوظٍۭ", "ٱلنَّارِ ذَاتِ ٱلْوَقُودِ",
    "ٱلَّذِى لَهُۥ مُلْكُ ٱلسَّمَٰوَٰتِ وَٱلْأَرْضِ ۚ وَٱللَّهُ عَلَىٰ كُلِّ شَىْءٍ شَهِيدٌ" .

<http://localhost:3333/At-Tariq> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "At-Tariq";
  dbo:ayah "5932"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5933"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Tariq";
  dbp:text "And what can make you know what is the night comer?", "By the sky and the night comer -";
  a dbr:Ayah;
  dbo:ayah "5934"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "At-Tariq";
  dbp:text "It is the piercing star -";
  <http://localhost:3333/has_arabic_text> "وَمَآ أَدْرَىٰكَ مَا ٱلطَّارِقُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5935"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Tariq";
  dbp:text "There is no soul but that it has over it a protector.";
  <http://localhost:3333/has_arabic_text> "إِن كُلُّ نَفْسٍ لَّمَّا عَلَيْهَا حَافِظٌ",
    "فَلْيَنظُرِ ٱلْإِنسَٰنُ مِمَّ خُلِقَ";
  a dbr:Ayah;
  dbo:Surah "At-Tariq";
  dbo:ayah "5936"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse;
  dbp:text "So let man observe from what he was created.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5937"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Tariq";
  dbp:text "He was created from a fluid, ejected,";
  <http://localhost:3333/has_arabic_text> "خُلِقَ مِن مَّآءٍ دَافِقٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5938"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Tariq";
  dbp:text "Emerging from between the backbone and the ribs.";
  <http://localhost:3333/has_arabic_text> "وَٱلسَّمَآءِ وَٱلطَّارِقِ";
  dbo:ayah "5939"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:Surah "At-Tariq";
  dbp:text "Indeed, Allah, to return him [to life], is Able.";
  <http://localhost:3333/has_arabic_text> "إِنَّهُۥ عَلَىٰ رَجْعِهِۦ لَقَادِرٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5940"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Tariq";
  dbp:text "The Day when secrets will be put on trial,";
  <http://localhost:3333/has_arabic_text> "يَخْرُجُ مِنۢ بَيْنِ ٱلصُّلْبِ وَٱلتَّرَآئِبِ";
  dbo:Surah "At-Tariq";
  <http://localhost:3333/has_arabic_text> "فَمَا لَهُۥ مِن قُوَّةٍ وَلَا نَاصِرٍ";
  dbp:text "Then man will have no power or any helper.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5941"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5942"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Tariq";
  dbp:text "By the sky which returns [rain]";
  <http://localhost:3333/has_arabic_text> "وَٱلسَّمَآءِ ذَاتِ ٱلرَّجْعِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5943"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Tariq";
  dbp:text "And [by] the earth which cracks open,";
  <http://localhost:3333/has_arabic_text> "وَٱلْأَرْضِ ذَاتِ ٱلصَّدْعِ";
  a dbr:Ayah;
  dbo:ayah "5944"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "At-Tariq";
  dbp:text "Indeed, the Qur'an is a decisive statement,";
  <http://localhost:3333/has_arabic_text> "إِنَّهُۥ لَقَوْلٌ فَصْلٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5945"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Tariq";
  dbp:text "And it is not amusement.";
  <http://localhost:3333/has_arabic_text> "وَمَا هُوَ بِٱلْهَزْلِ";
  a dbr:Ayah;
  dbp:text "Indeed, they are planning a plan,";
  dbo:Surah "At-Tariq";
  dbo:ayah "5946"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "إِنَّهُمْ يَكِيدُونَ كَيْدًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5947"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Tariq";
  dbp:text "But I am planning a plan.";
  <http://localhost:3333/has_arabic_text> "وَأَكِيدُ كَيْدًا";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "5948"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Tariq";
  dbp:text "So allow time for the disbelievers. Leave them awhile.";
  <http://localhost:3333/has_arabic_text> "فَمَهِّلِ ٱلْكَٰفِرِينَ أَمْهِلْهُمْ رُوَيْدًۢا",
    "يَوْمَ تُبْلَى ٱلسَّرَآئِرُ", "ٱلنَّجْمُ ٱلثَّاقِبُ" .

<http://localhost:3333/Al-A'la> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-A'la";
  dbo:ayah "5949"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayah, dbr:Ayat;
  dbo:ayah "5950"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-A'la";
  dbp:text "Exalt the name of your Lord, the Most High,", "Who created and proportioned";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5951"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-A'la";
  dbp:text "And who destined and [then] guided";
  <http://localhost:3333/has_arabic_text> "سَبِّحِ ٱسْمَ رَبِّكَ ٱلْأَعْلَى";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5952"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-A'la";
  dbp:text "And who brings out the pasture";
  <http://localhost:3333/has_arabic_text> "وَٱلَّذِى قَدَّرَ فَهَدَىٰ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5953"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-A'la";
  dbp:text "And [then] makes it black stubble.";
  <http://localhost:3333/has_arabic_text> "فَجَعَلَهُۥ غُثَآءً أَحْوَىٰ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5954"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-A'la";
  dbp:text "We will make you recite, [O Muhammad], and you will not forget,";
  <http://localhost:3333/has_arabic_text> "سَنُقْرِئُكَ فَلَا تَنسَىٰٓ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5955"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-A'la";
  dbp:text "Except what Allah should will. Indeed, He knows what is declared and what is hidden.";
  <http://localhost:3333/has_arabic_text> "إِلَّا مَا شَآءَ ٱللَّهُ ۚ إِنَّهُۥ يَعْلَمُ ٱلْجَهْرَ وَمَا يَخْفَىٰ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5956"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-A'la";
  dbp:text "And We will ease you toward ease.";
  <http://localhost:3333/has_arabic_text> "وَنُيَسِّرُكَ لِلْيُسْرَىٰ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5957"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-A'la";
  dbp:text "So remind, if the reminder should benefit;";
  <http://localhost:3333/has_arabic_text> "فَذَكِّرْ إِن نَّفَعَتِ ٱلذِّكْرَىٰ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5958"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-A'la";
  dbp:text "He who fears [Allah] will be reminded.";
  <http://localhost:3333/has_arabic_text> "سَيَذَّكَّرُ مَن يَخْشَىٰ", "وَيَتَجَنَّبُهَا ٱلْأَشْقَى";
  a dbr:Ayah;
  dbp:text "But the wretched one will avoid it -";
  dbo:ayah "5959"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat;
  dbo:Surah "Al-A'la", "Al-A'la";
  dbo:ayah "5960"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbp:text "[He] who will [enter and] burn in the greatest Fire,";
  <http://localhost:3333/has_arabic_text> "وَٱلَّذِىٓ أَخْرَجَ ٱلْمَرْعَىٰ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5961"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-A'la";
  dbp:text "Neither dying therein nor living.";
  <http://localhost:3333/has_arabic_text> "ثُمَّ لَا يَمُوتُ فِيهَا وَلَا يَحْيَىٰ";
  a dbr:Ayah;
  dbo:Surah "Al-A'la";
  dbo:ayah "5962"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat;
  dbp:text "He has certainly succeeded who purifies himself";
  <http://localhost:3333/has_arabic_text> "قَدْ أَفْلَحَ مَن تَزَكَّىٰ";
  a dbr:Ayah;
  dbo:ayah "5963"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat;
  dbo:Surah "Al-A'la";
  dbp:text "And mentions the name of his Lord and prays.";
  <http://localhost:3333/has_arabic_text> "وَذَكَرَ ٱسْمَ رَبِّهِۦ فَصَلَّىٰ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5964"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-A'la";
  dbp:text "But you prefer the worldly life,";
  <http://localhost:3333/has_arabic_text> "بَلْ تُؤْثِرُونَ ٱلْحَيَوٰةَ ٱلدُّنْيَا";
  dbo:ayah "5965"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat, dbr:Ayah;
  dbo:Surah "Al-A'la";
  dbp:text "While the Hereafter is better and more enduring.";
  <http://localhost:3333/has_arabic_text> "وَٱلْءَاخِرَةُ خَيْرٌ وَأَبْقَىٰٓ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5966"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-A'la";
  dbp:text "Indeed, this is in the former scriptures,";
  <http://localhost:3333/has_arabic_text> "إِنَّ هَٰذَا لَفِى ٱلصُّحُفِ ٱلْأُولَىٰ";
  a dbr:Ayat;
  dbo:Surah "Al-A'la";
  a dbr:Ayah;
  dbo:ayah "5967"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse;
  dbp:text "The scriptures of Abraham and Moses.";
  <http://localhost:3333/has_arabic_text> "صُحُفِ إِبْرَٰهِيمَ وَمُوسَىٰ", "ٱلَّذِى خَلَقَ فَسَوَّىٰ",
    "ٱلَّذِى يَصْلَى ٱلنَّارَ ٱلْكُبْرَىٰ" .

<http://localhost:3333/Al-Ghashiyah> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Ghashiyah";
  dbo:ayah "5968"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ghashiyah";
  dbo:ayah "5969"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbp:text "Has there reached you the report of the Overwhelming [event]?", "[Some] faces, that Day, will be humbled,";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5970"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ghashiyah";
  dbp:text "Working [hard] and exhausted.";
  <http://localhost:3333/has_arabic_text> "عَامِلَةٌ نَّاصِبَةٌ";
  a dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Ghashiyah";
  dbo:ayah "5971"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah;
  dbp:text "They will [enter to] burn in an intensely hot Fire.";
  <http://localhost:3333/has_arabic_text> "تَصْلَىٰ نَارًا حَامِيَةً";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5972"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ghashiyah";
  dbp:text "They will be given drink from a boiling spring.";
  <http://localhost:3333/has_arabic_text> "تُسْقَىٰ مِنْ عَيْنٍ ءَانِيَةٍ";
  a dbr:Ayah;
  dbo:Surah "Al-Ghashiyah";
  dbo:ayah "5973"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse;
  dbp:text "For them there will be no food except from a poisonous, thorny plant";
  <http://localhost:3333/has_arabic_text> "لَّيْسَ لَهُمْ طَعَامٌ إِلَّا مِن ضَرِيعٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5974"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ghashiyah";
  dbp:text "Which neither nourishes nor avails against hunger.";
  <http://localhost:3333/has_arabic_text> "لَّا يُسْمِنُ وَلَا يُغْنِى مِن جُوعٍ";
  a dbr:Quranic_verse;
  dbp:text "[Other] faces, that Day, will show pleasure.";
  dbo:Surah "Al-Ghashiyah";
  dbo:ayah "5975"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "هَلْ أَتَىٰكَ حَدِيثُ ٱلْغَٰشِيَةِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5976"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ghashiyah";
  dbp:text "With their effort [they are] satisfied";
  <http://localhost:3333/has_arabic_text> "لِّسَعْيِهَا رَاضِيَةٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5977"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ghashiyah";
  dbp:text "In an elevated garden,";
  <http://localhost:3333/has_arabic_text> "فِى جَنَّةٍ عَالِيَةٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5978"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ghashiyah";
  dbp:text "Wherein they will hear no unsuitable speech.";
  <http://localhost:3333/has_arabic_text> "لَّا تَسْمَعُ فِيهَا لَٰغِيَةً";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5979"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ghashiyah";
  dbp:text "Within it is a flowing spring.";
  <http://localhost:3333/has_arabic_text> "فِيهَا عَيْنٌ جَارِيَةٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5980"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ghashiyah";
  dbp:text "Within it are couches raised high";
  <http://localhost:3333/has_arabic_text> "فِيهَا سُرُرٌ مَّرْفُوعَةٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5981"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ghashiyah";
  dbp:text "And cups put in place";
  <http://localhost:3333/has_arabic_text> "وَأَكْوَابٌ مَّوْضُوعَةٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5982"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ghashiyah";
  dbp:text "And cushions lined up";
  <http://localhost:3333/has_arabic_text> "وَنَمَارِقُ مَصْفُوفَةٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5983"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ghashiyah";
  dbp:text "And carpets spread around.";
  <http://localhost:3333/has_arabic_text> "وَزَرَابِىُّ مَبْثُوثَةٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5984"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ghashiyah";
  dbp:text "Then do they not look at the camels - how they are created?";
  <http://localhost:3333/has_arabic_text> "أَفَلَا يَنظُرُونَ إِلَى ٱلْإِبِلِ كَيْفَ خُلِقَتْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5985"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ghashiyah";
  dbp:text "And at the sky - how it is raised?";
  <http://localhost:3333/has_arabic_text> "وَإِلَى ٱلسَّمَآءِ كَيْفَ رُفِعَتْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5986"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ghashiyah";
  dbp:text "And at the mountains - how they are erected?";
  <http://localhost:3333/has_arabic_text> "وَإِلَى ٱلْجِبَالِ كَيْفَ نُصِبَتْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5987"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ghashiyah";
  dbp:text "And at the earth - how it is spread out?";
  <http://localhost:3333/has_arabic_text> "وَإِلَى ٱلْأَرْضِ كَيْفَ سُطِحَتْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5988"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ghashiyah";
  dbp:text "So remind, [O Muhammad]; you are only a reminder.";
  <http://localhost:3333/has_arabic_text> "فَذَكِّرْ إِنَّمَآ أَنتَ مُذَكِّرٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5989"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ghashiyah";
  dbp:text "You are not over them a controller.";
  <http://localhost:3333/has_arabic_text> "لَّسْتَ عَلَيْهِم بِمُصَيْطِرٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5990"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ghashiyah";
  dbp:text "However, he who turns away and disbelieves -";
  <http://localhost:3333/has_arabic_text> "إِلَّا مَن تَوَلَّىٰ وَكَفَرَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5991"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ghashiyah";
  dbp:text "Then Allah will punish him with the greatest punishment.";
  <http://localhost:3333/has_arabic_text> "فَيُعَذِّبُهُ ٱللَّهُ ٱلْعَذَابَ ٱلْأَكْبَرَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5992"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ghashiyah";
  dbp:text "Indeed, to Us is their return.";
  <http://localhost:3333/has_arabic_text> "إِنَّ إِلَيْنَآ إِيَابَهُمْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5993"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ghashiyah";
  dbp:text "Then indeed, upon Us is their account.";
  <http://localhost:3333/has_arabic_text> "ثُمَّ إِنَّ عَلَيْنَا حِسَابَهُم", "وُجُوهٌ يَوْمَئِذٍ خَٰشِعَةٌ",
    "وُجُوهٌ يَوْمَئِذٍ نَّاعِمَةٌ" .

<http://localhost:3333/Al-Fajr> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Fajr";
  dbo:ayah "5994"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5995"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "And [by] ten nights", "By the dawn";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5996"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "And [by] the even [number] and the odd";
  <http://localhost:3333/has_arabic_text> "وَلَيَالٍ عَشْرٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5997"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "And [by] the night when it passes,";
  <http://localhost:3333/has_arabic_text> "وَٱلشَّفْعِ وَٱلْوَتْرِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5998"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "Is there [not] in [all] that an oath [sufficient] for one of perception?";
  <http://localhost:3333/has_arabic_text> "هَلْ فِى ذَٰلِكَ قَسَمٌ لِّذِى حِجْرٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "5999"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "Have you not considered how your Lord dealt with 'Aad -";
  <http://localhost:3333/has_arabic_text> "أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِعَادٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6000"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "[With] Iram - who had lofty pillars,";
  <http://localhost:3333/has_arabic_text> "إِرَمَ ذَاتِ ٱلْعِمَادِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6001"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "The likes of whom had never been created in the land?";
  <http://localhost:3333/has_arabic_text> "وَٱلَّيْلِ إِذَا يَسْرِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6002"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "And [with] Thamud, who carved out the rocks in the valley?";
  <http://localhost:3333/has_arabic_text> "وَثَمُودَ ٱلَّذِينَ جَابُوا۟ ٱلصَّخْرَ بِٱلْوَادِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6003"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "And [with] Pharaoh, owner of the stakes? -";
  <http://localhost:3333/has_arabic_text> "وَفِرْعَوْنَ ذِى ٱلْأَوْتَادِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6004"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "[All of] whom oppressed within the lands";
  <http://localhost:3333/has_arabic_text> "وَٱلْفَجْرِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6005"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "And increased therein the corruption.";
  <http://localhost:3333/has_arabic_text> "فَأَكْثَرُوا۟ فِيهَا ٱلْفَسَادَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6006"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "So your Lord poured upon them a scourge of punishment.";
  <http://localhost:3333/has_arabic_text> "فَصَبَّ عَلَيْهِمْ رَبُّكَ سَوْطَ عَذَابٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6007"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "Indeed, your Lord is in observation.";
  <http://localhost:3333/has_arabic_text> "إِنَّ رَبَّكَ لَبِٱلْمِرْصَادِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6008"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "And as for man, when his Lord tries him and [thus] is generous to him and favors him, he says, \"My Lord has honored me.\"";
  <http://localhost:3333/has_arabic_text> "فَأَمَّا ٱلْإِنسَٰنُ إِذَا مَا ٱبْتَلَىٰهُ رَبُّهُۥ فَأَكْرَمَهُۥ وَنَعَّمَهُۥ فَيَقُولُ رَبِّىٓ أَكْرَمَنِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6009"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "But when He tries him and restricts his provision, he says, \"My Lord has humiliated me.\"";
  <http://localhost:3333/has_arabic_text> "وَأَمَّآ إِذَا مَا ٱبْتَلَىٰهُ فَقَدَرَ عَلَيْهِ رِزْقَهُۥ فَيَقُولُ رَبِّىٓ أَهَٰنَنِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6010"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "No! But you do not honor the orphan";
  <http://localhost:3333/has_arabic_text> "كَلَّا ۖ بَل لَّا تُكْرِمُونَ ٱلْيَتِيمَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6011"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "And you do not encourage one another to feed the poor.";
  <http://localhost:3333/has_arabic_text> "وَلَا تَحَٰٓضُّونَ عَلَىٰ طَعَامِ ٱلْمِسْكِينِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6012"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "And you consume inheritance, devouring [it] altogether,";
  <http://localhost:3333/has_arabic_text> "وَتَأْكُلُونَ ٱلتُّرَاثَ أَكْلًا لَّمًّا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6013"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "And you love wealth with immense love.";
  <http://localhost:3333/has_arabic_text> "وَتُحِبُّونَ ٱلْمَالَ حُبًّا جَمًّا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6014"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "No! When the earth has been leveled - pounded and crushed -";
  <http://localhost:3333/has_arabic_text> "كَلَّآ إِذَا دُكَّتِ ٱلْأَرْضُ دَكًّا دَكًّا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6015"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "And your Lord has come and the angels, rank upon rank,";
  <http://localhost:3333/has_arabic_text> "وَجَآءَ رَبُّكَ وَٱلْمَلَكُ صَفًّا صَفًّا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6016"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "And brought [within view], that Day, is Hell - that Day, man will remember, but what good to him will be the remembrance?";
  <http://localhost:3333/has_arabic_text> "وَجِا۟ىٓءَ يَوْمَئِذٍۭ بِجَهَنَّمَ ۚ يَوْمَئِذٍ يَتَذَكَّرُ ٱلْإِنسَٰنُ وَأَنَّىٰ لَهُ ٱلذِّكْرَىٰ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6017"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "He will say, \"Oh, I wish I had sent ahead [some good] for my life.\"";
  <http://localhost:3333/has_arabic_text> "يَقُولُ يَٰلَيْتَنِى قَدَّمْتُ لِحَيَاتِى";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6018"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "So on that Day, none will punish [as severely] as His punishment,";
  <http://localhost:3333/has_arabic_text> "فَيَوْمَئِذٍ لَّا يُعَذِّبُ عَذَابَهُۥٓ أَحَدٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6019"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "And none will bind [as severely] as His binding [of the evildoers].";
  <http://localhost:3333/has_arabic_text> "وَلَا يُوثِقُ وَثَاقَهُۥٓ أَحَدٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6020"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "[To the righteous it will be said], \"O reassured soul,";
  <http://localhost:3333/has_arabic_text> "يَٰٓأَيَّتُهَا ٱلنَّفْسُ ٱلْمُطْمَئِنَّةُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6021"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "Return to your Lord, well-pleased and pleasing [to Him],";
  <http://localhost:3333/has_arabic_text> "ٱرْجِعِىٓ إِلَىٰ رَبِّكِ رَاضِيَةً مَّرْضِيَّةً";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6022"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "And enter among My [righteous] servants";
  <http://localhost:3333/has_arabic_text> "فَٱدْخُلِى فِى عِبَٰدِى";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6023"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Fajr";
  dbp:text "And enter My Paradise.\"";
  <http://localhost:3333/has_arabic_text> "وَٱدْخُلِى جَنَّتِى", "ٱلَّتِى لَمْ يُخْلَقْ مِثْلُهَا فِى ٱلْبِلَٰدِ",
    "ٱلَّذِينَ طَغَوْا۟ فِى ٱلْبِلَٰدِ" .

<http://localhost:3333/Al-Balad> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Balad";
  dbo:ayah "6024"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6025"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Balad";
  dbp:text "And you, [O Muhammad], are free of restriction in this city -", "I swear by this city, Makkah -";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6026"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Balad";
  dbp:text "And [by] the father and that which was born [of him],";
  <http://localhost:3333/has_arabic_text> "لَآ أُقْسِمُ بِهَٰذَا ٱلْبَلَدِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6027"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Balad";
  dbp:text "We have certainly created man into hardship.";
  <http://localhost:3333/has_arabic_text> "لَقَدْ خَلَقْنَا ٱلْإِنسَٰنَ فِى كَبَدٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6028"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Balad";
  dbp:text "Does he think that never will anyone overcome him?";
  <http://localhost:3333/has_arabic_text> "أَيَحْسَبُ أَن لَّن يَقْدِرَ عَلَيْهِ أَحَدٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6029"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Balad";
  dbp:text "He says, \"I have spent wealth in abundance.\"";
  <http://localhost:3333/has_arabic_text> "وَأَنتَ حِلٌّۢ بِهَٰذَا ٱلْبَلَدِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6030"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Balad";
  dbp:text "Does he think that no one has seen him?";
  <http://localhost:3333/has_arabic_text> "أَيَحْسَبُ أَن لَّمْ يَرَهُۥٓ أَحَدٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6031"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Balad";
  dbp:text "Have We not made for him two eyes?";
  <http://localhost:3333/has_arabic_text> "أَلَمْ نَجْعَل لَّهُۥ عَيْنَيْنِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6032"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Balad";
  dbp:text "And a tongue and two lips?";
  <http://localhost:3333/has_arabic_text> "وَلِسَانًا وَشَفَتَيْنِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6033"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Balad";
  dbp:text "And have shown him the two ways?";
  <http://localhost:3333/has_arabic_text> "وَهَدَيْنَٰهُ ٱلنَّجْدَيْنِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6034"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Balad";
  dbp:text "But he has not broken through the difficult pass.";
  <http://localhost:3333/has_arabic_text> "فَلَا ٱقْتَحَمَ ٱلْعَقَبَةَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6035"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Balad";
  dbp:text "And what can make you know what is [breaking through] the difficult pass?";
  <http://localhost:3333/has_arabic_text> "وَمَآ أَدْرَىٰكَ مَا ٱلْعَقَبَةُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6036"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Balad";
  dbp:text "It is the freeing of a slave";
  <http://localhost:3333/has_arabic_text> "فَكُّ رَقَبَةٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6037"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Balad";
  dbp:text "Or feeding on a day of severe hunger";
  <http://localhost:3333/has_arabic_text> "أَوْ إِطْعَٰمٌ فِى يَوْمٍ ذِى مَسْغَبَةٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6038"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Balad";
  dbp:text "An orphan of near relationship";
  <http://localhost:3333/has_arabic_text> "وَوَالِدٍ وَمَا وَلَدَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6039"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Balad";
  dbp:text "Or a needy person in misery";
  <http://localhost:3333/has_arabic_text> "أَوْ مِسْكِينًا ذَا مَتْرَبَةٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6040"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Balad";
  dbp:text "And then being among those who believed and advised one another to patience and advised one another to compassion.";
  <http://localhost:3333/has_arabic_text> "ثُمَّ كَانَ مِنَ ٱلَّذِينَ ءَامَنُوا۟ وَتَوَاصَوْا۟ بِٱلصَّبْرِ وَتَوَاصَوْا۟ بِٱلْمَرْحَمَةِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6041"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Balad";
  dbp:text "Those are the companions of the right.";
  <http://localhost:3333/has_arabic_text> "أُو۟لَٰٓئِكَ أَصْحَٰبُ ٱلْمَيْمَنَةِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6042"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Balad";
  dbp:text "But they who disbelieved in Our signs - those are the companions of the left.";
  <http://localhost:3333/has_arabic_text> "وَٱلَّذِينَ كَفَرُوا۟ بِـَٔايَٰتِنَا هُمْ أَصْحَٰبُ ٱلْمَشْـَٔمَةِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6043"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Balad";
  dbp:text "Over them will be fire closed in.";
  <http://localhost:3333/has_arabic_text> "عَلَيْهِمْ نَارٌ مُّؤْصَدَةٌۢ", "يَتِيمًا ذَا مَقْرَبَةٍ",
    "يَقُولُ أَهْلَكْتُ مَالًا لُّبَدًا" .

<http://localhost:3333/Ash-Shams> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Ash-Shams";
  dbo:ayah "6044"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6045"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Ash-Shams";
  dbp:text "And [by] the moon when it follows it", "By the sun and its brightness";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6046"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Ash-Shams";
  dbp:text "And [by] the day when it displays it";
  <http://localhost:3333/has_arabic_text> "وَٱلشَّمْسِ وَضُحَىٰهَا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6047"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Ash-Shams";
  dbp:text "And [by] the night when it covers it";
  <http://localhost:3333/has_arabic_text> "وَٱلنَّهَارِ إِذَا جَلَّىٰهَا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6048"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Ash-Shams";
  dbp:text "And [by] the sky and He who constructed it";
  <http://localhost:3333/has_arabic_text> "وَٱلسَّمَآءِ وَمَا بَنَىٰهَا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6049"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Ash-Shams";
  dbp:text "And [by] the earth and He who spread it";
  <http://localhost:3333/has_arabic_text> "وَٱلَّيْلِ إِذَا يَغْشَىٰهَا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6050"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Ash-Shams";
  dbp:text "And [by] the soul and He who proportioned it";
  <http://localhost:3333/has_arabic_text> "وَنَفْسٍ وَمَا سَوَّىٰهَا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6051"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Ash-Shams";
  dbp:text "And inspired it [with discernment of] its wickedness and its righteousness,";
  <http://localhost:3333/has_arabic_text> "فَأَلْهَمَهَا فُجُورَهَا وَتَقْوَىٰهَا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6052"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Ash-Shams";
  dbp:text "He has succeeded who purifies it,";
  <http://localhost:3333/has_arabic_text> "قَدْ أَفْلَحَ مَن زَكَّىٰهَا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6053"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Ash-Shams";
  dbp:text "And he has failed who instills it [with corruption].";
  <http://localhost:3333/has_arabic_text> "وَقَدْ خَابَ مَن دَسَّىٰهَا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6054"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Ash-Shams";
  dbp:text "Thamud denied [their prophet] by reason of their transgression,";
  <http://localhost:3333/has_arabic_text> "كَذَّبَتْ ثَمُودُ بِطَغْوَىٰهَآ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6055"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Ash-Shams";
  dbp:text "When the most wretched of them was sent forth.";
  <http://localhost:3333/has_arabic_text> "إِذِ ٱنۢبَعَثَ أَشْقَىٰهَا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6056"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Ash-Shams";
  dbp:text "And the messenger of Allah [Salih] said to them, \"[Do not harm] the she-camel of Allah or [prevent her from] her drink.\"";
  <http://localhost:3333/has_arabic_text> "فَقَالَ لَهُمْ رَسُولُ ٱللَّهِ نَاقَةَ ٱللَّهِ وَسُقْيَٰهَا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6057"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Ash-Shams";
  dbp:text "But they denied him and hamstrung her. So their Lord brought down upon them destruction for their sin and made it equal [upon all of them].";
  <http://localhost:3333/has_arabic_text> "فَكَذَّبُوهُ فَعَقَرُوهَا فَدَمْدَمَ عَلَيْهِمْ رَبُّهُم بِذَنۢبِهِمْ فَسَوَّىٰهَا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6058"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Ash-Shams";
  dbp:text "And He does not fear the consequence thereof.";
  <http://localhost:3333/has_arabic_text> "وَلَا يَخَافُ عُقْبَٰهَا", "وَٱلْأَرْضِ وَمَا طَحَىٰهَا",
    "وَٱلْقَمَرِ إِذَا تَلَىٰهَا" .

<http://localhost:3333/Al-Lail> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Lail";
  dbo:ayah "6059"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6060"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Lail";
  dbp:text "And [by] the day when it appears", "By the night when it covers";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6061"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Lail";
  dbp:text "And [by] He who created the male and female,";
  <http://localhost:3333/has_arabic_text> "وَمَا خَلَقَ ٱلذَّكَرَ وَٱلْأُنثَىٰٓ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6062"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Lail";
  dbp:text "Indeed, your efforts are diverse.";
  <http://localhost:3333/has_arabic_text> "إِنَّ سَعْيَكُمْ لَشَتَّىٰ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6063"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Lail";
  dbp:text "As for he who gives and fears Allah";
  <http://localhost:3333/has_arabic_text> "فَأَمَّا مَنْ أَعْطَىٰ وَٱتَّقَىٰ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6064"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Lail";
  dbp:text "And believes in the best [reward],";
  <http://localhost:3333/has_arabic_text> "وَصَدَّقَ بِٱلْحُسْنَىٰ", "فَسَنُيَسِّرُهُۥ لِلْيُسْرَىٰ";
  a dbr:Ayah;
  dbo:ayah "6065"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat;
  dbp:text "We will ease him toward ease.";
  dbo:Surah "Al-Lail";
  dbp:text "But as for he who withholds and considers himself free of need";
  dbo:Surah "Al-Lail";
  a dbr:Ayat, dbr:Ayah, dbr:Quranic_verse;
  dbo:ayah "6066"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "وَأَمَّا مَنۢ بَخِلَ وَٱسْتَغْنَىٰ";
  a dbr:Ayat, dbr:Ayah;
  dbp:text "And denies the best [reward],";
  dbo:Surah "Al-Lail";
  dbo:ayah "6067"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "وَكَذَّبَ بِٱلْحُسْنَىٰ", "فَسَنُيَسِّرُهُۥ لِلْعُسْرَىٰ";
  a dbr:Ayah;
  dbp:text "We will ease him toward difficulty.";
  dbo:Surah "Al-Lail";
  dbo:ayah "6068"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse;
  dbp:text "And what will his wealth avail him when he falls?";
  dbo:Surah "Al-Lail";
  dbo:ayah "6069"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "وَمَا يُغْنِى عَنْهُ مَالُهُۥٓ إِذَا تَرَدَّىٰٓ",
    "إِنَّ عَلَيْنَا لَلْهُدَىٰ";
  dbp:text "Indeed, [incumbent] upon Us is guidance.";
  dbo:Surah "Al-Lail";
  dbo:ayah "6070"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "وَإِنَّ لَنَا لَلْءَاخِرَةَ وَٱلْأُولَىٰ";
  dbp:text "And indeed, to Us belongs the Hereafter and the first [life].";
  dbo:Surah "Al-Lail";
  dbo:ayah "6071"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "فَأَنذَرْتُكُمْ نَارًا تَلَظَّىٰ";
  dbp:text "So I have warned you of a Fire which is blazing.";
  dbo:Surah "Al-Lail";
  dbo:ayah "6072"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Ayah, dbr:Quranic_verse;
  dbp:text "None will [enter to] burn therein except the most wretched one.";
  dbo:Surah "Al-Lail";
  dbo:ayah "6073"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Ayah, dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "لَا يَصْلَىٰهَآ إِلَّا ٱلْأَشْقَى", "وَٱلنَّهَارِ إِذَا تَجَلَّىٰ";
  dbp:text "Who had denied and turned away.";
  dbo:Surah "Al-Lail";
  dbo:ayah "6074"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbp:text "But the righteous one will avoid it -";
  dbo:Surah "Al-Lail";
  dbo:ayah "6075"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Ayah, dbr:Quranic_verse;
  <http://localhost:3333/has_arabic_text> "وَسَيُجَنَّبُهَا ٱلْأَتْقَى", "وَٱلَّيْلِ إِذَا يَغْشَىٰ";
  dbp:text "[He] who gives [from] his wealth to purify himself";
  dbo:Surah "Al-Lail";
  dbo:ayah "6076"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "وَمَا لِأَحَدٍ عِندَهُۥ مِن نِّعْمَةٍ تُجْزَىٰٓ";
  dbp:text "And not [giving] for anyone who has [done him] a favor to be rewarded";
  dbo:Surah "Al-Lail";
  dbo:ayah "6077"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat, dbr:Ayah;
  dbp:text "But only seeking the countenance of his Lord, Most High.";
  dbo:Surah "Al-Lail";
  dbo:ayah "6078"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "إِلَّا ٱبْتِغَآءَ وَجْهِ رَبِّهِ ٱلْأَعْلَىٰ",
    "وَلَسَوْفَ يَرْضَىٰ";
  dbp:text "And he is going to be satisfied.";
  dbo:Surah "Al-Lail";
  dbo:ayah "6079"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "ٱلَّذِى كَذَّبَ وَتَوَلَّىٰ", "ٱلَّذِى يُؤْتِى مَالَهُۥ يَتَزَكَّىٰ" .

<http://localhost:3333/Ad-Dhuha> dbo:Surah "Ad-Dhuha";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "6080"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "By the morning brightness", "And [by] the night when it covers with darkness,";
  dbo:Surah "Ad-Dhuha";
  dbo:ayah "6081"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "مَا وَدَّعَكَ رَبُّكَ وَمَا قَلَىٰ";
  dbp:text "Your Lord has not taken leave of you, [O Muhammad], nor has He detested [you].";
  dbo:Surah "Ad-Dhuha";
  dbo:ayah "6082"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah, dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:ayah "6083"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Ad-Dhuha";
  dbp:text "And the Hereafter is better for you than the first [life].";
  <http://localhost:3333/has_arabic_text> "وَلَلْءَاخِرَةُ خَيْرٌ لَّكَ مِنَ ٱلْأُولَىٰ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6084"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Ad-Dhuha";
  dbp:text "And your Lord is going to give you, and you will be satisfied.";
  <http://localhost:3333/has_arabic_text> "وَلَسَوْفَ يُعْطِيكَ رَبُّكَ فَتَرْضَىٰٓ";
  dbo:ayah "6085"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "Did He not find you an orphan and give [you] refuge?";
  a dbr:Ayat;
  dbo:Surah "Ad-Dhuha";
  a dbr:Quranic_verse, dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "أَلَمْ يَجِدْكَ يَتِيمًا فَـَٔاوَىٰ";
  dbo:ayah "6086"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayah;
  dbp:text "And He found you lost and guided [you],";
  dbo:Surah "Ad-Dhuha";
  a dbr:Ayat;
  <http://localhost:3333/has_arabic_text> "وَوَجَدَكَ ضَآلًّا فَهَدَىٰ", "وَوَجَدَكَ عَآئِلًا فَأَغْنَىٰ";
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbp:text "And He found you poor and made [you] self-sufficient.";
  dbo:Surah "Ad-Dhuha";
  dbo:ayah "6087"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Ayah, dbr:Quranic_verse;
  dbp:text "So as for the orphan, do not oppress [him].";
  dbo:Surah "Ad-Dhuha";
  dbo:ayah "6088"^^<http://www.w3.org/2001/XMLSchema#int>;
  <http://localhost:3333/has_arabic_text> "فَأَمَّا ٱلْيَتِيمَ فَلَا تَقْهَرْ";
  dbo:ayah "6089"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Quranic_verse, dbr:Ayah;
  dbo:Surah "Ad-Dhuha";
  dbp:text "And as for the petitioner, do not repel [him].";
  <http://localhost:3333/has_arabic_text> "وَأَمَّا ٱلسَّآئِلَ فَلَا تَنْهَرْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6090"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Ad-Dhuha";
  dbp:text "But as for the favor of your Lord, report [it].";
  <http://localhost:3333/has_arabic_text> "وَأَمَّا بِنِعْمَةِ رَبِّكَ فَحَدِّثْ", "وَٱلضُّحَىٰ",
    "وَٱلَّيْلِ إِذَا سَجَىٰ" .

<http://localhost:3333/Al-Inshirah> a dbr:Ayah;
  dbo:Surah "Al-Inshirah";
  a dbr:Ayat, dbr:Quranic_verse;
  dbo:ayah "6091"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Quranic_verse, dbr:Ayat, dbr:Ayah;
  dbo:ayah "6092"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Inshirah";
  dbp:text "And We removed from you your burden", "Did We not expand for you, [O Muhammad], your breast?";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6093"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Inshirah";
  dbp:text "Which had weighed upon your back";
  <http://localhost:3333/has_arabic_text> "أَلَمْ نَشْرَحْ لَكَ صَدْرَكَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6094"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Inshirah";
  dbp:text "And raised high for you your repute.";
  <http://localhost:3333/has_arabic_text> "وَرَفَعْنَا لَكَ ذِكْرَكَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6095"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Inshirah";
  dbp:text "For indeed, with hardship [will be] ease.";
  <http://localhost:3333/has_arabic_text> "فَإِنَّ مَعَ ٱلْعُسْرِ يُسْرًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6096"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Inshirah";
  dbp:text "Indeed, with hardship [will be] ease.";
  <http://localhost:3333/has_arabic_text> "إِنَّ مَعَ ٱلْعُسْرِ يُسْرًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6097"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Inshirah";
  dbp:text "So when you have finished [your duties], then stand up [for worship].";
  <http://localhost:3333/has_arabic_text> "فَإِذَا فَرَغْتَ فَٱنصَبْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbp:text "And to your Lord direct [your] longing.";
  dbo:ayah "6098"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Inshirah";
  <http://localhost:3333/has_arabic_text> "وَإِلَىٰ رَبِّكَ فَٱرْغَب", "وَوَضَعْنَا عَنكَ وِزْرَكَ",
    "ٱلَّذِىٓ أَنقَضَ ظَهْرَكَ" .

<http://localhost:3333/Al-Teen> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Teen";
  dbo:ayah "6099"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6100"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Teen";
  dbp:text "And [by] Mount Sinai", "By the fig and the olive";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6101"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Teen";
  dbp:text "And [by] this secure city [Makkah],";
  <http://localhost:3333/has_arabic_text> "بِّسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ وَٱلتِّينِ وَٱلزَّيْتُونِ";
  dbo:ayah "6102"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat, dbr:Ayah, dbr:Quranic_verse;
  dbo:Surah "Al-Teen";
  dbp:text "We have certainly created man in the best of stature;";
  <http://localhost:3333/has_arabic_text> "لَقَدْ خَلَقْنَا ٱلْإِنسَٰنَ فِىٓ أَحْسَنِ تَقْوِيمٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6103"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Teen";
  dbp:text "Then We return him to the lowest of the low,";
  <http://localhost:3333/has_arabic_text> "ثُمَّ رَدَدْنَٰهُ أَسْفَلَ سَٰفِلِينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6104"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Teen";
  dbp:text "Except for those who believe and do righteous deeds, for they will have a reward uninterrupted.";
  <http://localhost:3333/has_arabic_text> "إِلَّا ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّٰلِحَٰتِ فَلَهُمْ أَجْرٌ غَيْرُ مَمْنُونٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6105"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Teen";
  dbp:text "So what yet causes you to deny the Recompense?";
  <http://localhost:3333/has_arabic_text> "فَمَا يُكَذِّبُكَ بَعْدُ بِٱلدِّينِ";
  a dbr:Ayah, dbr:Quranic_verse;
  dbo:ayah "6106"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat;
  dbo:Surah "Al-Teen";
  dbp:text "Is not Allah the most just of judges?";
  <http://localhost:3333/has_arabic_text> "أَلَيْسَ ٱللَّهُ بِأَحْكَمِ ٱلْحَٰكِمِينَ",
    "وَطُورِ سِينِينَ", "وَهَٰذَا ٱلْبَلَدِ ٱلْأَمِينِ" .

<http://localhost:3333/al-%60Alaq> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "al-`Alaq";
  dbo:ayah "6107"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat;
  dbo:ayah "6108"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "al-`Alaq";
  dbp:text "Created man from a clinging substance.";
  a dbr:Ayah, dbr:Quranic_verse;
  dbp:text "Recite in the name of your Lord who created -";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6109"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "al-`Alaq";
  dbp:text "Recite, and your Lord is the most Generous -";
  <http://localhost:3333/has_arabic_text> "خَلَقَ ٱلْإِنسَٰنَ مِنْ عَلَقٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6110"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "al-`Alaq";
  dbp:text "Who taught by the pen -";
  <http://localhost:3333/has_arabic_text> "ٱقْرَأْ بِٱسْمِ رَبِّكَ ٱلَّذِى خَلَقَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6111"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "al-`Alaq";
  dbp:text "Taught man that which he knew not.";
  <http://localhost:3333/has_arabic_text> "عَلَّمَ ٱلْإِنسَٰنَ مَا لَمْ يَعْلَمْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6112"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "al-`Alaq";
  dbp:text "No! [But] indeed, man transgresses";
  <http://localhost:3333/has_arabic_text> "كَلَّآ إِنَّ ٱلْإِنسَٰنَ لَيَطْغَىٰٓ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6113"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "al-`Alaq";
  dbp:text "Because he sees himself self-sufficient.";
  <http://localhost:3333/has_arabic_text> "أَن رَّءَاهُ ٱسْتَغْنَىٰٓ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6114"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "al-`Alaq";
  dbp:text "Indeed, to your Lord is the return.";
  <http://localhost:3333/has_arabic_text> "إِنَّ إِلَىٰ رَبِّكَ ٱلرُّجْعَىٰٓ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6115"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "al-`Alaq";
  dbp:text "Have you seen the one who forbids";
  <http://localhost:3333/has_arabic_text> "أَرَءَيْتَ ٱلَّذِى يَنْهَىٰ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6116"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "al-`Alaq";
  dbp:text "A servant when he prays?";
  <http://localhost:3333/has_arabic_text> "عَبْدًا إِذَا صَلَّىٰٓ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6117"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "al-`Alaq";
  dbp:text "Have you seen if he is upon guidance";
  <http://localhost:3333/has_arabic_text> "أَرَءَيْتَ إِن كَانَ عَلَى ٱلْهُدَىٰٓ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6118"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "al-`Alaq";
  dbp:text "Or enjoins righteousness?";
  <http://localhost:3333/has_arabic_text> "أَوْ أَمَرَ بِٱلتَّقْوَىٰٓ";
  a dbr:Quranic_verse;
  dbp:text "Have you seen if he denies and turns away -";
  dbo:ayah "6119"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayat;
  dbo:Surah "al-`Alaq";
  a dbr:Ayah;
  <http://localhost:3333/has_arabic_text> "أَرَءَيْتَ إِن كَذَّبَ وَتَوَلَّىٰٓ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6120"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "al-`Alaq";
  dbp:text "Does he not know that Allah sees?";
  <http://localhost:3333/has_arabic_text> "أَلَمْ يَعْلَم بِأَنَّ ٱللَّهَ يَرَىٰ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6121"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "al-`Alaq";
  dbp:text "No! If he does not desist, We will surely drag him by the forelock -";
  <http://localhost:3333/has_arabic_text> "كَلَّا لَئِن لَّمْ يَنتَهِ لَنَسْفَعًۢا بِٱلنَّاصِيَةِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6122"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "al-`Alaq";
  dbp:text "A lying, sinning forelock.";
  <http://localhost:3333/has_arabic_text> "نَاصِيَةٍ كَٰذِبَةٍ خَاطِئَةٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6123"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "al-`Alaq";
  dbp:text "Then let him call his associates;";
  <http://localhost:3333/has_arabic_text> "فَلْيَدْعُ نَادِيَهُۥ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6124"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "al-`Alaq";
  dbp:text "We will call the angels of Hell.";
  <http://localhost:3333/has_arabic_text> "سَنَدْعُ ٱلزَّبَانِيَةَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6125"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "al-`Alaq";
  dbp:text "No! Do not obey him. But prostrate and draw near [to Allah].";
  <http://localhost:3333/has_arabic_text> "كَلَّا لَا تُطِعْهُ وَٱسْجُدْ وَٱقْتَرِب ۩",
    "ٱقْرَأْ وَرَبُّكَ ٱلْأَكْرَمُ", "ٱلَّذِى عَلَّمَ بِٱلْقَلَمِ" .

<http://localhost:3333/Al-Qadr> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Qadr";
  dbo:ayah "6126"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6127"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qadr";
  dbp:text "And what can make you know what is the Night of Decree?", "Indeed, We sent the Qur'an down during the Night of Decree.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6128"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qadr";
  dbp:text "The Night of Decree is better than a thousand months.";
  <http://localhost:3333/has_arabic_text> "بِّسْمِ ٱللَّهِ ٱلرَّحْمَٰنِ ٱلرَّحِيمِ إِنَّآ أَنزَلْنَٰهُ فِى لَيْلَةِ ٱلْقَدْرِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6129"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qadr";
  dbp:text "The angels and the Spirit descend therein by permission of their Lord for every matter.";
  <http://localhost:3333/has_arabic_text> "تَنَزَّلُ ٱلْمَلَٰٓئِكَةُ وَٱلرُّوحُ فِيهَا بِإِذْنِ رَبِّهِم مِّن كُلِّ أَمْرٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6130"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Qadr";
  dbp:text "Peace it is until the emergence of dawn.";
  <http://localhost:3333/has_arabic_text> "سَلَٰمٌ هِىَ حَتَّىٰ مَطْلَعِ ٱلْفَجْرِ",
    "لَيْلَةُ ٱلْقَدْرِ خَيْرٌ مِّنْ أَلْفِ شَهْرٍ", "وَمَآ أَدْرَىٰكَ مَا لَيْلَةُ ٱلْقَدْرِ" .

<http://localhost:3333/Al-Bayyinah> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Bayyinah";
  dbo:ayah "6131"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6132"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Bayyinah";
  dbp:text "A Messenger from Allah, reciting purified scriptures", "Those who disbelieved among the People of the Scripture and the polytheists were not to be parted [from misbelief] until there came to them clear evidence -";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6133"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Bayyinah";
  dbp:text "Within which are correct writings.";
  <http://localhost:3333/has_arabic_text> "رَسُولٌ مِّنَ ٱللَّهِ يَتْلُوا۟ صُحُفًا مُّطَهَّرَةً";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6134"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Bayyinah";
  dbp:text "Nor did those who were given the Scripture become divided until after there had come to them clear evidence.";
  <http://localhost:3333/has_arabic_text> "فِيهَا كُتُبٌ قَيِّمَةٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6135"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Bayyinah";
  dbp:text "And they were not commanded except to worship Allah, [being] sincere to Him in religion, inclining to truth, and to establish prayer and to give zakah. And that is the correct religion.";
  <http://localhost:3333/has_arabic_text> "لَمْ يَكُنِ ٱلَّذِينَ كَفَرُوا۟ مِنْ أَهْلِ ٱلْكِتَٰبِ وَٱلْمُشْرِكِينَ مُنفَكِّينَ حَتَّىٰ تَأْتِيَهُمُ ٱلْبَيِّنَةُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6136"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Bayyinah";
  dbp:text "Indeed, they who disbelieved among the People of the Scripture and the polytheists will be in the fire of Hell, abiding eternally therein. Those are the worst of creatures.";
  <http://localhost:3333/has_arabic_text> "إِنَّ ٱلَّذِينَ كَفَرُوا۟ مِنْ أَهْلِ ٱلْكِتَٰبِ وَٱلْمُشْرِكِينَ فِى نَارِ جَهَنَّمَ خَٰلِدِينَ فِيهَآ ۚ أُو۟لَٰٓئِكَ هُمْ شَرُّ ٱلْبَرِيَّةِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6137"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Bayyinah";
  dbp:text "Indeed, they who have believed and done righteous deeds - those are the best of creatures.";
  <http://localhost:3333/has_arabic_text> "إِنَّ ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّٰلِحَٰتِ أُو۟لَٰٓئِكَ هُمْ خَيْرُ ٱلْبَرِيَّةِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6138"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Bayyinah";
  dbp:text "Their reward with Allah will be gardens of perpetual residence beneath which rivers flow, wherein they will abide forever, Allah being pleased with them and they with Him. That is for whoever has feared his Lord.";
  <http://localhost:3333/has_arabic_text> "جَزَآؤُهُمْ عِندَ رَبِّهِمْ جَنَّٰتُ عَدْنٍ تَجْرِى مِن تَحْتِهَا ٱلْأَنْهَٰرُ خَٰلِدِينَ فِيهَآ أَبَدًا ۖ رَّضِىَ ٱللَّهُ عَنْهُمْ وَرَضُوا۟ عَنْهُ ۚ ذَٰلِكَ لِمَنْ خَشِىَ رَبَّهُۥ",
    "وَمَا تَفَرَّقَ ٱلَّذِينَ أُوتُوا۟ ٱلْكِتَٰبَ إِلَّا مِنۢ بَعْدِ مَا جَآءَتْهُمُ ٱلْبَيِّنَةُ",
    "وَمَآ أُمِرُوٓا۟ إِلَّا لِيَعْبُدُوا۟ ٱللَّهَ مُخْلِصِينَ لَهُ ٱلدِّينَ حُنَفَآءَ وَيُقِيمُوا۟ ٱلصَّلَوٰةَ وَيُؤْتُوا۟ ٱلزَّكَوٰةَ ۚ وَذَٰلِكَ دِينُ ٱلْقَيِّمَةِ" .

<http://localhost:3333/Az-Zalzala> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Az-Zalzala";
  dbo:ayah "6139"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6140"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Az-Zalzala";
  dbp:text "And the earth discharges its burdens", "When the earth is shaken with its [final] earthquake";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6141"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Az-Zalzala";
  dbp:text "And man says, \"What is [wrong] with it?\" -";
  <http://localhost:3333/has_arabic_text> "إِذَا زُلْزِلَتِ ٱلْأَرْضُ زِلْزَالَهَا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6142"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Az-Zalzala";
  dbp:text "That Day, it will report its news";
  <http://localhost:3333/has_arabic_text> "وَأَخْرَجَتِ ٱلْأَرْضُ أَثْقَالَهَا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6143"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Az-Zalzala";
  dbp:text "Because your Lord has commanded it.";
  <http://localhost:3333/has_arabic_text> "بِأَنَّ رَبَّكَ أَوْحَىٰ لَهَا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6144"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Az-Zalzala";
  dbp:text "That Day, the people will depart separated [into categories] to be shown [the result of] their deeds.";
  <http://localhost:3333/has_arabic_text> "وَقَالَ ٱلْإِنسَٰنُ مَا لَهَا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6145"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Az-Zalzala";
  dbp:text "So whoever does an atom's weight of good will see it,";
  <http://localhost:3333/has_arabic_text> "فَمَن يَعْمَلْ مِثْقَالَ ذَرَّةٍ خَيْرًا يَرَهُۥ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6146"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Az-Zalzala";
  dbp:text "And whoever does an atom's weight of evil will see it.";
  <http://localhost:3333/has_arabic_text> "وَمَن يَعْمَلْ مِثْقَالَ ذَرَّةٍ شَرًّا يَرَهُۥ",
    "يَوْمَئِذٍ تُحَدِّثُ أَخْبَارَهَا", "يَوْمَئِذٍ يَصْدُرُ ٱلنَّاسُ أَشْتَاتًا لِّيُرَوْا۟ أَعْمَٰلَهُمْ" .

<http://localhost:3333/Al-Adiyat> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Adiyat";
  dbo:ayah "6147"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6148"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Adiyat";
  dbp:text "And the producers of sparks [when] striking", "By the racers, panting,";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6149"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Adiyat";
  dbp:text "And the chargers at dawn,";
  <http://localhost:3333/has_arabic_text> "فَٱلْمُغِيرَٰتِ صُبْحًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6150"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Adiyat";
  dbp:text "Stirring up thereby [clouds of] dust,";
  <http://localhost:3333/has_arabic_text> "فَأَثَرْنَ بِهِۦ نَقْعًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6151"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Adiyat";
  dbp:text "Arriving thereby in the center collectively,";
  <http://localhost:3333/has_arabic_text> "فَوَسَطْنَ بِهِۦ جَمْعًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6152"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Adiyat";
  dbp:text "Indeed mankind, to his Lord, is ungrateful.";
  <http://localhost:3333/has_arabic_text> "إِنَّ ٱلْإِنسَٰنَ لِرَبِّهِۦ لَكَنُودٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6153"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Adiyat";
  dbp:text "And indeed, he is to that a witness.";
  <http://localhost:3333/has_arabic_text> "فَٱلْمُورِيَٰتِ قَدْحًا";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6154"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Adiyat";
  dbp:text "And indeed he is, in love of wealth, intense.";
  <http://localhost:3333/has_arabic_text> "وَإِنَّهُۥ عَلَىٰ ذَٰلِكَ لَشَهِيدٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6155"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Adiyat";
  dbp:text "But does he not know that when the contents of the graves are scattered";
  <http://localhost:3333/has_arabic_text> "أَفَلَا يَعْلَمُ إِذَا بُعْثِرَ مَا فِى ٱلْقُبُورِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6156"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Adiyat";
  dbp:text "And that within the breasts is obtained,";
  <http://localhost:3333/has_arabic_text> "وَإِنَّهُۥ لِحُبِّ ٱلْخَيْرِ لَشَدِيدٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6157"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Adiyat";
  dbp:text "Indeed, their Lord with them, that Day, is [fully] Acquainted.";
  <http://localhost:3333/has_arabic_text> "إِنَّ رَبَّهُم بِهِمْ يَوْمَئِذٍ لَّخَبِيرٌۢ",
    "وَحُصِّلَ مَا فِى ٱلصُّدُورِ", "وَٱلْعَٰدِيَٰتِ ضَبْحًا" .

<http://localhost:3333/al-Qari%60ah> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "al-Qari`ah";
  dbo:ayah "6158"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6159"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "al-Qari`ah";
  dbp:text "The Striking Calamity -", "What is the Striking Calamity?";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6160"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "al-Qari`ah";
  dbp:text "And what can make you know what is the Striking Calamity?";
  <http://localhost:3333/has_arabic_text> "مَا ٱلْقَارِعَةُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6161"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "al-Qari`ah";
  dbp:text "It is the Day when people will be like moths, dispersed,";
  <http://localhost:3333/has_arabic_text> "وَمَآ أَدْرَىٰكَ مَا ٱلْقَارِعَةُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6162"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "al-Qari`ah";
  dbp:text "And the mountains will be like wool, fluffed up.";
  <http://localhost:3333/has_arabic_text> "وَتَكُونُ ٱلْجِبَالُ كَٱلْعِهْنِ ٱلْمَنفُوشِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6163"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "al-Qari`ah";
  dbp:text "Then as for one whose scales are heavy [with good deeds],";
  <http://localhost:3333/has_arabic_text> "فَأَمَّا مَن ثَقُلَتْ مَوَٰزِينُهُۥ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6164"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "al-Qari`ah";
  dbp:text "He will be in a pleasant life.";
  <http://localhost:3333/has_arabic_text> "فَهُوَ فِى عِيشَةٍ رَّاضِيَةٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6165"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "al-Qari`ah";
  dbp:text "But as for one whose scales are light,";
  <http://localhost:3333/has_arabic_text> "وَأَمَّا مَنْ خَفَّتْ مَوَٰزِينُهُۥ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6166"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "al-Qari`ah";
  dbp:text "His refuge will be an abyss.";
  <http://localhost:3333/has_arabic_text> "فَأُمُّهُۥ هَاوِيَةٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6167"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "al-Qari`ah";
  dbp:text "And what can make you know what that is?";
  <http://localhost:3333/has_arabic_text> "وَمَآ أَدْرَىٰكَ مَا هِيَهْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6168"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "al-Qari`ah";
  dbp:text "It is a Fire, intensely hot.";
  <http://localhost:3333/has_arabic_text> "نَارٌ حَامِيَةٌۢ", "يَوْمَ يَكُونُ ٱلنَّاسُ كَٱلْفَرَاشِ ٱلْمَبْثُوثِ",
    "ٱلْقَارِعَةُ" .

<http://localhost:3333/At-Takathur> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "At-Takathur";
  dbo:ayah "6169"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6170"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takathur";
  dbp:text "Competition in [worldly] increase diverts you", "Until you visit the graveyards.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6171"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takathur";
  dbp:text "No! You are going to know.";
  <http://localhost:3333/has_arabic_text> "أَلْهَىٰكُمُ ٱلتَّكَاثُرُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6172"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takathur";
  dbp:text "Then no! You are going to know.";
  <http://localhost:3333/has_arabic_text> "ثُمَّ كَلَّا سَوْفَ تَعْلَمُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6173"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takathur";
  dbp:text "No! If you only knew with knowledge of certainty...";
  <http://localhost:3333/has_arabic_text> "حَتَّىٰ زُرْتُمُ ٱلْمَقَابِرَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6174"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takathur";
  dbp:text "You will surely see the Hellfire.";
  <http://localhost:3333/has_arabic_text> "كَلَّا سَوْفَ تَعْلَمُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6175"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takathur";
  dbp:text "Then you will surely see it with the eye of certainty.";
  <http://localhost:3333/has_arabic_text> "ثُمَّ لَتَرَوُنَّهَا عَيْنَ ٱلْيَقِينِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6176"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "At-Takathur";
  dbp:text "Then you will surely be asked that Day about pleasure.";
  <http://localhost:3333/has_arabic_text> "ثُمَّ لَتُسْـَٔلُنَّ يَوْمَئِذٍ عَنِ ٱلنَّعِيمِ",
    "كَلَّا لَوْ تَعْلَمُونَ عِلْمَ ٱلْيَقِينِ", "لَتَرَوُنَّ ٱلْجَحِيمَ" .

<http://localhost:3333/Al-Asr> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Asr";
  dbo:ayah "6177"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6178"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Asr";
  dbp:text "By time,", "Indeed, mankind is in loss,";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6179"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Asr";
  dbp:text "Except for those who have believed and done righteous deeds and advised each other to truth and advised each other to patience.";
  <http://localhost:3333/has_arabic_text> "إِلَّا ٱلَّذِينَ ءَامَنُوا۟ وَعَمِلُوا۟ ٱلصَّٰلِحَٰتِ وَتَوَاصَوْا۟ بِٱلْحَقِّ وَتَوَاصَوْا۟ بِٱلصَّبْرِ",
    "إِنَّ ٱلْإِنسَٰنَ لَفِى خُسْرٍ", "وَٱلْعَصْرِ" .

<http://localhost:3333/Al-Humazah> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Humazah";
  dbo:ayah "6180"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6181"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Humazah";
  dbp:text "Who collects wealth and [continuously] counts it.", "Woe to every scorner and mocker";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6182"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Humazah";
  dbp:text "He thinks that his wealth will make him immortal.";
  <http://localhost:3333/has_arabic_text> "وَيْلٌ لِّكُلِّ هُمَزَةٍ لُّمَزَةٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6183"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Humazah";
  dbp:text "No! He will surely be thrown into the Crusher.";
  <http://localhost:3333/has_arabic_text> "كَلَّا ۖ لَيُنۢبَذَنَّ فِى ٱلْحُطَمَةِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6184"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Humazah";
  dbp:text "And what can make you know what is the Crusher?";
  <http://localhost:3333/has_arabic_text> "وَمَآ أَدْرَىٰكَ مَا ٱلْحُطَمَةُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6185"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Humazah";
  dbp:text "It is the fire of Allah, [eternally] fueled,";
  <http://localhost:3333/has_arabic_text> "نَارُ ٱللَّهِ ٱلْمُوقَدَةُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6186"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Humazah";
  dbp:text "Which mounts directed at the hearts.";
  <http://localhost:3333/has_arabic_text> "يَحْسَبُ أَنَّ مَالَهُۥٓ أَخْلَدَهُۥ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6187"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Humazah";
  dbp:text "Indeed, Hellfire will be closed down upon them";
  <http://localhost:3333/has_arabic_text> "إِنَّهَا عَلَيْهِم مُّؤْصَدَةٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6188"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Humazah";
  dbp:text "In extended columns.";
  <http://localhost:3333/has_arabic_text> "فِى عَمَدٍ مُّمَدَّدَةٍۭ", "ٱلَّتِى تَطَّلِعُ عَلَى ٱلْأَفْـِٔدَةِ",
    "ٱلَّذِى جَمَعَ مَالًا وَعَدَّدَهُۥ" .

<http://localhost:3333/Al-Feel> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Feel";
  dbo:ayah "6189"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6190"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Feel";
  dbp:text "Did He not make their plan into misguidance?", "Have you not considered, [O Muhammad], how your Lord dealt with the companions of the elephant?";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6191"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Feel";
  dbp:text "And He sent against them birds in flocks,";
  <http://localhost:3333/has_arabic_text> "أَلَمْ تَرَ كَيْفَ فَعَلَ رَبُّكَ بِأَصْحَٰبِ ٱلْفِيلِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6192"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Feel";
  dbp:text "Striking them with stones of hard clay,";
  <http://localhost:3333/has_arabic_text> "أَلَمْ يَجْعَلْ كَيْدَهُمْ فِى تَضْلِيلٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6193"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Feel";
  dbp:text "And He made them like eaten straw.";
  <http://localhost:3333/has_arabic_text> "تَرْمِيهِم بِحِجَارَةٍ مِّن سِجِّيلٍ", "فَجَعَلَهُمْ كَعَصْفٍ مَّأْكُولٍۭ",
    "وَأَرْسَلَ عَلَيْهِمْ طَيْرًا أَبَابِيلَ" .

<http://localhost:3333/Al-Quraish> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Quraish";
  dbo:ayah "6194"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6195"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Quraish";
  dbp:text "For the accustomed security of the Quraysh -", "Their accustomed security [in] the caravan of winter and summer -";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6196"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Quraish";
  dbp:text "Let them worship the Lord of this House,";
  <http://localhost:3333/has_arabic_text> "إِۦلَٰفِهِمْ رِحْلَةَ ٱلشِّتَآءِ وَٱلصَّيْفِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6197"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Quraish";
  dbp:text "Who has fed them, [saving them] from hunger and made them safe, [saving them] from fear.";
  <http://localhost:3333/has_arabic_text> "فَلْيَعْبُدُوا۟ رَبَّ هَٰذَا ٱلْبَيْتِ",
    "لِإِيلَٰفِ قُرَيْشٍ", "ٱلَّذِىٓ أَطْعَمَهُم مِّن جُوعٍ وَءَامَنَهُم مِّنْ خَوْفٍۭ" .

<http://localhost:3333/Al-Maa'oun> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Maa'oun";
  dbo:ayah "6198"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6199"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Maa'oun";
  dbp:text "For that is the one who drives away the orphan", "Have you seen the one who denies the Recompense?";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6200"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Maa'oun";
  dbp:text "And does not encourage the feeding of the poor.";
  <http://localhost:3333/has_arabic_text> "أَرَءَيْتَ ٱلَّذِى يُكَذِّبُ بِٱلدِّينِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6201"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Maa'oun";
  dbp:text "So woe to those who pray";
  <http://localhost:3333/has_arabic_text> "فَذَٰلِكَ ٱلَّذِى يَدُعُّ ٱلْيَتِيمَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6202"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Maa'oun";
  dbp:text "[But] who are heedless of their prayer -";
  <http://localhost:3333/has_arabic_text> "فَوَيْلٌ لِّلْمُصَلِّينَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6203"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Maa'oun";
  dbp:text "Those who make show [of their deeds]";
  <http://localhost:3333/has_arabic_text> "وَلَا يَحُضُّ عَلَىٰ طَعَامِ ٱلْمِسْكِينِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6204"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Maa'oun";
  dbp:text "And withhold [simple] assistance.";
  <http://localhost:3333/has_arabic_text> "وَيَمْنَعُونَ ٱلْمَاعُونَ", "ٱلَّذِينَ هُمْ عَن صَلَاتِهِمْ سَاهُونَ",
    "ٱلَّذِينَ هُمْ يُرَآءُونَ" .

<http://localhost:3333/Al-Kawthar> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Kawthar";
  dbo:ayah "6205"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6206"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Kawthar";
  dbp:text "Indeed, We have granted you, [O Muhammad], al-Kawthar.", "So pray to your Lord and sacrifice [to Him alone].";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6207"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Kawthar";
  dbp:text "Indeed, your enemy is the one cut off.";
  <http://localhost:3333/has_arabic_text> "إِنَّ شَانِئَكَ هُوَ ٱلْأَبْتَرُ", "إِنَّآ أَعْطَيْنَٰكَ ٱلْكَوْثَرَ",
    "فَصَلِّ لِرَبِّكَ وَٱنْحَرْ" .

<http://localhost:3333/Al-Kafiroun> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Kafiroun";
  dbo:ayah "6208"^^<http://www.w3.org/2001/XMLSchema#int>;
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6209"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Kafiroun";
  dbp:text "I do not worship what you worship.", "Say, \"O disbelievers,";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6210"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Kafiroun";
  dbp:text "Nor are you worshippers of what I worship.";
  <http://localhost:3333/has_arabic_text> "قُلْ يَٰٓأَيُّهَا ٱلْكَٰفِرُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6211"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Kafiroun";
  dbp:text "Nor will I be a worshipper of what you worship.";
  <http://localhost:3333/has_arabic_text> "لَآ أَعْبُدُ مَا تَعْبُدُونَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6212"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Kafiroun";
  dbp:text "Nor will you be worshippers of what I worship.";
  <http://localhost:3333/has_arabic_text> "وَلَآ أَنتُمْ عَٰبِدُونَ مَآ أَعْبُدُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6213"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Kafiroun";
  dbp:text "For you is your religion, and for me is my religion.\"";
  <http://localhost:3333/has_arabic_text> "لَكُمْ دِينُكُمْ وَلِىَ دِينِ", "وَلَآ أَنَا۠ عَابِدٌ مَّا عَبَدتُّمْ" .

<http://localhost:3333/An-Nasr> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "An-Nasr";
  dbo:ayah "6214"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "When the victory of Allah has come and the conquest,";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6215"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nasr";
  dbp:text "And you see the people entering into the religion of Allah in multitudes,";
  <http://localhost:3333/has_arabic_text> "إِذَا جَآءَ نَصْرُ ٱللَّهِ وَٱلْفَتْحُ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6216"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "An-Nasr";
  dbp:text "Then exalt [Him] with praise of your Lord and ask forgiveness of Him. Indeed, He is ever Accepting of repentance.";
  <http://localhost:3333/has_arabic_text> "فَسَبِّحْ بِحَمْدِ رَبِّكَ وَٱسْتَغْفِرْهُ ۚ إِنَّهُۥ كَانَ تَوَّابًۢا",
    "وَرَأَيْتَ ٱلنَّاسَ يَدْخُلُونَ فِى دِينِ ٱللَّهِ أَفْوَاجًا" .

<http://localhost:3333/Al-Masad> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Masad";
  dbo:ayah "6217"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "May the hands of Abu Lahab be ruined, and ruined is he.";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6218"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Masad";
  dbp:text "His wealth will not avail him or that which he gained.";
  <http://localhost:3333/has_arabic_text> "تَبَّتْ يَدَآ أَبِى لَهَبٍ وَتَبَّ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6219"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Masad";
  dbp:text "He will [enter to] burn in a Fire of [blazing] flame";
  <http://localhost:3333/has_arabic_text> "سَيَصْلَىٰ نَارًا ذَاتَ لَهَبٍ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6220"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Masad";
  dbp:text "And his wife [as well] - the carrier of firewood.";
  <http://localhost:3333/has_arabic_text> "مَآ أَغْنَىٰ عَنْهُ مَالُهُۥ وَمَا كَسَبَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6221"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Masad";
  dbp:text "Around her neck is a rope of [twisted] fiber.";
  <http://localhost:3333/has_arabic_text> "فِى جِيدِهَا حَبْلٌ مِّن مَّسَدٍۭ", "وَٱمْرَأَتُهُۥ حَمَّالَةَ ٱلْحَطَبِ" .

<http://localhost:3333/Al-Ikhlas> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Ikhlas";
  dbo:ayah "6222"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "Say, \"He is Allah, [who is] One,";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6223"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ikhlas";
  dbp:text "Allah, the Eternal Refuge.";
  <http://localhost:3333/has_arabic_text> "قُلْ هُوَ ٱللَّهُ أَحَدٌ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6224"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ikhlas";
  dbp:text "He neither begets nor is born,";
  <http://localhost:3333/has_arabic_text> "لَمْ يَلِدْ وَلَمْ يُولَدْ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6225"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Ikhlas";
  dbp:text "Nor is there to Him any equivalent.\"";
  <http://localhost:3333/has_arabic_text> "وَلَمْ يَكُن لَّهُۥ كُفُوًا أَحَدٌۢ", "ٱللَّهُ ٱلصَّمَدُ" .

<http://localhost:3333/Al-Falaq> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Falaq";
  dbo:ayah "6226"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "Say, \"I seek refuge in the Lord of daybreak";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6227"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Falaq";
  dbp:text "From the evil of that which He created";
  <http://localhost:3333/has_arabic_text> "قُلْ أَعُوذُ بِرَبِّ ٱلْفَلَقِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6228"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Falaq";
  dbp:text "And from the evil of darkness when it settles";
  <http://localhost:3333/has_arabic_text> "مِن شَرِّ مَا خَلَقَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6229"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Falaq";
  dbp:text "And from the evil of the blowers in knots";
  <http://localhost:3333/has_arabic_text> "وَمِن شَرِّ غَاسِقٍ إِذَا وَقَبَ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6230"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Falaq";
  dbp:text "And from the evil of an envier when he envies.\"";
  <http://localhost:3333/has_arabic_text> "وَمِن شَرِّ حَاسِدٍ إِذَا حَسَدَ", "وَمِن شَرِّ ٱلنَّفَّٰثَٰتِ فِى ٱلْعُقَدِ" .

<http://localhost:3333/Al-Nas> a dbr:Ayah, dbr:Ayat, dbr:Quranic_verse;
  dbo:Surah "Al-Nas";
  dbo:ayah "6231"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbp:text "Say, \"I seek refuge in the Lord of mankind,";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6232"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Nas";
  dbp:text "The Sovereign of mankind.";
  <http://localhost:3333/has_arabic_text> "قُلْ أَعُوذُ بِرَبِّ ٱلنَّاسِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6233"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Nas";
  dbp:text "The God of mankind,";
  <http://localhost:3333/has_arabic_text> "إِلَٰهِ ٱلنَّاسِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6234"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Nas";
  dbp:text "From the evil of the retreating whisperer -";
  <http://localhost:3333/has_arabic_text> "مَلِكِ ٱلنَّاسِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6235"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Nas";
  dbp:text "Who whispers [evil] into the breasts of mankind -";
  <http://localhost:3333/has_arabic_text> "مِن شَرِّ ٱلْوَسْوَاسِ ٱلْخَنَّاسِ";
  a dbr:Ayah, dbr:Quranic_verse, dbr:Ayat;
  dbo:ayah "6236"^^<http://www.w3.org/2001/XMLSchema#int>;
  dbo:Surah "Al-Nas";
  dbp:text "From among the jinn and mankind.\"";
  <http://localhost:3333/has_arabic_text> "مِنَ ٱلْجِنَّةِ وَٱلنَّاسِ", "ٱلَّذِى يُوَسْوِسُ فِى صُدُورِ ٱلنَّاسِ" .
